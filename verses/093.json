[
  {
    "verse_id": 1,
    "verse_content": " وَٱلضُّحَىٰ",
    "verse_key": "93:1"
  },
  {
    "verse_id": 2,
    "verse_content": "وَٱلَّيْلِ إِذَا سَجَىٰ",
    "verse_key": "93:2"
  },
  {
    "verse_id": 3,
    "verse_content": "مَا وَدَّعَكَ رَبُّكَ وَمَا قَلَىٰ",
    "verse_key": "93:3"
  },
  {
    "verse_id": 4,
    "verse_content": "وَلَلْـَٔاخِرَةُ خَيْرٌ لَّكَ مِنَ ٱلْأُولَىٰ",
    "verse_key": "93:4"
  },
  {
    "verse_id": 5,
    "verse_content": "وَلَسَوْفَ يُعْطِيكَ رَبُّكَ فَتَرْضَىٰٓ",
    "verse_key": "93:5"
  },
  {
    "verse_id": 6,
    "verse_content": "أَلَمْ يَجِدْكَ يَتِيمًا فَـَٔاوَىٰ",
    "verse_key": "93:6"
  },
  {
    "verse_id": 7,
    "verse_content": "وَوَجَدَكَ ضَآلًّا فَهَدَىٰ",
    "verse_key": "93:7"
  },
  {
    "verse_id": 8,
    "verse_content": "وَوَجَدَكَ عَآئِلًا فَأَغْنَىٰ",
    "verse_key": "93:8"
  },
  {
    "verse_id": 9,
    "verse_content": "فَأَمَّا ٱلْيَتِيمَ فَلَا تَقْهَرْ",
    "verse_key": "93:9"
  },
  {
    "verse_id": 10,
    "verse_content": "وَأَمَّا ٱلسَّآئِلَ فَلَا تَنْهَرْ",
    "verse_key": "93:10"
  },
  {
    "verse_id": 11,
    "verse_content": "وَأَمَّا بِنِعْمَةِ رَبِّكَ فَحَدِّثْ",
    "verse_key": "93:11"
  }
]