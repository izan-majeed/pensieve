  },
  {
    "verse_id": 63,
    "verse_content": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
    "verse_key": "55:63"
  },
  {
    "verse_id": 64,
    "verse_content": "مُدْهَآمَّتَانِ",
    "verse_key": "55:64"
  },
  {
    "verse_id": 65,
    "verse_content": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
    "verse_key": "55:65"
  },
  {
    "verse_id": 66,
    "verse_content": "فِيهِمَا عَيْنَانِ نَضَّاخَتَانِ",
    "verse_key": "55:66"
  },
  {
    "verse_id": 67,
    "verse_content": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
    "verse_key": "55:67"
  },
  {
    "verse_id": 68,
    "verse_content": "فِيهِمَا فَـٰكِهَةٌ وَنَخْلٌ وَرُمَّانٌ",
    "verse_key": "55:68"
  },
  {
    "verse_id": 69,
    "verse_content": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
    "verse_key": "55:69"
  },
  {
    "verse_id": 70,
    "verse_content": "فِيهِنَّ خَيْرَٰتٌ حِسَانٌ",
    "verse_key": "55:70"
  },
  {
    "verse_id": 71,
    "verse_content": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
    "verse_key": "55:71"
  },
  {
    "verse_id": 72,
    "verse_content": "حُورٌ مَّقْصُورَٰتٌ فِى ٱلْخِيَامِ",
