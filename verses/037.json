[
  {
    "verse_id": 1,
    "verse_content": " وَٱلصَّـٰٓفَّـٰتِ صَفًّا",
    "verse_key": "37:1"
  },
  {
    "verse_id": 2,
    "verse_content": "فَٱلزَّٰجِرَٰتِ زَجْرًا",
    "verse_key": "37:2"
  },
  {
    "verse_id": 3,
    "verse_content": "فَٱلتَّـٰلِيَـٰتِ ذِكْرًا",
    "verse_key": "37:3"
  },
  {
    "verse_id": 4,
    "verse_content": "إِنَّ إِلَـٰهَكُمْ لَوَٰحِدٌ",
    "verse_key": "37:4"
  },
  {
    "verse_id": 5,
    "verse_content": "رَّبُّ ٱلسَّمَـٰوَٰتِ وَٱلْأَرْضِ وَمَا بَيْنَهُمَا وَرَبُّ ٱلْمَشَـٰرِقِ",
    "verse_key": "37:5"
  },
  {
    "verse_id": 6,
    "verse_content": "إِنَّا زَيَّنَّا ٱلسَّمَآءَ ٱلدُّنْيَا بِزِينَةٍ ٱلْكَوَاكِبِ",
    "verse_key": "37:6"
  },
  {
    "verse_id": 7,
    "verse_content": "وَحِفْظًا مِّن كُلِّ شَيْطَـٰنٍ مَّارِدٍ",
    "verse_key": "37:7"
  },
  {
    "verse_id": 8,
    "verse_content": "لَّا يَسَّمَّعُونَ إِلَى ٱلْمَلَإِ ٱلْأَعْلَىٰ وَيُقْذَفُونَ مِن كُلِّ جَانِبٍ",
    "verse_key": "37:8"
  },
  {
    "verse_id": 9,
    "verse_content": "دُحُورًا ۖ وَلَهُمْ عَذَابٌ وَاصِبٌ",
    "verse_key": "37:9"
  },
  {
    "verse_id": 10,
    "verse_content": "إِلَّا مَنْ خَطِفَ ٱلْخَطْفَةَ فَأَتْبَعَهُۥ شِهَابٌ ثَاقِبٌ",
    "verse_key": "37:10"
  },
  {
    "verse_id": 11,
    "verse_content": "فَٱسْتَفْتِهِمْ أَهُمْ أَشَدُّ خَلْقًا أَم مَّنْ خَلَقْنَآ ۚ إِنَّا خَلَقْنَـٰهُم مِّن طِينٍ لَّازِبٍۭ",
    "verse_key": "37:11"
  },
  {
    "verse_id": 12,
    "verse_content": "بَلْ عَجِبْتَ وَيَسْخَرُونَ",
    "verse_key": "37:12"
  },
  {
    "verse_id": 13,
    "verse_content": "وَإِذَا ذُكِّرُوا۟ لَا يَذْكُرُونَ",
    "verse_key": "37:13"
  },
  {
    "verse_id": 14,
    "verse_content": "وَإِذَا رَأَوْا۟ ءَايَةً يَسْتَسْخِرُونَ",
    "verse_key": "37:14"
  },
  {
    "verse_id": 15,
    "verse_content": "وَقَالُوٓا۟ إِنْ هَـٰذَآ إِلَّا سِحْرٌ مُّبِينٌ",
    "verse_key": "37:15"
  },
  {
    "verse_id": 16,
    "verse_content": "أَءِذَا مِتْنَا وَكُنَّا تُرَابًا وَعِظَـٰمًا أَءِنَّا لَمَبْعُوثُونَ",
    "verse_key": "37:16"
  },
  {
    "verse_id": 17,
    "verse_content": "أَوَءَابَآؤُنَا ٱلْأَوَّلُونَ",
    "verse_key": "37:17"
  },
  {
    "verse_id": 18,
    "verse_content": "قُلْ نَعَمْ وَأَنتُمْ دَٰخِرُونَ",
    "verse_key": "37:18"
  },
  {
    "verse_id": 19,
    "verse_content": "فَإِنَّمَا هِىَ زَجْرَةٌ وَٰحِدَةٌ فَإِذَا هُمْ يَنظُرُونَ",
    "verse_key": "37:19"
  },
  {
    "verse_id": 20,
    "verse_content": "وَقَالُوا۟ يَـٰوَيْلَنَا هَـٰذَا يَوْمُ ٱلدِّينِ",
    "verse_key": "37:20"
  },
  {
    "verse_id": 21,
    "verse_content": "هَـٰذَا يَوْمُ ٱلْفَصْلِ ٱلَّذِى كُنتُم بِهِۦ تُكَذِّبُونَ",
    "verse_key": "37:21"
  },
  {
    "verse_id": 22,
    "verse_content": "۞ ٱحْشُرُوا۟ ٱلَّذِينَ ظَلَمُوا۟ وَأَزْوَٰجَهُمْ وَمَا كَانُوا۟ يَعْبُدُونَ",
    "verse_key": "37:22"
  },
  {
    "verse_id": 23,
    "verse_content": "مِن دُونِ ٱللَّهِ فَٱهْدُوهُمْ إِلَىٰ صِرَٰطِ ٱلْجَحِيمِ",
    "verse_key": "37:23"
  },
  {
    "verse_id": 24,
    "verse_content": "وَقِفُوهُمْ ۖ إِنَّهُم مَّسْـُٔولُونَ",
    "verse_key": "37:24"
  },
  {
    "verse_id": 25,
    "verse_content": "مَا لَكُمْ لَا تَنَاصَرُونَ",
    "verse_key": "37:25"
  },
  {
    "verse_id": 26,
    "verse_content": "بَلْ هُمُ ٱلْيَوْمَ مُسْتَسْلِمُونَ",
    "verse_key": "37:26"
  },
  {
    "verse_id": 27,
    "verse_content": "وَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٍ يَتَسَآءَلُونَ",
    "verse_key": "37:27"
  },
  {
    "verse_id": 28,
    "verse_content": "قَالُوٓا۟ إِنَّكُمْ كُنتُمْ تَأْتُونَنَا عَنِ ٱلْيَمِينِ",
    "verse_key": "37:28"
  },
  {
    "verse_id": 29,
    "verse_content": "قَالُوا۟ بَل لَّمْ تَكُونُوا۟ مُؤْمِنِينَ",
    "verse_key": "37:29"
  },
  {
    "verse_id": 30,
    "verse_content": "وَمَا كَانَ لَنَا عَلَيْكُم مِّن سُلْطَـٰنٍۭ ۖ بَلْ كُنتُمْ قَوْمًا طَـٰغِينَ",
    "verse_key": "37:30"
  },
  {
    "verse_id": 31,
    "verse_content": "فَحَقَّ عَلَيْنَا قَوْلُ رَبِّنَآ ۖ إِنَّا لَذَآئِقُونَ",
    "verse_key": "37:31"
  },
  {
    "verse_id": 32,
    "verse_content": "فَأَغْوَيْنَـٰكُمْ إِنَّا كُنَّا غَـٰوِينَ",
    "verse_key": "37:32"
  },
  {
    "verse_id": 33,
    "verse_content": "فَإِنَّهُمْ يَوْمَئِذٍ فِى ٱلْعَذَابِ مُشْتَرِكُونَ",
    "verse_key": "37:33"
  },
  {
    "verse_id": 34,
    "verse_content": "إِنَّا كَذَٰلِكَ نَفْعَلُ بِٱلْمُجْرِمِينَ",
    "verse_key": "37:34"
  },
  {
    "verse_id": 35,
    "verse_content": "إِنَّهُمْ كَانُوٓا۟ إِذَا قِيلَ لَهُمْ لَآ إِلَـٰهَ إِلَّا ٱللَّهُ يَسْتَكْبِرُونَ",
    "verse_key": "37:35"
  },
  {
    "verse_id": 36,
    "verse_content": "وَيَقُولُونَ أَئِنَّا لَتَارِكُوٓا۟ ءَالِهَتِنَا لِشَاعِرٍ مَّجْنُونٍۭ",
    "verse_key": "37:36"
  },
  {
    "verse_id": 37,
    "verse_content": "بَلْ جَآءَ بِٱلْحَقِّ وَصَدَّقَ ٱلْمُرْسَلِينَ",
    "verse_key": "37:37"
  },
  {
    "verse_id": 38,
    "verse_content": "إِنَّكُمْ لَذَآئِقُوا۟ ٱلْعَذَابِ ٱلْأَلِيمِ",
    "verse_key": "37:38"
  },
  {
    "verse_id": 39,
    "verse_content": "وَمَا تُجْزَوْنَ إِلَّا مَا كُنتُمْ تَعْمَلُونَ",
    "verse_key": "37:39"
  },
  {
    "verse_id": 40,
    "verse_content": "إِلَّا عِبَادَ ٱللَّهِ ٱلْمُخْلَصِينَ",
    "verse_key": "37:40"
  },
  {
    "verse_id": 41,
    "verse_content": "أُو۟لَـٰٓئِكَ لَهُمْ رِزْقٌ مَّعْلُومٌ",
    "verse_key": "37:41"
  },
  {
    "verse_id": 42,
    "verse_content": "فَوَٰكِهُ ۖ وَهُم مُّكْرَمُونَ",
    "verse_key": "37:42"
  },
  {
    "verse_id": 43,
    "verse_content": "فِى جَنَّـٰتِ ٱلنَّعِيمِ",
    "verse_key": "37:43"
  },
  {
    "verse_id": 44,
    "verse_content": "عَلَىٰ سُرُرٍ مُّتَقَـٰبِلِينَ",
    "verse_key": "37:44"
  },
  {
    "verse_id": 45,
    "verse_content": "يُطَافُ عَلَيْهِم بِكَأْسٍ مِّن مَّعِينٍۭ",
    "verse_key": "37:45"
  },
  {
    "verse_id": 46,
    "verse_content": "بَيْضَآءَ لَذَّةٍ لِّلشَّـٰرِبِينَ",
    "verse_key": "37:46"
  },
  {
    "verse_id": 47,
    "verse_content": "لَا فِيهَا غَوْلٌ وَلَا هُمْ عَنْهَا يُنزَفُونَ",
    "verse_key": "37:47"
  },
  {
    "verse_id": 48,
    "verse_content": "وَعِندَهُمْ قَـٰصِرَٰتُ ٱلطَّرْفِ عِينٌ",
    "verse_key": "37:48"
  },
  {
    "verse_id": 49,
    "verse_content": "كَأَنَّهُنَّ بَيْضٌ مَّكْنُونٌ",
    "verse_key": "37:49"
  },
  {
    "verse_id": 50,
    "verse_content": "فَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٍ يَتَسَآءَلُونَ",
    "verse_key": "37:50"
  },
  {
    "verse_id": 51,
    "verse_content": "قَالَ قَآئِلٌ مِّنْهُمْ إِنِّى كَانَ لِى قَرِينٌ",
    "verse_key": "37:51"
  },
  {
    "verse_id": 52,
    "verse_content": "يَقُولُ أَءِنَّكَ لَمِنَ ٱلْمُصَدِّقِينَ",
    "verse_key": "37:52"
  },
  {
    "verse_id": 53,
    "verse_content": "أَءِذَا مِتْنَا وَكُنَّا تُرَابًا وَعِظَـٰمًا أَءِنَّا لَمَدِينُونَ",
    "verse_key": "37:53"
  },
  {
    "verse_id": 54,
    "verse_content": "قَالَ هَلْ أَنتُم مُّطَّلِعُونَ",
    "verse_key": "37:54"
  },
  {
    "verse_id": 55,
    "verse_content": "فَٱطَّلَعَ فَرَءَاهُ فِى سَوَآءِ ٱلْجَحِيمِ",
    "verse_key": "37:55"
  },
  {
    "verse_id": 56,
    "verse_content": "قَالَ تَٱللَّهِ إِن كِدتَّ لَتُرْدِينِ",
    "verse_key": "37:56"
  },
  {
    "verse_id": 57,
    "verse_content": "وَلَوْلَا نِعْمَةُ رَبِّى لَكُنتُ مِنَ ٱلْمُحْضَرِينَ",
    "verse_key": "37:57"
  },
  {
    "verse_id": 58,
    "verse_content": "أَفَمَا نَحْنُ بِمَيِّتِينَ",
    "verse_key": "37:58"
  },
  {
    "verse_id": 59,
    "verse_content": "إِلَّا مَوْتَتَنَا ٱلْأُولَىٰ وَمَا نَحْنُ بِمُعَذَّبِينَ",
    "verse_key": "37:59"
  },
  {
    "verse_id": 60,
    "verse_content": "إِنَّ هَـٰذَا لَهُوَ ٱلْفَوْزُ ٱلْعَظِيمُ",
    "verse_key": "37:60"
  },
  {
    "verse_id": 61,
    "verse_content": "لِمِثْلِ هَـٰذَا فَلْيَعْمَلِ ٱلْعَـٰمِلُونَ",
    "verse_key": "37:61"
  },
  {
    "verse_id": 62,
    "verse_content": "أَذَٰلِكَ خَيْرٌ نُّزُلًا أَمْ شَجَرَةُ ٱلزَّقُّومِ",
    "verse_key": "37:62"
  },
  {
    "verse_id": 63,
    "verse_content": "إِنَّا جَعَلْنَـٰهَا فِتْنَةً لِّلظَّـٰلِمِينَ",
    "verse_key": "37:63"
  },
  {
    "verse_id": 64,
    "verse_content": "إِنَّهَا شَجَرَةٌ تَخْرُجُ فِىٓ أَصْلِ ٱلْجَحِيمِ",
    "verse_key": "37:64"
  },
  {
    "verse_id": 65,
    "verse_content": "طَلْعُهَا كَأَنَّهُۥ رُءُوسُ ٱلشَّيَـٰطِينِ",
    "verse_key": "37:65"
  },
  {
    "verse_id": 66,
    "verse_content": "فَإِنَّهُمْ لَـَٔاكِلُونَ مِنْهَا فَمَالِـُٔونَ مِنْهَا ٱلْبُطُونَ",
    "verse_key": "37:66"
  },
  {
    "verse_id": 67,
    "verse_content": "ثُمَّ إِنَّ لَهُمْ عَلَيْهَا لَشَوْبًا مِّنْ حَمِيمٍ",
    "verse_key": "37:67"
  },
  {
    "verse_id": 68,
    "verse_content": "ثُمَّ إِنَّ مَرْجِعَهُمْ لَإِلَى ٱلْجَحِيمِ",
    "verse_key": "37:68"
  },
  {
    "verse_id": 69,
    "verse_content": "إِنَّهُمْ أَلْفَوْا۟ ءَابَآءَهُمْ ضَآلِّينَ",
    "verse_key": "37:69"
  },
  {
    "verse_id": 70,
    "verse_content": "فَهُمْ عَلَىٰٓ ءَاثَـٰرِهِمْ يُهْرَعُونَ",
    "verse_key": "37:70"
  },
  {
    "verse_id": 71,
    "verse_content": "وَلَقَدْ ضَلَّ قَبْلَهُمْ أَكْثَرُ ٱلْأَوَّلِينَ",
    "verse_key": "37:71"
  },
  {
    "verse_id": 72,
    "verse_content": "وَلَقَدْ أَرْسَلْنَا فِيهِم مُّنذِرِينَ",
    "verse_key": "37:72"
  },
  {
    "verse_id": 73,
    "verse_content": "فَٱنظُرْ كَيْفَ كَانَ عَـٰقِبَةُ ٱلْمُنذَرِينَ",
    "verse_key": "37:73"
  },
  {
    "verse_id": 74,
    "verse_content": "إِلَّا عِبَادَ ٱللَّهِ ٱلْمُخْلَصِينَ",
    "verse_key": "37:74"
  },
  {
    "verse_id": 75,
    "verse_content": "وَلَقَدْ نَادَىٰنَا نُوحٌ فَلَنِعْمَ ٱلْمُجِيبُونَ",
    "verse_key": "37:75"
  },
  {
    "verse_id": 76,
    "verse_content": "وَنَجَّيْنَـٰهُ وَأَهْلَهُۥ مِنَ ٱلْكَرْبِ ٱلْعَظِيمِ",
    "verse_key": "37:76"
  },
  {
    "verse_id": 77,
    "verse_content": "وَجَعَلْنَا ذُرِّيَّتَهُۥ هُمُ ٱلْبَاقِينَ",
    "verse_key": "37:77"
  },
  {
    "verse_id": 78,
    "verse_content": "وَتَرَكْنَا عَلَيْهِ فِى ٱلْـَٔاخِرِينَ",
    "verse_key": "37:78"
  },
  {
    "verse_id": 79,
    "verse_content": "سَلَـٰمٌ عَلَىٰ نُوحٍ فِى ٱلْعَـٰلَمِينَ",
    "verse_key": "37:79"
  },
  {
    "verse_id": 80,
    "verse_content": "إِنَّا كَذَٰلِكَ نَجْزِى ٱلْمُحْسِنِينَ",
    "verse_key": "37:80"
  },
  {
    "verse_id": 81,
    "verse_content": "إِنَّهُۥ مِنْ عِبَادِنَا ٱلْمُؤْمِنِينَ",
    "verse_key": "37:81"
  },
  {
    "verse_id": 82,
    "verse_content": "ثُمَّ أَغْرَقْنَا ٱلْـَٔاخَرِينَ",
    "verse_key": "37:82"
  },
  {
    "verse_id": 83,
    "verse_content": "۞ وَإِنَّ مِن شِيعَتِهِۦ لَإِبْرَٰهِيمَ",
    "verse_key": "37:83"
  },
  {
    "verse_id": 84,
    "verse_content": "إِذْ جَآءَ رَبَّهُۥ بِقَلْبٍ سَلِيمٍ",
    "verse_key": "37:84"
  },
  {
    "verse_id": 85,
    "verse_content": "إِذْ قَالَ لِأَبِيهِ وَقَوْمِهِۦ مَاذَا تَعْبُدُونَ",
    "verse_key": "37:85"
  },
  {
    "verse_id": 86,
    "verse_content": "أَئِفْكًا ءَالِهَةً دُونَ ٱللَّهِ تُرِيدُونَ",
    "verse_key": "37:86"
  },
  {
    "verse_id": 87,
    "verse_content": "فَمَا ظَنُّكُم بِرَبِّ ٱلْعَـٰلَمِينَ",
    "verse_key": "37:87"
  },
  {
    "verse_id": 88,
    "verse_content": "فَنَظَرَ نَظْرَةً فِى ٱلنُّجُومِ",
    "verse_key": "37:88"
  },
  {
    "verse_id": 89,
    "verse_content": "فَقَالَ إِنِّى سَقِيمٌ",
    "verse_key": "37:89"
  },
  {
    "verse_id": 90,
    "verse_content": "فَتَوَلَّوْا۟ عَنْهُ مُدْبِرِينَ",
    "verse_key": "37:90"
  },
  {
    "verse_id": 91,
    "verse_content": "فَرَاغَ إِلَىٰٓ ءَالِهَتِهِمْ فَقَالَ أَلَا تَأْكُلُونَ",
    "verse_key": "37:91"
  },
  {
    "verse_id": 92,
    "verse_content": "مَا لَكُمْ لَا تَنطِقُونَ",
    "verse_key": "37:92"
  },
  {
    "verse_id": 93,
    "verse_content": "فَرَاغَ عَلَيْهِمْ ضَرْبًۢا بِٱلْيَمِينِ",
    "verse_key": "37:93"
  },
  {
    "verse_id": 94,
    "verse_content": "فَأَقْبَلُوٓا۟ إِلَيْهِ يَزِفُّونَ",
    "verse_key": "37:94"
  },
  {
    "verse_id": 95,
    "verse_content": "قَالَ أَتَعْبُدُونَ مَا تَنْحِتُونَ",
    "verse_key": "37:95"
  },
  {
    "verse_id": 96,
    "verse_content": "وَٱللَّهُ خَلَقَكُمْ وَمَا تَعْمَلُونَ",
    "verse_key": "37:96"
  },
  {
    "verse_id": 97,
    "verse_content": "قَالُوا۟ ٱبْنُوا۟ لَهُۥ بُنْيَـٰنًا فَأَلْقُوهُ فِى ٱلْجَحِيمِ",
    "verse_key": "37:97"
  },
  {
    "verse_id": 98,
    "verse_content": "فَأَرَادُوا۟ بِهِۦ كَيْدًا فَجَعَلْنَـٰهُمُ ٱلْأَسْفَلِينَ",
    "verse_key": "37:98"
  },
  {
    "verse_id": 99,
    "verse_content": "وَقَالَ إِنِّى ذَاهِبٌ إِلَىٰ رَبِّى سَيَهْدِينِ",
    "verse_key": "37:99"
  },
  {
    "verse_id": 100,
    "verse_content": "رَبِّ هَبْ لِى مِنَ ٱلصَّـٰلِحِينَ",
    "verse_key": "37:100"
  },
  {
    "verse_id": 101,
    "verse_content": "فَبَشَّرْنَـٰهُ بِغُلَـٰمٍ حَلِيمٍ",
    "verse_key": "37:101"
  },
  {
    "verse_id": 102,
    "verse_content": "فَلَمَّا بَلَغَ مَعَهُ ٱلسَّعْىَ قَالَ يَـٰبُنَىَّ إِنِّىٓ أَرَىٰ فِى ٱلْمَنَامِ أَنِّىٓ أَذْبَحُكَ فَٱنظُرْ مَاذَا تَرَىٰ ۚ قَالَ يَـٰٓأَبَتِ ٱفْعَلْ مَا تُؤْمَرُ ۖ سَتَجِدُنِىٓ إِن شَآءَ ٱللَّهُ مِنَ ٱلصَّـٰبِرِينَ",
    "verse_key": "37:102"
  },
  {
    "verse_id": 103,
    "verse_content": "فَلَمَّآ أَسْلَمَا وَتَلَّهُۥ لِلْجَبِينِ",
    "verse_key": "37:103"
  },
  {
    "verse_id": 104,
    "verse_content": "وَنَـٰدَيْنَـٰهُ أَن يَـٰٓإِبْرَٰهِيمُ",
    "verse_key": "37:104"
  },
  {
    "verse_id": 105,
    "verse_content": "قَدْ صَدَّقْتَ ٱلرُّءْيَآ ۚ إِنَّا كَذَٰلِكَ نَجْزِى ٱلْمُحْسِنِينَ",
    "verse_key": "37:105"
  },
  {
    "verse_id": 106,
    "verse_content": "إِنَّ هَـٰذَا لَهُوَ ٱلْبَلَـٰٓؤُا۟ ٱلْمُبِينُ",
    "verse_key": "37:106"
  },
  {
    "verse_id": 107,
    "verse_content": "وَفَدَيْنَـٰهُ بِذِبْحٍ عَظِيمٍ",
    "verse_key": "37:107"
  },
  {
    "verse_id": 108,
    "verse_content": "وَتَرَكْنَا عَلَيْهِ فِى ٱلْـَٔاخِرِينَ",
    "verse_key": "37:108"
  },
  {
    "verse_id": 109,
    "verse_content": "سَلَـٰمٌ عَلَىٰٓ إِبْرَٰهِيمَ",
    "verse_key": "37:109"
  },
  {
    "verse_id": 110,
    "verse_content": "كَذَٰلِكَ نَجْزِى ٱلْمُحْسِنِينَ",
    "verse_key": "37:110"
  },
  {
    "verse_id": 111,
    "verse_content": "إِنَّهُۥ مِنْ عِبَادِنَا ٱلْمُؤْمِنِينَ",
    "verse_key": "37:111"
  },
  {
    "verse_id": 112,
    "verse_content": "وَبَشَّرْنَـٰهُ بِإِسْحَـٰقَ نَبِيًّا مِّنَ ٱلصَّـٰلِحِينَ",
    "verse_key": "37:112"
  },
  {
    "verse_id": 113,
    "verse_content": "وَبَـٰرَكْنَا عَلَيْهِ وَعَلَىٰٓ إِسْحَـٰقَ ۚ وَمِن ذُرِّيَّتِهِمَا مُحْسِنٌ وَظَالِمٌ لِّنَفْسِهِۦ مُبِينٌ",
    "verse_key": "37:113"
  },
  {
    "verse_id": 114,
    "verse_content": "وَلَقَدْ مَنَنَّا عَلَىٰ مُوسَىٰ وَهَـٰرُونَ",
    "verse_key": "37:114"
  },
  {
    "verse_id": 115,
    "verse_content": "وَنَجَّيْنَـٰهُمَا وَقَوْمَهُمَا مِنَ ٱلْكَرْبِ ٱلْعَظِيمِ",
    "verse_key": "37:115"
  },
  {
    "verse_id": 116,
    "verse_content": "وَنَصَرْنَـٰهُمْ فَكَانُوا۟ هُمُ ٱلْغَـٰلِبِينَ",
    "verse_key": "37:116"
  },
  {
    "verse_id": 117,
    "verse_content": "وَءَاتَيْنَـٰهُمَا ٱلْكِتَـٰبَ ٱلْمُسْتَبِينَ",
    "verse_key": "37:117"
  },
  {
    "verse_id": 118,
    "verse_content": "وَهَدَيْنَـٰهُمَا ٱلصِّرَٰطَ ٱلْمُسْتَقِيمَ",
    "verse_key": "37:118"
  },
  {
    "verse_id": 119,
    "verse_content": "وَتَرَكْنَا عَلَيْهِمَا فِى ٱلْـَٔاخِرِينَ",
    "verse_key": "37:119"
  },
  {
    "verse_id": 120,
    "verse_content": "سَلَـٰمٌ عَلَىٰ مُوسَىٰ وَهَـٰرُونَ",
    "verse_key": "37:120"
  },
  {
    "verse_id": 121,
    "verse_content": "إِنَّا كَذَٰلِكَ نَجْزِى ٱلْمُحْسِنِينَ",
    "verse_key": "37:121"
  },
  {
    "verse_id": 122,
    "verse_content": "إِنَّهُمَا مِنْ عِبَادِنَا ٱلْمُؤْمِنِينَ",
    "verse_key": "37:122"
  },
  {
    "verse_id": 123,
    "verse_content": "وَإِنَّ إِلْيَاسَ لَمِنَ ٱلْمُرْسَلِينَ",
    "verse_key": "37:123"
  },
  {
    "verse_id": 124,
    "verse_content": "إِذْ قَالَ لِقَوْمِهِۦٓ أَلَا تَتَّقُونَ",
    "verse_key": "37:124"
  },
  {
    "verse_id": 125,
    "verse_content": "أَتَدْعُونَ بَعْلًا وَتَذَرُونَ أَحْسَنَ ٱلْخَـٰلِقِينَ",
    "verse_key": "37:125"
  },
  {
    "verse_id": 126,
    "verse_content": "ٱللَّهَ رَبَّكُمْ وَرَبَّ ءَابَآئِكُمُ ٱلْأَوَّلِينَ",
    "verse_key": "37:126"
  },
  {
    "verse_id": 127,
    "verse_content": "فَكَذَّبُوهُ فَإِنَّهُمْ لَمُحْضَرُونَ",
    "verse_key": "37:127"
  },
  {
    "verse_id": 128,
    "verse_content": "إِلَّا عِبَادَ ٱللَّهِ ٱلْمُخْلَصِينَ",
    "verse_key": "37:128"
  },
  {
    "verse_id": 129,
    "verse_content": "وَتَرَكْنَا عَلَيْهِ فِى ٱلْـَٔاخِرِينَ",
    "verse_key": "37:129"
  },
  {
    "verse_id": 130,
    "verse_content": "سَلَـٰمٌ عَلَىٰٓ إِلْ يَاسِينَ",
    "verse_key": "37:130"
  },
  {
    "verse_id": 131,
    "verse_content": "إِنَّا كَذَٰلِكَ نَجْزِى ٱلْمُحْسِنِينَ",
    "verse_key": "37:131"
  },
  {
    "verse_id": 132,
    "verse_content": "إِنَّهُۥ مِنْ عِبَادِنَا ٱلْمُؤْمِنِينَ",
    "verse_key": "37:132"
  },
  {
    "verse_id": 133,
    "verse_content": "وَإِنَّ لُوطًا لَّمِنَ ٱلْمُرْسَلِينَ",
    "verse_key": "37:133"
  },
  {
    "verse_id": 134,
    "verse_content": "إِذْ نَجَّيْنَـٰهُ وَأَهْلَهُۥٓ أَجْمَعِينَ",
    "verse_key": "37:134"
  },
  {
    "verse_id": 135,
    "verse_content": "إِلَّا عَجُوزًا فِى ٱلْغَـٰبِرِينَ",
    "verse_key": "37:135"
  },
  {
    "verse_id": 136,
    "verse_content": "ثُمَّ دَمَّرْنَا ٱلْـَٔاخَرِينَ",
    "verse_key": "37:136"
  },
  {
    "verse_id": 137,
    "verse_content": "وَإِنَّكُمْ لَتَمُرُّونَ عَلَيْهِم مُّصْبِحِينَ",
    "verse_key": "37:137"
  },
  {
    "verse_id": 138,
    "verse_content": "وَبِٱلَّيْلِ ۗ أَفَلَا تَعْقِلُونَ",
    "verse_key": "37:138"
  },
  {
    "verse_id": 139,
    "verse_content": "وَإِنَّ يُونُسَ لَمِنَ ٱلْمُرْسَلِينَ",
    "verse_key": "37:139"
  },
  {
    "verse_id": 140,
    "verse_content": "إِذْ أَبَقَ إِلَى ٱلْفُلْكِ ٱلْمَشْحُونِ",
    "verse_key": "37:140"
  },
  {
    "verse_id": 141,
    "verse_content": "فَسَاهَمَ فَكَانَ مِنَ ٱلْمُدْحَضِينَ",
    "verse_key": "37:141"
  },
  {
    "verse_id": 142,
    "verse_content": "فَٱلْتَقَمَهُ ٱلْحُوتُ وَهُوَ مُلِيمٌ",
    "verse_key": "37:142"
  },
  {
    "verse_id": 143,
    "verse_content": "فَلَوْلَآ أَنَّهُۥ كَانَ مِنَ ٱلْمُسَبِّحِينَ",
    "verse_key": "37:143"
  },
  {
    "verse_id": 144,
    "verse_content": "لَلَبِثَ فِى بَطْنِهِۦٓ إِلَىٰ يَوْمِ يُبْعَثُونَ",
    "verse_key": "37:144"
  },
  {
    "verse_id": 145,
    "verse_content": "۞ فَنَبَذْنَـٰهُ بِٱلْعَرَآءِ وَهُوَ سَقِيمٌ",
    "verse_key": "37:145"
  },
  {
    "verse_id": 146,
    "verse_content": "وَأَنۢبَتْنَا عَلَيْهِ شَجَرَةً مِّن يَقْطِينٍ",
    "verse_key": "37:146"
  },
  {
    "verse_id": 147,
    "verse_content": "وَأَرْسَلْنَـٰهُ إِلَىٰ مِا۟ئَةِ أَلْفٍ أَوْ يَزِيدُونَ",
    "verse_key": "37:147"
  },
  {
    "verse_id": 148,
    "verse_content": "فَـَٔامَنُوا۟ فَمَتَّعْنَـٰهُمْ إِلَىٰ حِينٍ",
    "verse_key": "37:148"
  },
  {
    "verse_id": 149,
    "verse_content": "فَٱسْتَفْتِهِمْ أَلِرَبِّكَ ٱلْبَنَاتُ وَلَهُمُ ٱلْبَنُونَ",
    "verse_key": "37:149"
  },
  {
    "verse_id": 150,
    "verse_content": "أَمْ خَلَقْنَا ٱلْمَلَـٰٓئِكَةَ إِنَـٰثًا وَهُمْ شَـٰهِدُونَ",
    "verse_key": "37:150"
  },
  {
    "verse_id": 151,
    "verse_content": "أَلَآ إِنَّهُم مِّنْ إِفْكِهِمْ لَيَقُولُونَ",
    "verse_key": "37:151"
  },
  {
    "verse_id": 152,
    "verse_content": "وَلَدَ ٱللَّهُ وَإِنَّهُمْ لَكَـٰذِبُونَ",
    "verse_key": "37:152"
  },
  {
    "verse_id": 153,
    "verse_content": "أَصْطَفَى ٱلْبَنَاتِ عَلَى ٱلْبَنِينَ",
    "verse_key": "37:153"
  },
  {
    "verse_id": 154,
    "verse_content": "مَا لَكُمْ كَيْفَ تَحْكُمُونَ",
    "verse_key": "37:154"
  },
  {
    "verse_id": 155,
    "verse_content": "أَفَلَا تَذَكَّرُونَ",
    "verse_key": "37:155"
  },
  {
    "verse_id": 156,
    "verse_content": "أَمْ لَكُمْ سُلْطَـٰنٌ مُّبِينٌ",
    "verse_key": "37:156"
  },
  {
    "verse_id": 157,
    "verse_content": "فَأْتُوا۟ بِكِتَـٰبِكُمْ إِن كُنتُمْ صَـٰدِقِينَ",
    "verse_key": "37:157"
  },
  {
    "verse_id": 158,
    "verse_content": "وَجَعَلُوا۟ بَيْنَهُۥ وَبَيْنَ ٱلْجِنَّةِ نَسَبًا ۚ وَلَقَدْ عَلِمَتِ ٱلْجِنَّةُ إِنَّهُمْ لَمُحْضَرُونَ",
    "verse_key": "37:158"
  },
  {
    "verse_id": 159,
    "verse_content": "سُبْحَـٰنَ ٱللَّهِ عَمَّا يَصِفُونَ",
    "verse_key": "37:159"
  },
  {
    "verse_id": 160,
    "verse_content": "إِلَّا عِبَادَ ٱللَّهِ ٱلْمُخْلَصِينَ",
    "verse_key": "37:160"
  },
  {
    "verse_id": 161,
    "verse_content": "فَإِنَّكُمْ وَمَا تَعْبُدُونَ",
    "verse_key": "37:161"
  },
  {
    "verse_id": 162,
    "verse_content": "مَآ أَنتُمْ عَلَيْهِ بِفَـٰتِنِينَ",
    "verse_key": "37:162"
  },
  {
    "verse_id": 163,
    "verse_content": "إِلَّا مَنْ هُوَ صَالِ ٱلْجَحِيمِ",
    "verse_key": "37:163"
  },
  {
    "verse_id": 164,
    "verse_content": "وَمَا مِنَّآ إِلَّا لَهُۥ مَقَامٌ مَّعْلُومٌ",
    "verse_key": "37:164"
  },
  {
    "verse_id": 165,
    "verse_content": "وَإِنَّا لَنَحْنُ ٱلصَّآفُّونَ",
    "verse_key": "37:165"
  },
  {
    "verse_id": 166,
    "verse_content": "وَإِنَّا لَنَحْنُ ٱلْمُسَبِّحُونَ",
    "verse_key": "37:166"
  },
  {
    "verse_id": 167,
    "verse_content": "وَإِن كَانُوا۟ لَيَقُولُونَ",
    "verse_key": "37:167"
  },
  {
    "verse_id": 168,
    "verse_content": "لَوْ أَنَّ عِندَنَا ذِكْرًا مِّنَ ٱلْأَوَّلِينَ",
