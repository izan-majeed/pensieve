[
  {
    "verse_id": 1,
    "verse_content": " ٱلرَّحْمَـٰنُ",
    "verse_key": "55:1"
  },
  {
    "verse_id": 2,
    "verse_content": "عَلَّمَ ٱلْقُرْءَانَ",
    "verse_key": "55:2"
  },
  {
    "verse_id": 3,
    "verse_content": "خَلَقَ ٱلْإِنسَـٰنَ",
    "verse_key": "55:3"
  },
  {
    "verse_id": 4,
    "verse_content": "عَلَّمَهُ ٱلْبَيَانَ",
    "verse_key": "55:4"
  },
  {
    "verse_id": 5,
    "verse_content": "ٱلشَّمْسُ وَٱلْقَمَرُ بِحُسْبَانٍ",
    "verse_key": "55:5"
  },
  {
    "verse_id": 6,
    "verse_content": "وَٱلنَّجْمُ وَٱلشَّجَرُ يَسْجُدَانِ",
    "verse_key": "55:6"
  },
  {
    "verse_id": 7,
    "verse_content": "وَٱلسَّمَآءَ رَفَعَهَا وَوَضَعَ ٱلْمِيزَانَ",
    "verse_key": "55:7"
  },
  {
    "verse_id": 8,
    "verse_content": "أَلَّا تَطْغَوْا۟ فِى ٱلْمِيزَانِ",
    "verse_key": "55:8"
  },
  {
    "verse_id": 9,
    "verse_content": "وَأَقِيمُوا۟ ٱلْوَزْنَ بِٱلْقِسْطِ وَلَا تُخْسِرُوا۟ ٱلْمِيزَانَ",
    "verse_key": "55:9"
  },
  {
    "verse_id": 10,
    "verse_content": "وَٱلْأَرْضَ وَضَعَهَا لِلْأَنَامِ",
    "verse_key": "55:10"
  },
  {
    "verse_id": 11,
    "verse_content": "فِيهَا فَـٰكِهَةٌ وَٱلنَّخْلُ ذَاتُ ٱلْأَكْمَامِ",
    "verse_key": "55:11"
  },
  {
    "verse_id": 12,
    "verse_content": "وَٱلْحَبُّ ذُو ٱلْعَصْفِ وَٱلرَّيْحَانُ",
    "verse_key": "55:12"
  },
  {
    "verse_id": 13,
    "verse_content": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
    "verse_key": "55:13"
  },
  {
    "verse_id": 14,
    "verse_content": "خَلَقَ ٱلْإِنسَـٰنَ مِن صَلْصَـٰلٍ كَٱلْفَخَّارِ",
    "verse_key": "55:14"
  },
  {
    "verse_id": 15,
    "verse_content": "وَخَلَقَ ٱلْجَآنَّ مِن مَّارِجٍ مِّن نَّارٍ",
    "verse_key": "55:15"
  },
  {
    "verse_id": 16,
    "verse_content": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
    "verse_key": "55:16"
  },
  {
    "verse_id": 17,
    "verse_content": "رَبُّ ٱلْمَشْرِقَيْنِ وَرَبُّ ٱلْمَغْرِبَيْنِ",
    "verse_key": "55:17"
  },
  {
    "verse_id": 18,
    "verse_content": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
    "verse_key": "55:18"
  },
  {
    "verse_id": 19,
    "verse_content": "مَرَجَ ٱلْبَحْرَيْنِ يَلْتَقِيَانِ",
    "verse_key": "55:19"
  },
  {
    "verse_id": 20,
    "verse_content": "بَيْنَهُمَا بَرْزَخٌ لَّا يَبْغِيَانِ",
    "verse_key": "55:20"
  },
  {
    "verse_id": 21,
    "verse_content": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
    "verse_key": "55:21"
  },
  {
    "verse_id": 22,
    "verse_content": "يَخْرُجُ مِنْهُمَا ٱللُّؤْلُؤُ وَٱلْمَرْجَانُ",
    "verse_key": "55:22"
  },
  {
    "verse_id": 23,
    "verse_content": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
    "verse_key": "55:23"
  },
  {
    "verse_id": 24,
    "verse_content": "وَلَهُ ٱلْجَوَارِ ٱلْمُنشَـَٔاتُ فِى ٱلْبَحْرِ كَٱلْأَعْلَـٰمِ",
    "verse_key": "55:24"
  },
  {
    "verse_id": 25,
    "verse_content": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
    "verse_key": "55:25"
  },
  {
    "verse_id": 26,
    "verse_content": "كُلُّ مَنْ عَلَيْهَا فَانٍ",
    "verse_key": "55:26"
  },
  {
    "verse_id": 27,
    "verse_content": "وَيَبْقَىٰ وَجْهُ رَبِّكَ ذُو ٱلْجَلَـٰلِ وَٱلْإِكْرَامِ",
    "verse_key": "55:27"
  },
  {
    "verse_id": 28,
    "verse_content": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
    "verse_key": "55:28"
  },
  {
    "verse_id": 29,
    "verse_content": "يَسْـَٔلُهُۥ مَن فِى ٱلسَّمَـٰوَٰتِ وَٱلْأَرْضِ ۚ كُلَّ يَوْمٍ هُوَ فِى شَأْنٍ",
    "verse_key": "55:29"
  },
  {
    "verse_id": 30,
    "verse_content": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
    "verse_key": "55:30"
  },
  {
    "verse_id": 31,
    "verse_content": "سَنَفْرُغُ لَكُمْ أَيُّهَ ٱلثَّقَلَانِ",
    "verse_key": "55:31"
  },
  {
    "verse_id": 32,
    "verse_content": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
    "verse_key": "55:32"
  },
  {
    "verse_id": 33,
    "verse_content": "يَـٰمَعْشَرَ ٱلْجِنِّ وَٱلْإِنسِ إِنِ ٱسْتَطَعْتُمْ أَن تَنفُذُوا۟ مِنْ أَقْطَارِ ٱلسَّمَـٰوَٰتِ وَٱلْأَرْضِ فَٱنفُذُوا۟ ۚ لَا تَنفُذُونَ إِلَّا بِسُلْطَـٰنٍ",
    "verse_key": "55:33"
  },
  {
    "verse_id": 34,
    "verse_content": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
    "verse_key": "55:34"
  },
  {
    "verse_id": 35,
    "verse_content": "يُرْسَلُ عَلَيْكُمَا شُوَاظٌ مِّن نَّارٍ وَنُحَاسٌ فَلَا تَنتَصِرَانِ",
    "verse_key": "55:35"
  },
  {
    "verse_id": 36,
    "verse_content": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
    "verse_key": "55:36"
  },
  {
    "verse_id": 37,
    "verse_content": "فَإِذَا ٱنشَقَّتِ ٱلسَّمَآءُ فَكَانَتْ وَرْدَةً كَٱلدِّهَانِ",
    "verse_key": "55:37"
  },
  {
    "verse_id": 38,
    "verse_content": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
    "verse_key": "55:38"
  },
  {
    "verse_id": 39,
    "verse_content": "فَيَوْمَئِذٍ لَّا يُسْـَٔلُ عَن ذَنۢبِهِۦٓ إِنسٌ وَلَا جَآنٌّ",
    "verse_key": "55:39"
  },
  {
    "verse_id": 40,
    "verse_content": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
    "verse_key": "55:40"
  },
  {
    "verse_id": 41,
    "verse_content": "يُعْرَفُ ٱلْمُجْرِمُونَ بِسِيمَـٰهُمْ فَيُؤْخَذُ بِٱلنَّوَٰصِى وَٱلْأَقْدَامِ",
    "verse_key": "55:41"
  },
  {
    "verse_id": 42,
    "verse_content": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
    "verse_key": "55:42"
  },
  {
    "verse_id": 43,
    "verse_content": "هَـٰذِهِۦ جَهَنَّمُ ٱلَّتِى يُكَذِّبُ بِهَا ٱلْمُجْرِمُونَ",
    "verse_key": "55:43"
  },
  {
    "verse_id": 44,
    "verse_content": "يَطُوفُونَ بَيْنَهَا وَبَيْنَ حَمِيمٍ ءَانٍ",
    "verse_key": "55:44"
  },
  {
    "verse_id": 45,
    "verse_content": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
    "verse_key": "55:45"
  },
  {
    "verse_id": 46,
    "verse_content": "وَلِمَنْ خَافَ مَقَامَ رَبِّهِۦ جَنَّتَانِ",
    "verse_key": "55:46"
  },
  {
    "verse_id": 47,
    "verse_content": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
    "verse_key": "55:47"
  },
  {
    "verse_id": 48,
    "verse_content": "ذَوَاتَآ أَفْنَانٍ",
    "verse_key": "55:48"
  },
  {
    "verse_id": 49,
    "verse_content": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
    "verse_key": "55:49"
  },
  {
    "verse_id": 50,
    "verse_content": "فِيهِمَا عَيْنَانِ تَجْرِيَانِ",
    "verse_key": "55:50"
  },
  {
    "verse_id": 51,
    "verse_content": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
    "verse_key": "55:51"
  },
  {
    "verse_id": 52,
    "verse_content": "فِيهِمَا مِن كُلِّ فَـٰكِهَةٍ زَوْجَانِ",
    "verse_key": "55:52"
  },
  {
    "verse_id": 53,
    "verse_content": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
    "verse_key": "55:53"
  },
  {
    "verse_id": 54,
    "verse_content": "مُتَّكِـِٔينَ عَلَىٰ فُرُشٍۭ بَطَآئِنُهَا مِنْ إِسْتَبْرَقٍ ۚ وَجَنَى ٱلْجَنَّتَيْنِ دَانٍ",
    "verse_key": "55:54"
  },
  {
    "verse_id": 55,
    "verse_content": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
    "verse_key": "55:55"
  },
  {
    "verse_id": 56,
    "verse_content": "فِيهِنَّ قَـٰصِرَٰتُ ٱلطَّرْفِ لَمْ يَطْمِثْهُنَّ إِنسٌ قَبْلَهُمْ وَلَا جَآنٌّ",
    "verse_key": "55:56"
  },
  {
    "verse_id": 57,
    "verse_content": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
    "verse_key": "55:57"
  },
  {
    "verse_id": 58,
    "verse_content": "كَأَنَّهُنَّ ٱلْيَاقُوتُ وَٱلْمَرْجَانُ",
    "verse_key": "55:58"
  },
  {
    "verse_id": 59,
    "verse_content": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
    "verse_key": "55:59"
  },
  {
    "verse_id": 60,
    "verse_content": "هَلْ جَزَآءُ ٱلْإِحْسَـٰنِ إِلَّا ٱلْإِحْسَـٰنُ",
    "verse_key": "55:60"
  },
  {
    "verse_id": 61,
    "verse_content": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
    "verse_key": "55:61"
  },
  {
    "verse_id": 62,
    "verse_content": "وَمِن دُونِهِمَا جَنَّتَانِ",
