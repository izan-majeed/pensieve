[
  {
    "verse_id": 1,
    "verse_content": " ٱقْرَأْ بِٱسْمِ رَبِّكَ ٱلَّذِى خَلَقَ",
    "verse_key": "96:1"
  },
  {
    "verse_id": 2,
    "verse_content": "خَلَقَ ٱلْإِنسَـٰنَ مِنْ عَلَقٍ",
    "verse_key": "96:2"
  },
  {
    "verse_id": 3,
    "verse_content": "ٱقْرَأْ وَرَبُّكَ ٱلْأَكْرَمُ",
    "verse_key": "96:3"
  },
  {
    "verse_id": 4,
    "verse_content": "ٱلَّذِى عَلَّمَ بِٱلْقَلَمِ",
    "verse_key": "96:4"
  },
  {
    "verse_id": 5,
    "verse_content": "عَلَّمَ ٱلْإِنسَـٰنَ مَا لَمْ يَعْلَمْ",
    "verse_key": "96:5"
  },
  {
    "verse_id": 6,
    "verse_content": "كَلَّآ إِنَّ ٱلْإِنسَـٰنَ لَيَطْغَىٰٓ",
    "verse_key": "96:6"
  },
  {
    "verse_id": 7,
    "verse_content": "أَن رَّءَاهُ ٱسْتَغْنَىٰٓ",
    "verse_key": "96:7"
  },
  {
    "verse_id": 8,
    "verse_content": "إِنَّ إِلَىٰ رَبِّكَ ٱلرُّجْعَىٰٓ",
    "verse_key": "96:8"
  },
  {
    "verse_id": 9,
    "verse_content": "أَرَءَيْتَ ٱلَّذِى يَنْهَىٰ",
    "verse_key": "96:9"
  },
  {
    "verse_id": 10,
    "verse_content": "عَبْدًا إِذَا صَلَّىٰٓ",
    "verse_key": "96:10"
  },
  {
    "verse_id": 11,
    "verse_content": "أَرَءَيْتَ إِن كَانَ عَلَى ٱلْهُدَىٰٓ",
    "verse_key": "96:11"
  },
  {
    "verse_id": 12,
    "verse_content": "أَوْ أَمَرَ بِٱلتَّقْوَىٰٓ",
    "verse_key": "96:12"
  },
  {
    "verse_id": 13,
    "verse_content": "أَرَءَيْتَ إِن كَذَّبَ وَتَوَلَّىٰٓ",
    "verse_key": "96:13"
  },
  {
    "verse_id": 14,
    "verse_content": "أَلَمْ يَعْلَم بِأَنَّ ٱللَّهَ يَرَىٰ",
    "verse_key": "96:14"
  },
  {
    "verse_id": 15,
    "verse_content": "كَلَّا لَئِن لَّمْ يَنتَهِ لَنَسْفَعًۢا بِٱلنَّاصِيَةِ",
    "verse_key": "96:15"
  },
  {
    "verse_id": 16,
    "verse_content": "نَاصِيَةٍ كَـٰذِبَةٍ خَاطِئَةٍ",
    "verse_key": "96:16"
  },
  {
    "verse_id": 17,
    "verse_content": "فَلْيَدْعُ نَادِيَهُۥ",
    "verse_key": "96:17"
  },
  {
    "verse_id": 18,
    "verse_content": "سَنَدْعُ ٱلزَّبَانِيَةَ",
    "verse_key": "96:18"
  },
  {
    "verse_id": 19,
    "verse_content": "كَلَّا لَا تُطِعْهُ وَٱسْجُدْ وَٱقْتَرِب ۩",
    "verse_key": "96:19"
  }
]