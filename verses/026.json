  {
    "verse_id": 161,
    "verse_content": "إِذْ قَالَ لَهُمْ أَخُوهُمْ لُوطٌ أَلَا تَتَّقُونَ",
    "verse_key": "26:161"
  },
  {
    "verse_id": 162,
    "verse_content": "إِنِّى لَكُمْ رَسُولٌ أَمِينٌ",
    "verse_key": "26:162"
  },
  {
    "verse_id": 163,
    "verse_content": "فَٱتَّقُوا۟ ٱللَّهَ وَأَطِيعُونِ",
    "verse_key": "26:163"
  },
  {
    "verse_id": 164,
    "verse_content": "وَمَآ أَسْـَٔلُكُمْ عَلَيْهِ مِنْ أَجْرٍ ۖ إِنْ أَجْرِىَ إِلَّا عَلَىٰ رَبِّ ٱلْعَـٰلَمِينَ",
    "verse_key": "26:164"
  },
  {
    "verse_id": 165,
    "verse_content": "أَتَأْتُونَ ٱلذُّكْرَانَ مِنَ ٱلْعَـٰلَمِينَ",
    "verse_key": "26:165"
  },
  {
    "verse_id": 166,
    "verse_content": "وَتَذَرُونَ مَا خَلَقَ لَكُمْ رَبُّكُم مِّنْ أَزْوَٰجِكُم ۚ بَلْ أَنتُمْ قَوْمٌ عَادُونَ",
    "verse_key": "26:166"
  },
  {
    "verse_id": 167,
    "verse_content": "قَالُوا۟ لَئِن لَّمْ تَنتَهِ يَـٰلُوطُ لَتَكُونَنَّ مِنَ ٱلْمُخْرَجِينَ",
    "verse_key": "26:167"
  },
  {
    "verse_id": 168,
    "verse_content": "قَالَ إِنِّى لِعَمَلِكُم مِّنَ ٱلْقَالِينَ",
    "verse_key": "26:168"
  },
  {
    "verse_id": 169,
    "verse_content": "رَبِّ نَجِّنِى وَأَهْلِى مِمَّا يَعْمَلُونَ",
    "verse_key": "26:169"
  },
  {
    "verse_id": 170,
    "verse_content": "فَنَجَّيْنَـٰهُ وَأَهْلَهُۥٓ أَجْمَعِينَ",
    "verse_key": "26:170"
  },
  {
    "verse_id": 171,
    "verse_content": "إِلَّا عَجُوزًا فِى ٱلْغَـٰبِرِينَ",
    "verse_key": "26:171"
  },
  {
    "verse_id": 172,
    "verse_content": "ثُمَّ دَمَّرْنَا ٱلْـَٔاخَرِينَ",
    "verse_key": "26:172"
  },
  {
    "verse_id": 173,
    "verse_content": "وَأَمْطَرْنَا عَلَيْهِم مَّطَرًا ۖ فَسَآءَ مَطَرُ ٱلْمُنذَرِينَ",
    "verse_key": "26:173"
  },
  {
    "verse_id": 174,
    "verse_content": "إِنَّ فِى ذَٰلِكَ لَـَٔايَةً ۖ وَمَا كَانَ أَكْثَرُهُم مُّؤْمِنِينَ",
    "verse_key": "26:174"
  },
  {
    "verse_id": 175,
    "verse_content": "وَإِنَّ رَبَّكَ لَهُوَ ٱلْعَزِيزُ ٱلرَّحِيمُ",
    "verse_key": "26:175"
  },
  {
    "verse_id": 176,
    "verse_content": "كَذَّبَ أَصْحَـٰبُ لْـَٔيْكَةِ ٱلْمُرْسَلِينَ",
    "verse_key": "26:176"
  },
  {
    "verse_id": 177,
    "verse_content": "إِذْ قَالَ لَهُمْ شُعَيْبٌ أَلَا تَتَّقُونَ",
    "verse_key": "26:177"
  },
  {
    "verse_id": 178,
    "verse_content": "إِنِّى لَكُمْ رَسُولٌ أَمِينٌ",
    "verse_key": "26:178"
  },
  {
    "verse_id": 179,
    "verse_content": "فَٱتَّقُوا۟ ٱللَّهَ وَأَطِيعُونِ",
    "verse_key": "26:179"
  },
  {
    "verse_id": 180,
    "verse_content": "وَمَآ أَسْـَٔلُكُمْ عَلَيْهِ مِنْ أَجْرٍ ۖ إِنْ أَجْرِىَ إِلَّا عَلَىٰ رَبِّ ٱلْعَـٰلَمِينَ",
    "verse_key": "26:180"
  },
  {
    "verse_id": 181,
    "verse_content": "۞ أَوْفُوا۟ ٱلْكَيْلَ وَلَا تَكُونُوا۟ مِنَ ٱلْمُخْسِرِينَ",
    "verse_key": "26:181"
  },
  {
    "verse_id": 182,
    "verse_content": "وَزِنُوا۟ بِٱلْقِسْطَاسِ ٱلْمُسْتَقِيمِ",
    "verse_key": "26:182"
  },
  {
    "verse_id": 183,
    "verse_content": "وَلَا تَبْخَسُوا۟ ٱلنَّاسَ أَشْيَآءَهُمْ وَلَا تَعْثَوْا۟ فِى ٱلْأَرْضِ مُفْسِدِينَ",
    "verse_key": "26:183"
  },
  {
    "verse_id": 184,
    "verse_content": "وَٱتَّقُوا۟ ٱلَّذِى خَلَقَكُمْ وَٱلْجِبِلَّةَ ٱلْأَوَّلِينَ",
    "verse_key": "26:184"
  },
  {
    "verse_id": 185,
    "verse_content": "قَالُوٓا۟ إِنَّمَآ أَنتَ مِنَ ٱلْمُسَحَّرِينَ",
    "verse_key": "26:185"
  },
  {
    "verse_id": 186,
    "verse_content": "وَمَآ أَنتَ إِلَّا بَشَرٌ مِّثْلُنَا وَإِن نَّظُنُّكَ لَمِنَ ٱلْكَـٰذِبِينَ",
    "verse_key": "26:186"
  },
  {
    "verse_id": 187,
    "verse_content": "فَأَسْقِطْ عَلَيْنَا كِسَفًا مِّنَ ٱلسَّمَآءِ إِن كُنتَ مِنَ ٱلصَّـٰدِقِينَ",
    "verse_key": "26:187"
  },
  {
    "verse_id": 188,
    "verse_content": "قَالَ رَبِّىٓ أَعْلَمُ بِمَا تَعْمَلُونَ",
    "verse_key": "26:188"
  },
  {
    "verse_id": 189,
    "verse_content": "فَكَذَّبُوهُ فَأَخَذَهُمْ عَذَابُ يَوْمِ ٱلظُّلَّةِ ۚ إِنَّهُۥ كَانَ عَذَابَ يَوْمٍ عَظِيمٍ",
    "verse_key": "26:189"
  },
  {
    "verse_id": 190,
    "verse_content": "إِنَّ فِى ذَٰلِكَ لَـَٔايَةً ۖ وَمَا كَانَ أَكْثَرُهُم مُّؤْمِنِينَ",
    "verse_key": "26:190"
  },
  {
    "verse_id": 191,
    "verse_content": "وَإِنَّ رَبَّكَ لَهُوَ ٱلْعَزِيزُ ٱلرَّحِيمُ",
    "verse_key": "26:191"
  },
  {
    "verse_id": 192,
    "verse_content": "وَإِنَّهُۥ لَتَنزِيلُ رَبِّ ٱلْعَـٰلَمِينَ",
    "verse_key": "26:192"
  },
  {
    "verse_id": 193,
    "verse_content": "نَزَلَ بِهِ ٱلرُّوحُ ٱلْأَمِينُ",
    "verse_key": "26:193"
  },
  {
    "verse_id": 194,
    "verse_content": "عَلَىٰ قَلْبِكَ لِتَكُونَ مِنَ ٱلْمُنذِرِينَ",
    "verse_key": "26:194"
  },
  {
    "verse_id": 195,
    "verse_content": "بِلِسَانٍ عَرَبِىٍّ مُّبِينٍ",
    "verse_key": "26:195"
  },
  {
    "verse_id": 196,
    "verse_content": "وَإِنَّهُۥ لَفِى زُبُرِ ٱلْأَوَّلِينَ",
    "verse_key": "26:196"
  },
  {
    "verse_id": 197,
    "verse_content": "أَوَلَمْ يَكُن لَّهُمْ ءَايَةً أَن يَعْلَمَهُۥ عُلَمَـٰٓؤُا۟ بَنِىٓ إِسْرَٰٓءِيلَ",
    "verse_key": "26:197"
  },
  {
    "verse_id": 198,
    "verse_content": "وَلَوْ نَزَّلْنَـٰهُ عَلَىٰ بَعْضِ ٱلْأَعْجَمِينَ",
    "verse_key": "26:198"
  },
  {
    "verse_id": 199,
    "verse_content": "فَقَرَأَهُۥ عَلَيْهِم مَّا كَانُوا۟ بِهِۦ مُؤْمِنِينَ",
    "verse_key": "26:199"
  },
  {
    "verse_id": 200,
    "verse_content": "كَذَٰلِكَ سَلَكْنَـٰهُ فِى قُلُوبِ ٱلْمُجْرِمِينَ",
    "verse_key": "26:200"
  },
  {
    "verse_id": 201,
    "verse_content": "لَا يُؤْمِنُونَ بِهِۦ حَتَّىٰ يَرَوُا۟ ٱلْعَذَابَ ٱلْأَلِيمَ",
    "verse_key": "26:201"
  },
  {
    "verse_id": 202,
    "verse_content": "فَيَأْتِيَهُم بَغْتَةً وَهُمْ لَا يَشْعُرُونَ",
    "verse_key": "26:202"
  },
  {
    "verse_id": 203,
    "verse_content": "فَيَقُولُوا۟ هَلْ نَحْنُ مُنظَرُونَ",
    "verse_key": "26:203"
  },
  {
    "verse_id": 204,
    "verse_content": "أَفَبِعَذَابِنَا يَسْتَعْجِلُونَ",
    "verse_key": "26:204"
  },
  {
    "verse_id": 205,
    "verse_content": "أَفَرَءَيْتَ إِن مَّتَّعْنَـٰهُمْ سِنِينَ",
    "verse_key": "26:205"
  },
  {
    "verse_id": 206,
    "verse_content": "ثُمَّ جَآءَهُم مَّا كَانُوا۟ يُوعَدُونَ",
    "verse_key": "26:206"
  },
  {
    "verse_id": 207,
    "verse_content": "مَآ أَغْنَىٰ عَنْهُم مَّا كَانُوا۟ يُمَتَّعُونَ",
    "verse_key": "26:207"
  },
  {
    "verse_id": 208,
    "verse_content": "وَمَآ أَهْلَكْنَا مِن قَرْيَةٍ إِلَّا لَهَا مُنذِرُونَ",
    "verse_key": "26:208"
  },
  {
    "verse_id": 209,
    "verse_content": "ذِكْرَىٰ وَمَا كُنَّا ظَـٰلِمِينَ",
    "verse_key": "26:209"
  },
  {
    "verse_id": 210,
    "verse_content": "وَمَا تَنَزَّلَتْ بِهِ ٱلشَّيَـٰطِينُ",
    "verse_key": "26:210"
  },
  {
    "verse_id": 211,
    "verse_content": "وَمَا يَنۢبَغِى لَهُمْ وَمَا يَسْتَطِيعُونَ",
    "verse_key": "26:211"
  },
  {
    "verse_id": 212,
    "verse_content": "إِنَّهُمْ عَنِ ٱلسَّمْعِ لَمَعْزُولُونَ",
    "verse_key": "26:212"
  },
  {
    "verse_id": 213,
    "verse_content": "فَلَا تَدْعُ مَعَ ٱللَّهِ إِلَـٰهًا ءَاخَرَ فَتَكُونَ مِنَ ٱلْمُعَذَّبِينَ",
    "verse_key": "26:213"
  },
  {
    "verse_id": 214,
    "verse_content": "وَأَنذِرْ عَشِيرَتَكَ ٱلْأَقْرَبِينَ",
    "verse_key": "26:214"
  },
  {
    "verse_id": 215,
    "verse_content": "وَٱخْفِضْ جَنَاحَكَ لِمَنِ ٱتَّبَعَكَ مِنَ ٱلْمُؤْمِنِينَ",
    "verse_key": "26:215"
  },
  {
    "verse_id": 216,
    "verse_content": "فَإِنْ عَصَوْكَ فَقُلْ إِنِّى بَرِىٓءٌ مِّمَّا تَعْمَلُونَ",
    "verse_key": "26:216"
  },
  {
    "verse_id": 217,
    "verse_content": "وَتَوَكَّلْ عَلَى ٱلْعَزِيزِ ٱلرَّحِيمِ",
    "verse_key": "26:217"
  },
  {
    "verse_id": 218,
    "verse_content": "ٱلَّذِى يَرَىٰكَ حِينَ تَقُومُ",
    "verse_key": "26:218"
  },
  {
    "verse_id": 219,
    "verse_content": "وَتَقَلُّبَكَ فِى ٱلسَّـٰجِدِينَ",
    "verse_key": "26:219"
  },
  {
    "verse_id": 220,
    "verse_content": "إِنَّهُۥ هُوَ ٱلسَّمِيعُ ٱلْعَلِيمُ",
    "verse_key": "26:220"
  },
  {
    "verse_id": 221,
    "verse_content": "هَلْ أُنَبِّئُكُمْ عَلَىٰ مَن تَنَزَّلُ ٱلشَّيَـٰطِينُ",
    "verse_key": "26:221"
  },
  {
    "verse_id": 222,
    "verse_content": "تَنَزَّلُ عَلَىٰ كُلِّ أَفَّاكٍ أَثِيمٍ",
    "verse_key": "26:222"
  },
  {
    "verse_id": 223,
    "verse_content": "يُلْقُونَ ٱلسَّمْعَ وَأَكْثَرُهُمْ كَـٰذِبُونَ",
    "verse_key": "26:223"
  },
  {
    "verse_id": 224,
    "verse_content": "وَٱلشُّعَرَآءُ يَتَّبِعُهُمُ ٱلْغَاوُۥنَ",
    "verse_key": "26:224"
  },
  {
    "verse_id": 225,
    "verse_content": "أَلَمْ تَرَ أَنَّهُمْ فِى كُلِّ وَادٍ يَهِيمُونَ",
    "verse_key": "26:225"
  },
  {
    "verse_id": 226,
    "verse_content": "وَأَنَّهُمْ يَقُولُونَ مَا لَا يَفْعَلُونَ",
    "verse_key": "26:226"
  },
  {
    "verse_id": 227,
    "verse_content": "إِلَّا ٱلَّذِينَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّـٰلِحَـٰتِ وَذَكَرُوا۟ ٱللَّهَ كَثِيرًا وَٱنتَصَرُوا۟ مِنۢ بَعْدِ مَا ظُلِمُوا۟ ۗ وَسَيَعْلَمُ ٱلَّذِينَ ظَلَمُوٓا۟ أَىَّ مُنقَلَبٍ يَنقَلِبُونَ",
    "verse_key": "26:227"
  }
]