  },
  {
    "verse_id": 169,
    "verse_content": "لَكُنَّا عِبَادَ ٱللَّهِ ٱلْمُخْلَصِينَ",
    "verse_key": "37:169"
  },
  {
    "verse_id": 170,
    "verse_content": "فَكَفَرُوا۟ بِهِۦ ۖ فَسَوْفَ يَعْلَمُونَ",
    "verse_key": "37:170"
  },
  {
    "verse_id": 171,
    "verse_content": "وَلَقَدْ سَبَقَتْ كَلِمَتُنَا لِعِبَادِنَا ٱلْمُرْسَلِينَ",
    "verse_key": "37:171"
  },
  {
    "verse_id": 172,
    "verse_content": "إِنَّهُمْ لَهُمُ ٱلْمَنصُورُونَ",
    "verse_key": "37:172"
  },
