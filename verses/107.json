[
  {
    "verse_id": 1,
    "verse_content": " أَرَءَيْتَ ٱلَّذِى يُكَذِّبُ بِٱلدِّينِ",
    "verse_key": "107:1"
  },
  {
    "verse_id": 2,
    "verse_content": "فَذَٰلِكَ ٱلَّذِى يَدُعُّ ٱلْيَتِيمَ",
    "verse_key": "107:2"
  },
  {
    "verse_id": 3,
    "verse_content": "وَلَا يَحُضُّ عَلَىٰ طَعَامِ ٱلْمِسْكِينِ",
    "verse_key": "107:3"
  },
  {
    "verse_id": 4,
    "verse_content": "فَوَيْلٌ لِّلْمُصَلِّينَ",
    "verse_key": "107:4"
  },
  {
    "verse_id": 5,
    "verse_content": "ٱلَّذِينَ هُمْ عَن صَلَاتِهِمْ سَاهُونَ",
    "verse_key": "107:5"
  },
  {
    "verse_id": 6,
    "verse_content": "ٱلَّذِينَ هُمْ يُرَآءُونَ",
    "verse_key": "107:6"
  },
  {
    "verse_id": 7,
    "verse_content": "وَيَمْنَعُونَ ٱلْمَاعُونَ",
    "verse_key": "107:7"
  }
]