[
  {
    "verse_id": 1,
    "verse_content": " عَمَّ يَتَسَآءَلُونَ",
    "verse_key": "78:1"
  },
  {
    "verse_id": 2,
    "verse_content": "عَنِ ٱلنَّبَإِ ٱلْعَظِيمِ",
    "verse_key": "78:2"
  },
  {
    "verse_id": 3,
    "verse_content": "ٱلَّذِى هُمْ فِيهِ مُخْتَلِفُونَ",
    "verse_key": "78:3"
  },
  {
    "verse_id": 4,
    "verse_content": "كَلَّا سَيَعْلَمُونَ",
    "verse_key": "78:4"
  },
  {
    "verse_id": 5,
    "verse_content": "ثُمَّ كَلَّا سَيَعْلَمُونَ",
    "verse_key": "78:5"
  },
  {
    "verse_id": 6,
    "verse_content": "أَلَمْ نَجْعَلِ ٱلْأَرْضَ مِهَـٰدًا",
    "verse_key": "78:6"
  },
  {
    "verse_id": 7,
    "verse_content": "وَٱلْجِبَالَ أَوْتَادًا",
    "verse_key": "78:7"
  },
  {
    "verse_id": 8,
    "verse_content": "وَخَلَقْنَـٰكُمْ أَزْوَٰجًا",
    "verse_key": "78:8"
  },
  {
    "verse_id": 9,
    "verse_content": "وَجَعَلْنَا نَوْمَكُمْ سُبَاتًا",
    "verse_key": "78:9"
  },
  {
    "verse_id": 10,
    "verse_content": "وَجَعَلْنَا ٱلَّيْلَ لِبَاسًا",
    "verse_key": "78:10"
  },
  {
    "verse_id": 11,
    "verse_content": "وَجَعَلْنَا ٱلنَّهَارَ مَعَاشًا",
    "verse_key": "78:11"
  },
  {
    "verse_id": 12,
    "verse_content": "وَبَنَيْنَا فَوْقَكُمْ سَبْعًا شِدَادًا",
    "verse_key": "78:12"
  },
  {
    "verse_id": 13,
    "verse_content": "وَجَعَلْنَا سِرَاجًا وَهَّاجًا",
    "verse_key": "78:13"
  },
  {
    "verse_id": 14,
    "verse_content": "وَأَنزَلْنَا مِنَ ٱلْمُعْصِرَٰتِ مَآءً ثَجَّاجًا",
    "verse_key": "78:14"
  },
  {
    "verse_id": 15,
    "verse_content": "لِّنُخْرِجَ بِهِۦ حَبًّا وَنَبَاتًا",
    "verse_key": "78:15"
  },
  {
    "verse_id": 16,
    "verse_content": "وَجَنَّـٰتٍ أَلْفَافًا",
    "verse_key": "78:16"
  },
  {
    "verse_id": 17,
    "verse_content": "إِنَّ يَوْمَ ٱلْفَصْلِ كَانَ مِيقَـٰتًا",
    "verse_key": "78:17"
  },
  {
    "verse_id": 18,
    "verse_content": "يَوْمَ يُنفَخُ فِى ٱلصُّورِ فَتَأْتُونَ أَفْوَاجًا",
    "verse_key": "78:18"
  },
  {
    "verse_id": 19,
    "verse_content": "وَفُتِحَتِ ٱلسَّمَآءُ فَكَانَتْ أَبْوَٰبًا",
    "verse_key": "78:19"
  },
  {
    "verse_id": 20,
    "verse_content": "وَسُيِّرَتِ ٱلْجِبَالُ فَكَانَتْ سَرَابًا",
    "verse_key": "78:20"
  },
  {
    "verse_id": 21,
    "verse_content": "إِنَّ جَهَنَّمَ كَانَتْ مِرْصَادًا",
    "verse_key": "78:21"
  },
  {
    "verse_id": 22,
    "verse_content": "لِّلطَّـٰغِينَ مَـَٔابًا",
    "verse_key": "78:22"
  },
  {
    "verse_id": 23,
    "verse_content": "لَّـٰبِثِينَ فِيهَآ أَحْقَابًا",
    "verse_key": "78:23"
  },
  {
    "verse_id": 24,
    "verse_content": "لَّا يَذُوقُونَ فِيهَا بَرْدًا وَلَا شَرَابًا",
    "verse_key": "78:24"
  },
  {
    "verse_id": 25,
    "verse_content": "إِلَّا حَمِيمًا وَغَسَّاقًا",
    "verse_key": "78:25"
  },
  {
    "verse_id": 26,
    "verse_content": "جَزَآءً وِفَاقًا",
    "verse_key": "78:26"
  },
  {
    "verse_id": 27,
    "verse_content": "إِنَّهُمْ كَانُوا۟ لَا يَرْجُونَ حِسَابًا",
    "verse_key": "78:27"
  },
  {
    "verse_id": 28,
    "verse_content": "وَكَذَّبُوا۟ بِـَٔايَـٰتِنَا كِذَّابًا",
    "verse_key": "78:28"
  },
  {
    "verse_id": 29,
    "verse_content": "وَكُلَّ شَىْءٍ أَحْصَيْنَـٰهُ كِتَـٰبًا",
    "verse_key": "78:29"
  },
  {
    "verse_id": 30,
    "verse_content": "فَذُوقُوا۟ فَلَن نَّزِيدَكُمْ إِلَّا عَذَابًا",
    "verse_key": "78:30"
  },
  {
    "verse_id": 31,
    "verse_content": "إِنَّ لِلْمُتَّقِينَ مَفَازًا",
    "verse_key": "78:31"
  },
  {
    "verse_id": 32,
    "verse_content": "حَدَآئِقَ وَأَعْنَـٰبًا",
    "verse_key": "78:32"
  },
  {
    "verse_id": 33,
    "verse_content": "وَكَوَاعِبَ أَتْرَابًا",
    "verse_key": "78:33"
  },
  {
    "verse_id": 34,
    "verse_content": "وَكَأْسًا دِهَاقًا",
    "verse_key": "78:34"
  },
  {
    "verse_id": 35,
    "verse_content": "لَّا يَسْمَعُونَ فِيهَا لَغْوًا وَلَا كِذَّٰبًا",
    "verse_key": "78:35"
  },
  {
    "verse_id": 36,
    "verse_content": "جَزَآءً مِّن رَّبِّكَ عَطَآءً حِسَابًا",
    "verse_key": "78:36"
  },
  {
    "verse_id": 37,
    "verse_content": "رَّبِّ ٱلسَّمَـٰوَٰتِ وَٱلْأَرْضِ وَمَا بَيْنَهُمَا ٱلرَّحْمَـٰنِ ۖ لَا يَمْلِكُونَ مِنْهُ خِطَابًا",
    "verse_key": "78:37"
  },
  {
    "verse_id": 38,
    "verse_content": "يَوْمَ يَقُومُ ٱلرُّوحُ وَٱلْمَلَـٰٓئِكَةُ صَفًّا ۖ لَّا يَتَكَلَّمُونَ إِلَّا مَنْ أَذِنَ لَهُ ٱلرَّحْمَـٰنُ وَقَالَ صَوَابًا",
    "verse_key": "78:38"
  },
  {
    "verse_id": 39,
    "verse_content": "ذَٰلِكَ ٱلْيَوْمُ ٱلْحَقُّ ۖ فَمَن شَآءَ ٱتَّخَذَ إِلَىٰ رَبِّهِۦ مَـَٔابًا",
    "verse_key": "78:39"
  },
  {
    "verse_id": 40,
    "verse_content": "إِنَّآ أَنذَرْنَـٰكُمْ عَذَابًا قَرِيبًا يَوْمَ يَنظُرُ ٱلْمَرْءُ مَا قَدَّمَتْ يَدَاهُ وَيَقُولُ ٱلْكَافِرُ يَـٰلَيْتَنِى كُنتُ تُرَٰبًۢا",
    "verse_key": "78:40"
  }
]