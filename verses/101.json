[
  {
    "verse_id": 1,
    "verse_content": " ٱلْقَارِعَةُ",
    "verse_key": "101:1"
  },
  {
    "verse_id": 2,
    "verse_content": "مَا ٱلْقَارِعَةُ",
    "verse_key": "101:2"
  },
  {
    "verse_id": 3,
    "verse_content": "وَمَآ أَدْرَىٰكَ مَا ٱلْقَارِعَةُ",
    "verse_key": "101:3"
  },
  {
    "verse_id": 4,
    "verse_content": "يَوْمَ يَكُونُ ٱلنَّاسُ كَٱلْفَرَاشِ ٱلْمَبْثُوثِ",
    "verse_key": "101:4"
  },
  {
    "verse_id": 5,
    "verse_content": "وَتَكُونُ ٱلْجِبَالُ كَٱلْعِهْنِ ٱلْمَنفُوشِ",
    "verse_key": "101:5"
  },
  {
    "verse_id": 6,
    "verse_content": "فَأَمَّا مَن ثَقُلَتْ مَوَٰزِينُهُۥ",
    "verse_key": "101:6"
  },
  {
    "verse_id": 7,
    "verse_content": "فَهُوَ فِى عِيشَةٍ رَّاضِيَةٍ",
    "verse_key": "101:7"
  },
  {
    "verse_id": 8,
    "verse_content": "وَأَمَّا مَنْ خَفَّتْ مَوَٰزِينُهُۥ",
    "verse_key": "101:8"
  },
  {
    "verse_id": 9,
    "verse_content": "فَأُمُّهُۥ هَاوِيَةٌ",
    "verse_key": "101:9"
  },
  {
    "verse_id": 10,
    "verse_content": "وَمَآ أَدْرَىٰكَ مَا هِيَهْ",
    "verse_key": "101:10"
  },
  {
    "verse_id": 11,
    "verse_content": "نَارٌ حَامِيَةٌۢ",
    "verse_key": "101:11"
  }
]