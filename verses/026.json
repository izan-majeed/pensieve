[
  {
    "verse_id": 1,
    "verse_content": " طسٓمٓ",
    "verse_key": "26:1"
  },
  {
    "verse_id": 2,
    "verse_content": "تِلْكَ ءَايَـٰتُ ٱلْكِتَـٰبِ ٱلْمُبِينِ",
    "verse_key": "26:2"
  },
  {
    "verse_id": 3,
    "verse_content": "لَعَلَّكَ بَـٰخِعٌ نَّفْسَكَ أَلَّا يَكُونُوا۟ مُؤْمِنِينَ",
    "verse_key": "26:3"
  },
  {
    "verse_id": 4,
    "verse_content": "إِن نَّشَأْ نُنَزِّلْ عَلَيْهِم مِّنَ ٱلسَّمَآءِ ءَايَةً فَظَلَّتْ أَعْنَـٰقُهُمْ لَهَا خَـٰضِعِينَ",
    "verse_key": "26:4"
  },
  {
    "verse_id": 5,
    "verse_content": "وَمَا يَأْتِيهِم مِّن ذِكْرٍ مِّنَ ٱلرَّحْمَـٰنِ مُحْدَثٍ إِلَّا كَانُوا۟ عَنْهُ مُعْرِضِينَ",
    "verse_key": "26:5"
  },
  {
    "verse_id": 6,
    "verse_content": "فَقَدْ كَذَّبُوا۟ فَسَيَأْتِيهِمْ أَنۢبَـٰٓؤُا۟ مَا كَانُوا۟ بِهِۦ يَسْتَهْزِءُونَ",
    "verse_key": "26:6"
  },
  {
    "verse_id": 7,
    "verse_content": "أَوَلَمْ يَرَوْا۟ إِلَى ٱلْأَرْضِ كَمْ أَنۢبَتْنَا فِيهَا مِن كُلِّ زَوْجٍ كَرِيمٍ",
    "verse_key": "26:7"
  },
  {
    "verse_id": 8,
    "verse_content": "إِنَّ فِى ذَٰلِكَ لَـَٔايَةً ۖ وَمَا كَانَ أَكْثَرُهُم مُّؤْمِنِينَ",
    "verse_key": "26:8"
  },
  {
    "verse_id": 9,
    "verse_content": "وَإِنَّ رَبَّكَ لَهُوَ ٱلْعَزِيزُ ٱلرَّحِيمُ",
    "verse_key": "26:9"
  },
  {
    "verse_id": 10,
    "verse_content": "وَإِذْ نَادَىٰ رَبُّكَ مُوسَىٰٓ أَنِ ٱئْتِ ٱلْقَوْمَ ٱلظَّـٰلِمِينَ",
    "verse_key": "26:10"
  },
  {
    "verse_id": 11,
    "verse_content": "قَوْمَ فِرْعَوْنَ ۚ أَلَا يَتَّقُونَ",
    "verse_key": "26:11"
  },
  {
    "verse_id": 12,
    "verse_content": "قَالَ رَبِّ إِنِّىٓ أَخَافُ أَن يُكَذِّبُونِ",
    "verse_key": "26:12"
  },
  {
    "verse_id": 13,
    "verse_content": "وَيَضِيقُ صَدْرِى وَلَا يَنطَلِقُ لِسَانِى فَأَرْسِلْ إِلَىٰ هَـٰرُونَ",
    "verse_key": "26:13"
  },
  {
    "verse_id": 14,
    "verse_content": "وَلَهُمْ عَلَىَّ ذَنۢبٌ فَأَخَافُ أَن يَقْتُلُونِ",
    "verse_key": "26:14"
  },
  {
    "verse_id": 15,
    "verse_content": "قَالَ كَلَّا ۖ فَٱذْهَبَا بِـَٔايَـٰتِنَآ ۖ إِنَّا مَعَكُم مُّسْتَمِعُونَ",
    "verse_key": "26:15"
  },
  {
    "verse_id": 16,
    "verse_content": "فَأْتِيَا فِرْعَوْنَ فَقُولَآ إِنَّا رَسُولُ رَبِّ ٱلْعَـٰلَمِينَ",
    "verse_key": "26:16"
  },
  {
    "verse_id": 17,
    "verse_content": "أَنْ أَرْسِلْ مَعَنَا بَنِىٓ إِسْرَٰٓءِيلَ",
    "verse_key": "26:17"
  },
  {
    "verse_id": 18,
    "verse_content": "قَالَ أَلَمْ نُرَبِّكَ فِينَا وَلِيدًا وَلَبِثْتَ فِينَا مِنْ عُمُرِكَ سِنِينَ",
    "verse_key": "26:18"
  },
  {
    "verse_id": 19,
    "verse_content": "وَفَعَلْتَ فَعْلَتَكَ ٱلَّتِى فَعَلْتَ وَأَنتَ مِنَ ٱلْكَـٰفِرِينَ",
    "verse_key": "26:19"
  },
  {
    "verse_id": 20,
    "verse_content": "قَالَ فَعَلْتُهَآ إِذًا وَأَنَا۠ مِنَ ٱلضَّآلِّينَ",
    "verse_key": "26:20"
  },
  {
    "verse_id": 21,
    "verse_content": "فَفَرَرْتُ مِنكُمْ لَمَّا خِفْتُكُمْ فَوَهَبَ لِى رَبِّى حُكْمًا وَجَعَلَنِى مِنَ ٱلْمُرْسَلِينَ",
    "verse_key": "26:21"
  },
  {
    "verse_id": 22,
    "verse_content": "وَتِلْكَ نِعْمَةٌ تَمُنُّهَا عَلَىَّ أَنْ عَبَّدتَّ بَنِىٓ إِسْرَٰٓءِيلَ",
    "verse_key": "26:22"
  },
  {
    "verse_id": 23,
    "verse_content": "قَالَ فِرْعَوْنُ وَمَا رَبُّ ٱلْعَـٰلَمِينَ",
    "verse_key": "26:23"
  },
  {
    "verse_id": 24,
    "verse_content": "قَالَ رَبُّ ٱلسَّمَـٰوَٰتِ وَٱلْأَرْضِ وَمَا بَيْنَهُمَآ ۖ إِن كُنتُم مُّوقِنِينَ",
    "verse_key": "26:24"
  },
  {
    "verse_id": 25,
    "verse_content": "قَالَ لِمَنْ حَوْلَهُۥٓ أَلَا تَسْتَمِعُونَ",
    "verse_key": "26:25"
  },
  {
    "verse_id": 26,
    "verse_content": "قَالَ رَبُّكُمْ وَرَبُّ ءَابَآئِكُمُ ٱلْأَوَّلِينَ",
    "verse_key": "26:26"
  },
  {
    "verse_id": 27,
    "verse_content": "قَالَ إِنَّ رَسُولَكُمُ ٱلَّذِىٓ أُرْسِلَ إِلَيْكُمْ لَمَجْنُونٌ",
    "verse_key": "26:27"
  },
  {
    "verse_id": 28,
    "verse_content": "قَالَ رَبُّ ٱلْمَشْرِقِ وَٱلْمَغْرِبِ وَمَا بَيْنَهُمَآ ۖ إِن كُنتُمْ تَعْقِلُونَ",
    "verse_key": "26:28"
  },
  {
    "verse_id": 29,
    "verse_content": "قَالَ لَئِنِ ٱتَّخَذْتَ إِلَـٰهًا غَيْرِى لَأَجْعَلَنَّكَ مِنَ ٱلْمَسْجُونِينَ",
    "verse_key": "26:29"
  },
  {
    "verse_id": 30,
    "verse_content": "قَالَ أَوَلَوْ جِئْتُكَ بِشَىْءٍ مُّبِينٍ",
    "verse_key": "26:30"
  },
  {
    "verse_id": 31,
    "verse_content": "قَالَ فَأْتِ بِهِۦٓ إِن كُنتَ مِنَ ٱلصَّـٰدِقِينَ",
    "verse_key": "26:31"
  },
  {
    "verse_id": 32,
    "verse_content": "فَأَلْقَىٰ عَصَاهُ فَإِذَا هِىَ ثُعْبَانٌ مُّبِينٌ",
    "verse_key": "26:32"
  },
  {
    "verse_id": 33,
    "verse_content": "وَنَزَعَ يَدَهُۥ فَإِذَا هِىَ بَيْضَآءُ لِلنَّـٰظِرِينَ",
    "verse_key": "26:33"
  },
  {
    "verse_id": 34,
    "verse_content": "قَالَ لِلْمَلَإِ حَوْلَهُۥٓ إِنَّ هَـٰذَا لَسَـٰحِرٌ عَلِيمٌ",
    "verse_key": "26:34"
  },
  {
    "verse_id": 35,
    "verse_content": "يُرِيدُ أَن يُخْرِجَكُم مِّنْ أَرْضِكُم بِسِحْرِهِۦ فَمَاذَا تَأْمُرُونَ",
    "verse_key": "26:35"
  },
  {
    "verse_id": 36,
    "verse_content": "قَالُوٓا۟ أَرْجِهْ وَأَخَاهُ وَٱبْعَثْ فِى ٱلْمَدَآئِنِ حَـٰشِرِينَ",
    "verse_key": "26:36"
  },
  {
    "verse_id": 37,
    "verse_content": "يَأْتُوكَ بِكُلِّ سَحَّارٍ عَلِيمٍ",
    "verse_key": "26:37"
  },
  {
    "verse_id": 38,
    "verse_content": "فَجُمِعَ ٱلسَّحَرَةُ لِمِيقَـٰتِ يَوْمٍ مَّعْلُومٍ",
    "verse_key": "26:38"
  },
  {
    "verse_id": 39,
    "verse_content": "وَقِيلَ لِلنَّاسِ هَلْ أَنتُم مُّجْتَمِعُونَ",
    "verse_key": "26:39"
  },
  {
    "verse_id": 40,
    "verse_content": "لَعَلَّنَا نَتَّبِعُ ٱلسَّحَرَةَ إِن كَانُوا۟ هُمُ ٱلْغَـٰلِبِينَ",
    "verse_key": "26:40"
  },
  {
    "verse_id": 41,
    "verse_content": "فَلَمَّا جَآءَ ٱلسَّحَرَةُ قَالُوا۟ لِفِرْعَوْنَ أَئِنَّ لَنَا لَأَجْرًا إِن كُنَّا نَحْنُ ٱلْغَـٰلِبِينَ",
    "verse_key": "26:41"
  },
  {
    "verse_id": 42,
    "verse_content": "قَالَ نَعَمْ وَإِنَّكُمْ إِذًا لَّمِنَ ٱلْمُقَرَّبِينَ",
    "verse_key": "26:42"
  },
  {
    "verse_id": 43,
    "verse_content": "قَالَ لَهُم مُّوسَىٰٓ أَلْقُوا۟ مَآ أَنتُم مُّلْقُونَ",
    "verse_key": "26:43"
  },
  {
    "verse_id": 44,
    "verse_content": "فَأَلْقَوْا۟ حِبَالَهُمْ وَعِصِيَّهُمْ وَقَالُوا۟ بِعِزَّةِ فِرْعَوْنَ إِنَّا لَنَحْنُ ٱلْغَـٰلِبُونَ",
    "verse_key": "26:44"
  },
  {
    "verse_id": 45,
    "verse_content": "فَأَلْقَىٰ مُوسَىٰ عَصَاهُ فَإِذَا هِىَ تَلْقَفُ مَا يَأْفِكُونَ",
    "verse_key": "26:45"
  },
  {
    "verse_id": 46,
    "verse_content": "فَأُلْقِىَ ٱلسَّحَرَةُ سَـٰجِدِينَ",
    "verse_key": "26:46"
  },
  {
    "verse_id": 47,
    "verse_content": "قَالُوٓا۟ ءَامَنَّا بِرَبِّ ٱلْعَـٰلَمِينَ",
    "verse_key": "26:47"
  },
  {
    "verse_id": 48,
    "verse_content": "رَبِّ مُوسَىٰ وَهَـٰرُونَ",
    "verse_key": "26:48"
  },
  {
    "verse_id": 49,
    "verse_content": "قَالَ ءَامَنتُمْ لَهُۥ قَبْلَ أَنْ ءَاذَنَ لَكُمْ ۖ إِنَّهُۥ لَكَبِيرُكُمُ ٱلَّذِى عَلَّمَكُمُ ٱلسِّحْرَ فَلَسَوْفَ تَعْلَمُونَ ۚ لَأُقَطِّعَنَّ أَيْدِيَكُمْ وَأَرْجُلَكُم مِّنْ خِلَـٰفٍ وَلَأُصَلِّبَنَّكُمْ أَجْمَعِينَ",
    "verse_key": "26:49"
  },
  {
    "verse_id": 50,
    "verse_content": "قَالُوا۟ لَا ضَيْرَ ۖ إِنَّآ إِلَىٰ رَبِّنَا مُنقَلِبُونَ",
    "verse_key": "26:50"
  },
  {
    "verse_id": 51,
    "verse_content": "إِنَّا نَطْمَعُ أَن يَغْفِرَ لَنَا رَبُّنَا خَطَـٰيَـٰنَآ أَن كُنَّآ أَوَّلَ ٱلْمُؤْمِنِينَ",
    "verse_key": "26:51"
  },
  {
    "verse_id": 52,
    "verse_content": "۞ وَأَوْحَيْنَآ إِلَىٰ مُوسَىٰٓ أَنْ أَسْرِ بِعِبَادِىٓ إِنَّكُم مُّتَّبَعُونَ",
    "verse_key": "26:52"
  },
  {
    "verse_id": 53,
    "verse_content": "فَأَرْسَلَ فِرْعَوْنُ فِى ٱلْمَدَآئِنِ حَـٰشِرِينَ",
    "verse_key": "26:53"
  },
  {
    "verse_id": 54,
    "verse_content": "إِنَّ هَـٰٓؤُلَآءِ لَشِرْذِمَةٌ قَلِيلُونَ",
    "verse_key": "26:54"
  },
  {
    "verse_id": 55,
    "verse_content": "وَإِنَّهُمْ لَنَا لَغَآئِظُونَ",
    "verse_key": "26:55"
  },
  {
    "verse_id": 56,
    "verse_content": "وَإِنَّا لَجَمِيعٌ حَـٰذِرُونَ",
    "verse_key": "26:56"
  },
  {
    "verse_id": 57,
    "verse_content": "فَأَخْرَجْنَـٰهُم مِّن جَنَّـٰتٍ وَعُيُونٍ",
    "verse_key": "26:57"
  },
  {
    "verse_id": 58,
    "verse_content": "وَكُنُوزٍ وَمَقَامٍ كَرِيمٍ",
    "verse_key": "26:58"
  },
  {
    "verse_id": 59,
    "verse_content": "كَذَٰلِكَ وَأَوْرَثْنَـٰهَا بَنِىٓ إِسْرَٰٓءِيلَ",
    "verse_key": "26:59"
  },
  {
    "verse_id": 60,
    "verse_content": "فَأَتْبَعُوهُم مُّشْرِقِينَ",
    "verse_key": "26:60"
  },
  {
    "verse_id": 61,
    "verse_content": "فَلَمَّا تَرَٰٓءَا ٱلْجَمْعَانِ قَالَ أَصْحَـٰبُ مُوسَىٰٓ إِنَّا لَمُدْرَكُونَ",
    "verse_key": "26:61"
  },
  {
    "verse_id": 62,
    "verse_content": "قَالَ كَلَّآ ۖ إِنَّ مَعِىَ رَبِّى سَيَهْدِينِ",
    "verse_key": "26:62"
  },
  {
    "verse_id": 63,
    "verse_content": "فَأَوْحَيْنَآ إِلَىٰ مُوسَىٰٓ أَنِ ٱضْرِب بِّعَصَاكَ ٱلْبَحْرَ ۖ فَٱنفَلَقَ فَكَانَ كُلُّ فِرْقٍ كَٱلطَّوْدِ ٱلْعَظِيمِ",
    "verse_key": "26:63"
  },
  {
    "verse_id": 64,
    "verse_content": "وَأَزْلَفْنَا ثَمَّ ٱلْـَٔاخَرِينَ",
    "verse_key": "26:64"
  },
  {
    "verse_id": 65,
    "verse_content": "وَأَنجَيْنَا مُوسَىٰ وَمَن مَّعَهُۥٓ أَجْمَعِينَ",
    "verse_key": "26:65"
  },
  {
    "verse_id": 66,
    "verse_content": "ثُمَّ أَغْرَقْنَا ٱلْـَٔاخَرِينَ",
    "verse_key": "26:66"
  },
  {
    "verse_id": 67,
    "verse_content": "إِنَّ فِى ذَٰلِكَ لَـَٔايَةً ۖ وَمَا كَانَ أَكْثَرُهُم مُّؤْمِنِينَ",
    "verse_key": "26:67"
  },
  {
    "verse_id": 68,
    "verse_content": "وَإِنَّ رَبَّكَ لَهُوَ ٱلْعَزِيزُ ٱلرَّحِيمُ",
    "verse_key": "26:68"
  },
  {
    "verse_id": 69,
    "verse_content": "وَٱتْلُ عَلَيْهِمْ نَبَأَ إِبْرَٰهِيمَ",
    "verse_key": "26:69"
  },
  {
    "verse_id": 70,
    "verse_content": "إِذْ قَالَ لِأَبِيهِ وَقَوْمِهِۦ مَا تَعْبُدُونَ",
    "verse_key": "26:70"
  },
  {
    "verse_id": 71,
    "verse_content": "قَالُوا۟ نَعْبُدُ أَصْنَامًا فَنَظَلُّ لَهَا عَـٰكِفِينَ",
    "verse_key": "26:71"
  },
  {
    "verse_id": 72,
    "verse_content": "قَالَ هَلْ يَسْمَعُونَكُمْ إِذْ تَدْعُونَ",
    "verse_key": "26:72"
  },
  {
    "verse_id": 73,
    "verse_content": "أَوْ يَنفَعُونَكُمْ أَوْ يَضُرُّونَ",
    "verse_key": "26:73"
  },
  {
    "verse_id": 74,
    "verse_content": "قَالُوا۟ بَلْ وَجَدْنَآ ءَابَآءَنَا كَذَٰلِكَ يَفْعَلُونَ",
    "verse_key": "26:74"
  },
  {
    "verse_id": 75,
    "verse_content": "قَالَ أَفَرَءَيْتُم مَّا كُنتُمْ تَعْبُدُونَ",
