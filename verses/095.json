[
  {
    "verse_id": 1,
    "verse_content": "وَٱلتِّينِ وَٱلزَّيْتُونِ",
    "verse_key": "95:1"
  },
  {
    "verse_id": 2,
    "verse_content": "وَطُورِ سِينِينَ",
    "verse_key": "95:2"
  },
  {
    "verse_id": 3,
    "verse_content": "وَهَـٰذَا ٱلْبَلَدِ ٱلْأَمِينِ",
    "verse_key": "95:3"
  },
  {
    "verse_id": 4,
    "verse_content": "لَقَدْ خَلَقْنَا ٱلْإِنسَـٰنَ فِىٓ أَحْسَنِ تَقْوِيمٍ",
    "verse_key": "95:4"
  },
  {
    "verse_id": 5,
    "verse_content": "ثُمَّ رَدَدْنَـٰهُ أَسْفَلَ سَـٰفِلِينَ",
    "verse_key": "95:5"
  },
  {
    "verse_id": 6,
    "verse_content": "إِلَّا ٱلَّذِينَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّـٰلِحَـٰتِ فَلَهُمْ أَجْرٌ غَيْرُ مَمْنُونٍ",
    "verse_key": "95:6"
  },
  {
    "verse_id": 7,
    "verse_content": "فَمَا يُكَذِّبُكَ بَعْدُ بِٱلدِّينِ",
    "verse_key": "95:7"
  },
  {
    "verse_id": 8,
    "verse_content": "أَلَيْسَ ٱللَّهُ بِأَحْكَمِ ٱلْحَـٰكِمِينَ",
    "verse_key": "95:8"
  }
]