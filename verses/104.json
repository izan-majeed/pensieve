[
  {
    "verse_id": 1,
    "verse_content": " وَيْلٌ لِّكُلِّ هُمَزَةٍ لُّمَزَةٍ",
    "verse_key": "104:1"
  },
  {
    "verse_id": 2,
    "verse_content": "ٱلَّذِى جَمَعَ مَالًا وَعَدَّدَهُۥ",
    "verse_key": "104:2"
  },
  {
    "verse_id": 3,
    "verse_content": "يَحْسَبُ أَنَّ مَالَهُۥٓ أَخْلَدَهُۥ",
    "verse_key": "104:3"
  },
  {
    "verse_id": 4,
    "verse_content": "كَلَّا ۖ لَيُنۢبَذَنَّ فِى ٱلْحُطَمَةِ",
    "verse_key": "104:4"
  },
  {
    "verse_id": 5,
    "verse_content": "وَمَآ أَدْرَىٰكَ مَا ٱلْحُطَمَةُ",
    "verse_key": "104:5"
  },
  {
    "verse_id": 6,
    "verse_content": "نَارُ ٱللَّهِ ٱلْمُوقَدَةُ",
    "verse_key": "104:6"
  },
  {
    "verse_id": 7,
    "verse_content": "ٱلَّتِى تَطَّلِعُ عَلَى ٱلْأَفْـِٔدَةِ",
    "verse_key": "104:7"
  },
  {
    "verse_id": 8,
    "verse_content": "إِنَّهَا عَلَيْهِم مُّؤْصَدَةٌ",
    "verse_key": "104:8"
  },
  {
    "verse_id": 9,
    "verse_content": "فِى عَمَدٍ مُّمَدَّدَةٍۭ",
    "verse_key": "104:9"
  }
]