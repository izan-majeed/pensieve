[
  {
    "verse_id": 1,
    "verse_content": " وَٱلسَّمَآءِ وَٱلطَّارِقِ",
    "verse_key": "86:1"
  },
  {
    "verse_id": 2,
    "verse_content": "وَمَآ أَدْرَىٰكَ مَا ٱلطَّارِقُ",
    "verse_key": "86:2"
  },
  {
    "verse_id": 3,
    "verse_content": "ٱلنَّجْمُ ٱلثَّاقِبُ",
    "verse_key": "86:3"
  },
  {
    "verse_id": 4,
    "verse_content": "إِن كُلُّ نَفْسٍ لَّمَّا عَلَيْهَا حَافِظٌ",
    "verse_key": "86:4"
  },
  {
    "verse_id": 5,
    "verse_content": "فَلْيَنظُرِ ٱلْإِنسَـٰنُ مِمَّ خُلِقَ",
    "verse_key": "86:5"
  },
  {
    "verse_id": 6,
    "verse_content": "خُلِقَ مِن مَّآءٍ دَافِقٍ",
    "verse_key": "86:6"
  },
  {
    "verse_id": 7,
    "verse_content": "يَخْرُجُ مِنۢ بَيْنِ ٱلصُّلْبِ وَٱلتَّرَآئِبِ",
    "verse_key": "86:7"
  },
  {
    "verse_id": 8,
    "verse_content": "إِنَّهُۥ عَلَىٰ رَجْعِهِۦ لَقَادِرٌ",
    "verse_key": "86:8"
  },
  {
    "verse_id": 9,
    "verse_content": "يَوْمَ تُبْلَى ٱلسَّرَآئِرُ",
    "verse_key": "86:9"
  },
  {
    "verse_id": 10,
    "verse_content": "فَمَا لَهُۥ مِن قُوَّةٍ وَلَا نَاصِرٍ",
    "verse_key": "86:10"
  },
  {
    "verse_id": 11,
    "verse_content": "وَٱلسَّمَآءِ ذَاتِ ٱلرَّجْعِ",
    "verse_key": "86:11"
  },
  {
    "verse_id": 12,
    "verse_content": "وَٱلْأَرْضِ ذَاتِ ٱلصَّدْعِ",
    "verse_key": "86:12"
  },
  {
    "verse_id": 13,
    "verse_content": "إِنَّهُۥ لَقَوْلٌ فَصْلٌ",
    "verse_key": "86:13"
  },
  {
    "verse_id": 14,
    "verse_content": "وَمَا هُوَ بِٱلْهَزْلِ",
    "verse_key": "86:14"
  },
  {
    "verse_id": 15,
    "verse_content": "إِنَّهُمْ يَكِيدُونَ كَيْدًا",
    "verse_key": "86:15"
  },
  {
    "verse_id": 16,
    "verse_content": "وَأَكِيدُ كَيْدًا",
    "verse_key": "86:16"
  },
  {
    "verse_id": 17,
    "verse_content": "فَمَهِّلِ ٱلْكَـٰفِرِينَ أَمْهِلْهُمْ رُوَيْدًۢا",
    "verse_key": "86:17"
  }
]