[
  {
    "verse_id": 1,
    "verse_content": " إِذَا وَقَعَتِ ٱلْوَاقِعَةُ",
    "verse_key": "56:1"
  },
  {
    "verse_id": 2,
    "verse_content": "لَيْسَ لِوَقْعَتِهَا كَاذِبَةٌ",
    "verse_key": "56:2"
  },
  {
    "verse_id": 3,
    "verse_content": "خَافِضَةٌ رَّافِعَةٌ",
    "verse_key": "56:3"
  },
  {
    "verse_id": 4,
    "verse_content": "إِذَا رُجَّتِ ٱلْأَرْضُ رَجًّا",
    "verse_key": "56:4"
  },
  {
    "verse_id": 5,
    "verse_content": "وَبُسَّتِ ٱلْجِبَالُ بَسًّا",
    "verse_key": "56:5"
  },
  {
    "verse_id": 6,
    "verse_content": "فَكَانَتْ هَبَآءً مُّنۢبَثًّا",
    "verse_key": "56:6"
  },
  {
    "verse_id": 7,
    "verse_content": "وَكُنتُمْ أَزْوَٰجًا ثَلَـٰثَةً",
    "verse_key": "56:7"
  },
  {
    "verse_id": 8,
    "verse_content": "فَأَصْحَـٰبُ ٱلْمَيْمَنَةِ مَآ أَصْحَـٰبُ ٱلْمَيْمَنَةِ",
    "verse_key": "56:8"
  },
  {
    "verse_id": 9,
    "verse_content": "وَأَصْحَـٰبُ ٱلْمَشْـَٔمَةِ مَآ أَصْحَـٰبُ ٱلْمَشْـَٔمَةِ",
    "verse_key": "56:9"
  },
  {
    "verse_id": 10,
    "verse_content": "وَٱلسَّـٰبِقُونَ ٱلسَّـٰبِقُونَ",
    "verse_key": "56:10"
  },
  {
    "verse_id": 11,
    "verse_content": "أُو۟لَـٰٓئِكَ ٱلْمُقَرَّبُونَ",
    "verse_key": "56:11"
  },
  {
    "verse_id": 12,
    "verse_content": "فِى جَنَّـٰتِ ٱلنَّعِيمِ",
    "verse_key": "56:12"
  },
  {
    "verse_id": 13,
    "verse_content": "ثُلَّةٌ مِّنَ ٱلْأَوَّلِينَ",
    "verse_key": "56:13"
  },
  {
    "verse_id": 14,
    "verse_content": "وَقَلِيلٌ مِّنَ ٱلْـَٔاخِرِينَ",
    "verse_key": "56:14"
  },
  {
    "verse_id": 15,
    "verse_content": "عَلَىٰ سُرُرٍ مَّوْضُونَةٍ",
    "verse_key": "56:15"
  },
  {
    "verse_id": 16,
    "verse_content": "مُّتَّكِـِٔينَ عَلَيْهَا مُتَقَـٰبِلِينَ",
    "verse_key": "56:16"
  },
  {
    "verse_id": 17,
    "verse_content": "يَطُوفُ عَلَيْهِمْ وِلْدَٰنٌ مُّخَلَّدُونَ",
    "verse_key": "56:17"
  },
  {
    "verse_id": 18,
    "verse_content": "بِأَكْوَابٍ وَأَبَارِيقَ وَكَأْسٍ مِّن مَّعِينٍ",
    "verse_key": "56:18"
  },
  {
    "verse_id": 19,
    "verse_content": "لَّا يُصَدَّعُونَ عَنْهَا وَلَا يُنزِفُونَ",
    "verse_key": "56:19"
  },
  {
    "verse_id": 20,
    "verse_content": "وَفَـٰكِهَةٍ مِّمَّا يَتَخَيَّرُونَ",
    "verse_key": "56:20"
  },
  {
    "verse_id": 21,
    "verse_content": "وَلَحْمِ طَيْرٍ مِّمَّا يَشْتَهُونَ",
    "verse_key": "56:21"
  },
  {
    "verse_id": 22,
    "verse_content": "وَحُورٌ عِينٌ",
    "verse_key": "56:22"
  },
  {
    "verse_id": 23,
    "verse_content": "كَأَمْثَـٰلِ ٱللُّؤْلُؤِ ٱلْمَكْنُونِ",
    "verse_key": "56:23"
  },
  {
    "verse_id": 24,
    "verse_content": "جَزَآءًۢ بِمَا كَانُوا۟ يَعْمَلُونَ",
    "verse_key": "56:24"
  },
  {
    "verse_id": 25,
    "verse_content": "لَا يَسْمَعُونَ فِيهَا لَغْوًا وَلَا تَأْثِيمًا",
    "verse_key": "56:25"
  },
  {
    "verse_id": 26,
    "verse_content": "إِلَّا قِيلًا سَلَـٰمًا سَلَـٰمًا",
    "verse_key": "56:26"
  },
  {
    "verse_id": 27,
    "verse_content": "وَأَصْحَـٰبُ ٱلْيَمِينِ مَآ أَصْحَـٰبُ ٱلْيَمِينِ",
    "verse_key": "56:27"
  },
  {
    "verse_id": 28,
    "verse_content": "فِى سِدْرٍ مَّخْضُودٍ",
    "verse_key": "56:28"
  },
  {
    "verse_id": 29,
    "verse_content": "وَطَلْحٍ مَّنضُودٍ",
    "verse_key": "56:29"
  },
  {
    "verse_id": 30,
    "verse_content": "وَظِلٍّ مَّمْدُودٍ",
    "verse_key": "56:30"
  },
  {
    "verse_id": 31,
    "verse_content": "وَمَآءٍ مَّسْكُوبٍ",
    "verse_key": "56:31"
  },
  {
    "verse_id": 32,
    "verse_content": "وَفَـٰكِهَةٍ كَثِيرَةٍ",
    "verse_key": "56:32"
  },
  {
    "verse_id": 33,
    "verse_content": "لَّا مَقْطُوعَةٍ وَلَا مَمْنُوعَةٍ",
    "verse_key": "56:33"
  },
  {
    "verse_id": 34,
    "verse_content": "وَفُرُشٍ مَّرْفُوعَةٍ",
    "verse_key": "56:34"
  },
  {
    "verse_id": 35,
    "verse_content": "إِنَّآ أَنشَأْنَـٰهُنَّ إِنشَآءً",
    "verse_key": "56:35"
  },
  {
    "verse_id": 36,
    "verse_content": "فَجَعَلْنَـٰهُنَّ أَبْكَارًا",
    "verse_key": "56:36"
  },
  {
    "verse_id": 37,
    "verse_content": "عُرُبًا أَتْرَابًا",
    "verse_key": "56:37"
  },
  {
    "verse_id": 38,
    "verse_content": "لِّأَصْحَـٰبِ ٱلْيَمِينِ",
    "verse_key": "56:38"
  },
  {
    "verse_id": 39,
    "verse_content": "ثُلَّةٌ مِّنَ ٱلْأَوَّلِينَ",
    "verse_key": "56:39"
  },
  {
    "verse_id": 40,
    "verse_content": "وَثُلَّةٌ مِّنَ ٱلْـَٔاخِرِينَ",
    "verse_key": "56:40"
  },
  {
    "verse_id": 41,
    "verse_content": "وَأَصْحَـٰبُ ٱلشِّمَالِ مَآ أَصْحَـٰبُ ٱلشِّمَالِ",
    "verse_key": "56:41"
  },
  {
    "verse_id": 42,
    "verse_content": "فِى سَمُومٍ وَحَمِيمٍ",
    "verse_key": "56:42"
  },
  {
    "verse_id": 43,
    "verse_content": "وَظِلٍّ مِّن يَحْمُومٍ",
    "verse_key": "56:43"
  },
  {
    "verse_id": 44,
    "verse_content": "لَّا بَارِدٍ وَلَا كَرِيمٍ",
    "verse_key": "56:44"
  },
  {
    "verse_id": 45,
    "verse_content": "إِنَّهُمْ كَانُوا۟ قَبْلَ ذَٰلِكَ مُتْرَفِينَ",
    "verse_key": "56:45"
  },
  {
    "verse_id": 46,
    "verse_content": "وَكَانُوا۟ يُصِرُّونَ عَلَى ٱلْحِنثِ ٱلْعَظِيمِ",
    "verse_key": "56:46"
  },
  {
    "verse_id": 47,
    "verse_content": "وَكَانُوا۟ يَقُولُونَ أَئِذَا مِتْنَا وَكُنَّا تُرَابًا وَعِظَـٰمًا أَءِنَّا لَمَبْعُوثُونَ",
    "verse_key": "56:47"
  },
  {
    "verse_id": 48,
    "verse_content": "أَوَءَابَآؤُنَا ٱلْأَوَّلُونَ",
    "verse_key": "56:48"
  },
  {
    "verse_id": 49,
    "verse_content": "قُلْ إِنَّ ٱلْأَوَّلِينَ وَٱلْـَٔاخِرِينَ",
    "verse_key": "56:49"
  },
  {
    "verse_id": 50,
    "verse_content": "لَمَجْمُوعُونَ إِلَىٰ مِيقَـٰتِ يَوْمٍ مَّعْلُومٍ",
    "verse_key": "56:50"
  },
  {
    "verse_id": 51,
    "verse_content": "ثُمَّ إِنَّكُمْ أَيُّهَا ٱلضَّآلُّونَ ٱلْمُكَذِّبُونَ",
    "verse_key": "56:51"
  },
  {
    "verse_id": 52,
    "verse_content": "لَـَٔاكِلُونَ مِن شَجَرٍ مِّن زَقُّومٍ",
    "verse_key": "56:52"
  },
  {
    "verse_id": 53,
    "verse_content": "فَمَالِـُٔونَ مِنْهَا ٱلْبُطُونَ",
    "verse_key": "56:53"
  },
  {
    "verse_id": 54,
    "verse_content": "فَشَـٰرِبُونَ عَلَيْهِ مِنَ ٱلْحَمِيمِ",
    "verse_key": "56:54"
  },
  {
    "verse_id": 55,
    "verse_content": "فَشَـٰرِبُونَ شُرْبَ ٱلْهِيمِ",
    "verse_key": "56:55"
  },
  {
    "verse_id": 56,
    "verse_content": "هَـٰذَا نُزُلُهُمْ يَوْمَ ٱلدِّينِ",
    "verse_key": "56:56"
  },
  {
    "verse_id": 57,
    "verse_content": "نَحْنُ خَلَقْنَـٰكُمْ فَلَوْلَا تُصَدِّقُونَ",
    "verse_key": "56:57"
  },
  {
    "verse_id": 58,
    "verse_content": "أَفَرَءَيْتُم مَّا تُمْنُونَ",
    "verse_key": "56:58"
  },
  {
    "verse_id": 59,
    "verse_content": "ءَأَنتُمْ تَخْلُقُونَهُۥٓ أَمْ نَحْنُ ٱلْخَـٰلِقُونَ",
    "verse_key": "56:59"
  },
  {
    "verse_id": 60,
    "verse_content": "نَحْنُ قَدَّرْنَا بَيْنَكُمُ ٱلْمَوْتَ وَمَا نَحْنُ بِمَسْبُوقِينَ",
    "verse_key": "56:60"
  },
  {
    "verse_id": 61,
    "verse_content": "عَلَىٰٓ أَن نُّبَدِّلَ أَمْثَـٰلَكُمْ وَنُنشِئَكُمْ فِى مَا لَا تَعْلَمُونَ",
    "verse_key": "56:61"
  },
  {
    "verse_id": 62,
    "verse_content": "وَلَقَدْ عَلِمْتُمُ ٱلنَّشْأَةَ ٱلْأُولَىٰ فَلَوْلَا تَذَكَّرُونَ",
    "verse_key": "56:62"
  },
  {
    "verse_id": 63,
    "verse_content": "أَفَرَءَيْتُم مَّا تَحْرُثُونَ",
    "verse_key": "56:63"
  },
  {
    "verse_id": 64,
    "verse_content": "ءَأَنتُمْ تَزْرَعُونَهُۥٓ أَمْ نَحْنُ ٱلزَّٰرِعُونَ",
    "verse_key": "56:64"
  },
  {
    "verse_id": 65,
    "verse_content": "لَوْ نَشَآءُ لَجَعَلْنَـٰهُ حُطَـٰمًا فَظَلْتُمْ تَفَكَّهُونَ",
    "verse_key": "56:65"
  },
  {
    "verse_id": 66,
    "verse_content": "إِنَّا لَمُغْرَمُونَ",
    "verse_key": "56:66"
  },
  {
    "verse_id": 67,
    "verse_content": "بَلْ نَحْنُ مَحْرُومُونَ",
    "verse_key": "56:67"
  },
  {
    "verse_id": 68,
    "verse_content": "أَفَرَءَيْتُمُ ٱلْمَآءَ ٱلَّذِى تَشْرَبُونَ",
    "verse_key": "56:68"
  },
  {
    "verse_id": 69,
    "verse_content": "ءَأَنتُمْ أَنزَلْتُمُوهُ مِنَ ٱلْمُزْنِ أَمْ نَحْنُ ٱلْمُنزِلُونَ",
    "verse_key": "56:69"
  },
  {
    "verse_id": 70,
    "verse_content": "لَوْ نَشَآءُ جَعَلْنَـٰهُ أُجَاجًا فَلَوْلَا تَشْكُرُونَ",
    "verse_key": "56:70"
  },
  {
    "verse_id": 71,
    "verse_content": "أَفَرَءَيْتُمُ ٱلنَّارَ ٱلَّتِى تُورُونَ",
    "verse_key": "56:71"
  },
  {
    "verse_id": 72,
    "verse_content": "ءَأَنتُمْ أَنشَأْتُمْ شَجَرَتَهَآ أَمْ نَحْنُ ٱلْمُنشِـُٔونَ",
    "verse_key": "56:72"
  },
  {
    "verse_id": 73,
    "verse_content": "نَحْنُ جَعَلْنَـٰهَا تَذْكِرَةً وَمَتَـٰعًا لِّلْمُقْوِينَ",
    "verse_key": "56:73"
  },
  {
    "verse_id": 74,
    "verse_content": "فَسَبِّحْ بِٱسْمِ رَبِّكَ ٱلْعَظِيمِ",
    "verse_key": "56:74"
  },
  {
    "verse_id": 75,
    "verse_content": "۞ فَلَآ أُقْسِمُ بِمَوَٰقِعِ ٱلنُّجُومِ",
    "verse_key": "56:75"
  },
  {
    "verse_id": 76,
    "verse_content": "وَإِنَّهُۥ لَقَسَمٌ لَّوْ تَعْلَمُونَ عَظِيمٌ",
    "verse_key": "56:76"
  },
  {
    "verse_id": 77,
    "verse_content": "إِنَّهُۥ لَقُرْءَانٌ كَرِيمٌ",
    "verse_key": "56:77"
  },
  {
    "verse_id": 78,
    "verse_content": "فِى كِتَـٰبٍ مَّكْنُونٍ",
    "verse_key": "56:78"
  },
  {
    "verse_id": 79,
    "verse_content": "لَّا يَمَسُّهُۥٓ إِلَّا ٱلْمُطَهَّرُونَ",
    "verse_key": "56:79"
  },
  {
    "verse_id": 80,
    "verse_content": "تَنزِيلٌ مِّن رَّبِّ ٱلْعَـٰلَمِينَ",
    "verse_key": "56:80"
  },
  {
    "verse_id": 81,
    "verse_content": "أَفَبِهَـٰذَا ٱلْحَدِيثِ أَنتُم مُّدْهِنُونَ",
    "verse_key": "56:81"
  },
  {
    "verse_id": 82,
    "verse_content": "وَتَجْعَلُونَ رِزْقَكُمْ أَنَّكُمْ تُكَذِّبُونَ",
    "verse_key": "56:82"
  },
  {
    "verse_id": 83,
    "verse_content": "فَلَوْلَآ إِذَا بَلَغَتِ ٱلْحُلْقُومَ",
    "verse_key": "56:83"
  },
  {
    "verse_id": 84,
    "verse_content": "وَأَنتُمْ حِينَئِذٍ تَنظُرُونَ",
    "verse_key": "56:84"
  },
  {
    "verse_id": 85,
    "verse_content": "وَنَحْنُ أَقْرَبُ إِلَيْهِ مِنكُمْ وَلَـٰكِن لَّا تُبْصِرُونَ",
    "verse_key": "56:85"
  },
  {
    "verse_id": 86,
    "verse_content": "فَلَوْلَآ إِن كُنتُمْ غَيْرَ مَدِينِينَ",
    "verse_key": "56:86"
  },
  {
    "verse_id": 87,
    "verse_content": "تَرْجِعُونَهَآ إِن كُنتُمْ صَـٰدِقِينَ",
    "verse_key": "56:87"
  },
  {
    "verse_id": 88,
    "verse_content": "فَأَمَّآ إِن كَانَ مِنَ ٱلْمُقَرَّبِينَ",
    "verse_key": "56:88"
  },
  {
    "verse_id": 89,
    "verse_content": "فَرَوْحٌ وَرَيْحَانٌ وَجَنَّتُ نَعِيمٍ",
    "verse_key": "56:89"
  },
  {
    "verse_id": 90,
    "verse_content": "وَأَمَّآ إِن كَانَ مِنْ أَصْحَـٰبِ ٱلْيَمِينِ",
    "verse_key": "56:90"
  },
  {
    "verse_id": 91,
    "verse_content": "فَسَلَـٰمٌ لَّكَ مِنْ أَصْحَـٰبِ ٱلْيَمِينِ",
    "verse_key": "56:91"
  },
  {
    "verse_id": 92,
    "verse_content": "وَأَمَّآ إِن كَانَ مِنَ ٱلْمُكَذِّبِينَ ٱلضَّآلِّينَ",
    "verse_key": "56:92"
  },
  {
    "verse_id": 93,
    "verse_content": "فَنُزُلٌ مِّنْ حَمِيمٍ",
    "verse_key": "56:93"
  },
  {
    "verse_id": 94,
    "verse_content": "وَتَصْلِيَةُ جَحِيمٍ",
    "verse_key": "56:94"
  },
  {
    "verse_id": 95,
    "verse_content": "إِنَّ هَـٰذَا لَهُوَ حَقُّ ٱلْيَقِينِ",
    "verse_key": "56:95"
  },
