[
  {
    "verse_id": 1,
    "translation": "WalAAadiyati dabha"
  },
  {
    "verse_id": 2,
    "translation": "Falmooriyati qadha"
  },
  {
    "verse_id": 3,
    "translation": "Falmugheerati subha"
  },
  {
    "verse_id": 4,
    "translation": "Faatharna bihi naqAAa"
  },
  {
    "verse_id": 5,
    "translation": "Fawasatna bihi jamAAa"
  },
  {
    "verse_id": 6,
    "translation": "Inna al-insana lirabbihi lakanood"
  },
  {
    "verse_id": 7,
    "translation": "Wa-innahu AAala thalikalashaheed"
  },
  {
    "verse_id": 8,
    "translation": "Wa-innahu lihubbi alkhayri lashadeed"
  },
  {
    "verse_id": 9,
    "translation": "Afala yaAAlamu itha buAAthira mafee alquboor"
  },
  {
    "verse_id": 10,
    "translation": "Wahussila ma fee assudoor"
  },
  {
    "verse_id": 11,
    "translation": "Inna rabbahum bihim yawma-ithinlakhabeer"
  }
]