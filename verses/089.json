[
  {
    "verse_id": 1,
    "verse_content": " وَٱلْفَجْرِ",
    "verse_key": "89:1"
  },
  {
    "verse_id": 2,
    "verse_content": "وَلَيَالٍ عَشْرٍ",
    "verse_key": "89:2"
  },
  {
    "verse_id": 3,
    "verse_content": "وَٱلشَّفْعِ وَٱلْوَتْرِ",
    "verse_key": "89:3"
  },
  {
    "verse_id": 4,
    "verse_content": "وَٱلَّيْلِ إِذَا يَسْرِ",
    "verse_key": "89:4"
  },
  {
    "verse_id": 5,
    "verse_content": "هَلْ فِى ذَٰلِكَ قَسَمٌ لِّذِى حِجْرٍ",
    "verse_key": "89:5"
  },
  {
    "verse_id": 6,
    "verse_content": "أَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِعَادٍ",
    "verse_key": "89:6"
  },
  {
    "verse_id": 7,
    "verse_content": "إِرَمَ ذَاتِ ٱلْعِمَادِ",
    "verse_key": "89:7"
  },
  {
    "verse_id": 8,
    "verse_content": "ٱلَّتِى لَمْ يُخْلَقْ مِثْلُهَا فِى ٱلْبِلَـٰدِ",
    "verse_key": "89:8"
  },
  {
    "verse_id": 9,
    "verse_content": "وَثَمُودَ ٱلَّذِينَ جَابُوا۟ ٱلصَّخْرَ بِٱلْوَادِ",
    "verse_key": "89:9"
  },
  {
    "verse_id": 10,
    "verse_content": "وَفِرْعَوْنَ ذِى ٱلْأَوْتَادِ",
    "verse_key": "89:10"
  },
  {
    "verse_id": 11,
    "verse_content": "ٱلَّذِينَ طَغَوْا۟ فِى ٱلْبِلَـٰدِ",
    "verse_key": "89:11"
  },
  {
    "verse_id": 12,
    "verse_content": "فَأَكْثَرُوا۟ فِيهَا ٱلْفَسَادَ",
    "verse_key": "89:12"
  },
  {
    "verse_id": 13,
    "verse_content": "فَصَبَّ عَلَيْهِمْ رَبُّكَ سَوْطَ عَذَابٍ",
    "verse_key": "89:13"
  },
  {
    "verse_id": 14,
    "verse_content": "إِنَّ رَبَّكَ لَبِٱلْمِرْصَادِ",
    "verse_key": "89:14"
  },
  {
    "verse_id": 15,
    "verse_content": "فَأَمَّا ٱلْإِنسَـٰنُ إِذَا مَا ٱبْتَلَىٰهُ رَبُّهُۥ فَأَكْرَمَهُۥ وَنَعَّمَهُۥ فَيَقُولُ رَبِّىٓ أَكْرَمَنِ",
    "verse_key": "89:15"
  },
  {
    "verse_id": 16,
    "verse_content": "وَأَمَّآ إِذَا مَا ٱبْتَلَىٰهُ فَقَدَرَ عَلَيْهِ رِزْقَهُۥ فَيَقُولُ رَبِّىٓ أَهَـٰنَنِ",
    "verse_key": "89:16"
  },
  {
    "verse_id": 17,
    "verse_content": "كَلَّا ۖ بَل لَّا تُكْرِمُونَ ٱلْيَتِيمَ",
    "verse_key": "89:17"
  },
  {
    "verse_id": 18,
    "verse_content": "وَلَا تَحَـٰٓضُّونَ عَلَىٰ طَعَامِ ٱلْمِسْكِينِ",
    "verse_key": "89:18"
  },
  {
    "verse_id": 19,
    "verse_content": "وَتَأْكُلُونَ ٱلتُّرَاثَ أَكْلًا لَّمًّا",
    "verse_key": "89:19"
  },
  {
    "verse_id": 20,
    "verse_content": "وَتُحِبُّونَ ٱلْمَالَ حُبًّا جَمًّا",
    "verse_key": "89:20"
  },
  {
    "verse_id": 21,
    "verse_content": "كَلَّآ إِذَا دُكَّتِ ٱلْأَرْضُ دَكًّا دَكًّا",
    "verse_key": "89:21"
  },
  {
    "verse_id": 22,
    "verse_content": "وَجَآءَ رَبُّكَ وَٱلْمَلَكُ صَفًّا صَفًّا",
    "verse_key": "89:22"
  },
  {
    "verse_id": 23,
    "verse_content": "وَجِا۟ىٓءَ يَوْمَئِذٍۭ بِجَهَنَّمَ ۚ يَوْمَئِذٍ يَتَذَكَّرُ ٱلْإِنسَـٰنُ وَأَنَّىٰ لَهُ ٱلذِّكْرَىٰ",
    "verse_key": "89:23"
  },
  {
    "verse_id": 24,
    "verse_content": "يَقُولُ يَـٰلَيْتَنِى قَدَّمْتُ لِحَيَاتِى",
    "verse_key": "89:24"
  },
  {
    "verse_id": 25,
    "verse_content": "فَيَوْمَئِذٍ لَّا يُعَذِّبُ عَذَابَهُۥٓ أَحَدٌ",
    "verse_key": "89:25"
  },
  {
    "verse_id": 26,
    "verse_content": "وَلَا يُوثِقُ وَثَاقَهُۥٓ أَحَدٌ",
    "verse_key": "89:26"
  },
  {
    "verse_id": 27,
    "verse_content": "يَـٰٓأَيَّتُهَا ٱلنَّفْسُ ٱلْمُطْمَئِنَّةُ",
    "verse_key": "89:27"
  },
  {
    "verse_id": 28,
    "verse_content": "ٱرْجِعِىٓ إِلَىٰ رَبِّكِ رَاضِيَةً مَّرْضِيَّةً",
    "verse_key": "89:28"
  },
  {
    "verse_id": 29,
    "verse_content": "فَٱدْخُلِى فِى عِبَـٰدِى",
    "verse_key": "89:29"
  },
  {
    "verse_id": 30,
    "verse_content": "وَٱدْخُلِى جَنَّتِى",
    "verse_key": "89:30"
  }
]