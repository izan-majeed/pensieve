[
  {
    "verse_id": 1,
    "verse_content": " عَبَسَ وَتَوَلَّىٰٓ",
    "verse_key": "80:1"
  },
  {
    "verse_id": 2,
    "verse_content": "أَن جَآءَهُ ٱلْأَعْمَىٰ",
    "verse_key": "80:2"
  },
  {
    "verse_id": 3,
    "verse_content": "وَمَا يُدْرِيكَ لَعَلَّهُۥ يَزَّكَّىٰٓ",
    "verse_key": "80:3"
  },
  {
    "verse_id": 4,
    "verse_content": "أَوْ يَذَّكَّرُ فَتَنفَعَهُ ٱلذِّكْرَىٰٓ",
    "verse_key": "80:4"
  },
  {
    "verse_id": 5,
    "verse_content": "أَمَّا مَنِ ٱسْتَغْنَىٰ",
    "verse_key": "80:5"
  },
  {
    "verse_id": 6,
    "verse_content": "فَأَنتَ لَهُۥ تَصَدَّىٰ",
    "verse_key": "80:6"
  },
  {
    "verse_id": 7,
    "verse_content": "وَمَا عَلَيْكَ أَلَّا يَزَّكَّىٰ",
    "verse_key": "80:7"
  },
  {
    "verse_id": 8,
    "verse_content": "وَأَمَّا مَن جَآءَكَ يَسْعَىٰ",
    "verse_key": "80:8"
  },
  {
    "verse_id": 9,
    "verse_content": "وَهُوَ يَخْشَىٰ",
    "verse_key": "80:9"
  },
  {
    "verse_id": 10,
    "verse_content": "فَأَنتَ عَنْهُ تَلَهَّىٰ",
    "verse_key": "80:10"
  },
  {
    "verse_id": 11,
    "verse_content": "كَلَّآ إِنَّهَا تَذْكِرَةٌ",
    "verse_key": "80:11"
  },
  {
    "verse_id": 12,
    "verse_content": "فَمَن شَآءَ ذَكَرَهُۥ",
    "verse_key": "80:12"
  },
  {
    "verse_id": 13,
    "verse_content": "فِى صُحُفٍ مُّكَرَّمَةٍ",
    "verse_key": "80:13"
  },
  {
    "verse_id": 14,
    "verse_content": "مَّرْفُوعَةٍ مُّطَهَّرَةٍۭ",
    "verse_key": "80:14"
  },
  {
    "verse_id": 15,
    "verse_content": "بِأَيْدِى سَفَرَةٍ",
    "verse_key": "80:15"
  },
  {
    "verse_id": 16,
    "verse_content": "كِرَامٍۭ بَرَرَةٍ",
    "verse_key": "80:16"
  },
  {
    "verse_id": 17,
    "verse_content": "قُتِلَ ٱلْإِنسَـٰنُ مَآ أَكْفَرَهُۥ",
    "verse_key": "80:17"
  },
  {
    "verse_id": 18,
    "verse_content": "مِنْ أَىِّ شَىْءٍ خَلَقَهُۥ",
    "verse_key": "80:18"
  },
  {
    "verse_id": 19,
    "verse_content": "مِن نُّطْفَةٍ خَلَقَهُۥ فَقَدَّرَهُۥ",
    "verse_key": "80:19"
  },
  {
    "verse_id": 20,
    "verse_content": "ثُمَّ ٱلسَّبِيلَ يَسَّرَهُۥ",
    "verse_key": "80:20"
  },
  {
    "verse_id": 21,
    "verse_content": "ثُمَّ أَمَاتَهُۥ فَأَقْبَرَهُۥ",
    "verse_key": "80:21"
  },
  {
    "verse_id": 22,
    "verse_content": "ثُمَّ إِذَا شَآءَ أَنشَرَهُۥ",
    "verse_key": "80:22"
  },
  {
    "verse_id": 23,
    "verse_content": "كَلَّا لَمَّا يَقْضِ مَآ أَمَرَهُۥ",
    "verse_key": "80:23"
  },
  {
    "verse_id": 24,
    "verse_content": "فَلْيَنظُرِ ٱلْإِنسَـٰنُ إِلَىٰ طَعَامِهِۦٓ",
    "verse_key": "80:24"
  },
  {
    "verse_id": 25,
    "verse_content": "أَنَّا صَبَبْنَا ٱلْمَآءَ صَبًّا",
    "verse_key": "80:25"
  },
  {
    "verse_id": 26,
    "verse_content": "ثُمَّ شَقَقْنَا ٱلْأَرْضَ شَقًّا",
    "verse_key": "80:26"
  },
  {
    "verse_id": 27,
    "verse_content": "فَأَنۢبَتْنَا فِيهَا حَبًّا",
    "verse_key": "80:27"
  },
  {
    "verse_id": 28,
    "verse_content": "وَعِنَبًا وَقَضْبًا",
    "verse_key": "80:28"
  },
  {
    "verse_id": 29,
    "verse_content": "وَزَيْتُونًا وَنَخْلًا",
    "verse_key": "80:29"
  },
  {
    "verse_id": 30,
    "verse_content": "وَحَدَآئِقَ غُلْبًا",
    "verse_key": "80:30"
  },
  {
    "verse_id": 31,
    "verse_content": "وَفَـٰكِهَةً وَأَبًّا",
    "verse_key": "80:31"
  },
  {
    "verse_id": 32,
    "verse_content": "مَّتَـٰعًا لَّكُمْ وَلِأَنْعَـٰمِكُمْ",
    "verse_key": "80:32"
  },
  {
    "verse_id": 33,
    "verse_content": "فَإِذَا جَآءَتِ ٱلصَّآخَّةُ",
    "verse_key": "80:33"
  },
  {
    "verse_id": 34,
    "verse_content": "يَوْمَ يَفِرُّ ٱلْمَرْءُ مِنْ أَخِيهِ",
    "verse_key": "80:34"
  },
  {
    "verse_id": 35,
    "verse_content": "وَأُمِّهِۦ وَأَبِيهِ",
    "verse_key": "80:35"
  },
  {
    "verse_id": 36,
    "verse_content": "وَصَـٰحِبَتِهِۦ وَبَنِيهِ",
    "verse_key": "80:36"
  },
  {
    "verse_id": 37,
    "verse_content": "لِكُلِّ ٱمْرِئٍ مِّنْهُمْ يَوْمَئِذٍ شَأْنٌ يُغْنِيهِ",
    "verse_key": "80:37"
  },
  {
    "verse_id": 38,
    "verse_content": "وُجُوهٌ يَوْمَئِذٍ مُّسْفِرَةٌ",
    "verse_key": "80:38"
  },
  {
    "verse_id": 39,
    "verse_content": "ضَاحِكَةٌ مُّسْتَبْشِرَةٌ",
    "verse_key": "80:39"
  },
  {
    "verse_id": 40,
    "verse_content": "وَوُجُوهٌ يَوْمَئِذٍ عَلَيْهَا غَبَرَةٌ",
    "verse_key": "80:40"
  },
  {
    "verse_id": 41,
    "verse_content": "تَرْهَقُهَا قَتَرَةٌ",
    "verse_key": "80:41"
  },
  {
    "verse_id": 42,
    "verse_content": "أُو۟لَـٰٓئِكَ هُمُ ٱلْكَفَرَةُ ٱلْفَجَرَةُ",
    "verse_key": "80:42"
  }
]