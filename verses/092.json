[
  {
    "verse_id": 1,
    "verse_content": " وَٱلَّيْلِ إِذَا يَغْشَىٰ",
    "verse_key": "92:1"
  },
  {
    "verse_id": 2,
    "verse_content": "وَٱلنَّهَارِ إِذَا تَجَلَّىٰ",
    "verse_key": "92:2"
  },
  {
    "verse_id": 3,
    "verse_content": "وَمَا خَلَقَ ٱلذَّكَرَ وَٱلْأُنثَىٰٓ",
    "verse_key": "92:3"
  },
  {
    "verse_id": 4,
    "verse_content": "إِنَّ سَعْيَكُمْ لَشَتَّىٰ",
    "verse_key": "92:4"
  },
  {
    "verse_id": 5,
    "verse_content": "فَأَمَّا مَنْ أَعْطَىٰ وَٱتَّقَىٰ",
    "verse_key": "92:5"
  },
  {
    "verse_id": 6,
    "verse_content": "وَصَدَّقَ بِٱلْحُسْنَىٰ",
    "verse_key": "92:6"
  },
  {
    "verse_id": 7,
    "verse_content": "فَسَنُيَسِّرُهُۥ لِلْيُسْرَىٰ",
    "verse_key": "92:7"
  },
  {
    "verse_id": 8,
    "verse_content": "وَأَمَّا مَنۢ بَخِلَ وَٱسْتَغْنَىٰ",
    "verse_key": "92:8"
  },
  {
    "verse_id": 9,
    "verse_content": "وَكَذَّبَ بِٱلْحُسْنَىٰ",
    "verse_key": "92:9"
  },
  {
    "verse_id": 10,
    "verse_content": "فَسَنُيَسِّرُهُۥ لِلْعُسْرَىٰ",
    "verse_key": "92:10"
  },
  {
    "verse_id": 11,
    "verse_content": "وَمَا يُغْنِى عَنْهُ مَالُهُۥٓ إِذَا تَرَدَّىٰٓ",
    "verse_key": "92:11"
  },
  {
    "verse_id": 12,
    "verse_content": "إِنَّ عَلَيْنَا لَلْهُدَىٰ",
    "verse_key": "92:12"
  },
  {
    "verse_id": 13,
    "verse_content": "وَإِنَّ لَنَا لَلْـَٔاخِرَةَ وَٱلْأُولَىٰ",
    "verse_key": "92:13"
  },
  {
    "verse_id": 14,
    "verse_content": "فَأَنذَرْتُكُمْ نَارًا تَلَظَّىٰ",
    "verse_key": "92:14"
  },
  {
    "verse_id": 15,
    "verse_content": "لَا يَصْلَىٰهَآ إِلَّا ٱلْأَشْقَى",
    "verse_key": "92:15"
  },
  {
    "verse_id": 16,
    "verse_content": "ٱلَّذِى كَذَّبَ وَتَوَلَّىٰ",
    "verse_key": "92:16"
  },
  {
    "verse_id": 17,
    "verse_content": "وَسَيُجَنَّبُهَا ٱلْأَتْقَى",
    "verse_key": "92:17"
  },
  {
    "verse_id": 18,
    "verse_content": "ٱلَّذِى يُؤْتِى مَالَهُۥ يَتَزَكَّىٰ",
    "verse_key": "92:18"
  },
  {
    "verse_id": 19,
    "verse_content": "وَمَا لِأَحَدٍ عِندَهُۥ مِن نِّعْمَةٍ تُجْزَىٰٓ",
    "verse_key": "92:19"
  },
  {
    "verse_id": 20,
    "verse_content": "إِلَّا ٱبْتِغَآءَ وَجْهِ رَبِّهِ ٱلْأَعْلَىٰ",
    "verse_key": "92:20"
  },
  {
    "verse_id": 21,
    "verse_content": "وَلَسَوْفَ يَرْضَىٰ",
    "verse_key": "92:21"
  }
]