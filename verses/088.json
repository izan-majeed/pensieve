[
  {
    "verse_id": 1,
    "verse_content": " هَلْ أَتَىٰكَ حَدِيثُ ٱلْغَـٰشِيَةِ",
    "verse_key": "88:1"
  },
  {
    "verse_id": 2,
    "verse_content": "وُجُوهٌ يَوْمَئِذٍ خَـٰشِعَةٌ",
    "verse_key": "88:2"
  },
  {
    "verse_id": 3,
    "verse_content": "عَامِلَةٌ نَّاصِبَةٌ",
    "verse_key": "88:3"
  },
  {
    "verse_id": 4,
    "verse_content": "تَصْلَىٰ نَارًا حَامِيَةً",
    "verse_key": "88:4"
  },
  {
    "verse_id": 5,
    "verse_content": "تُسْقَىٰ مِنْ عَيْنٍ ءَانِيَةٍ",
    "verse_key": "88:5"
  },
  {
    "verse_id": 6,
    "verse_content": "لَّيْسَ لَهُمْ طَعَامٌ إِلَّا مِن ضَرِيعٍ",
    "verse_key": "88:6"
  },
  {
    "verse_id": 7,
    "verse_content": "لَّا يُسْمِنُ وَلَا يُغْنِى مِن جُوعٍ",
    "verse_key": "88:7"
  },
  {
    "verse_id": 8,
    "verse_content": "وُجُوهٌ يَوْمَئِذٍ نَّاعِمَةٌ",
    "verse_key": "88:8"
  },
  {
    "verse_id": 9,
    "verse_content": "لِّسَعْيِهَا رَاضِيَةٌ",
    "verse_key": "88:9"
  },
  {
    "verse_id": 10,
    "verse_content": "فِى جَنَّةٍ عَالِيَةٍ",
    "verse_key": "88:10"
  },
  {
    "verse_id": 11,
    "verse_content": "لَّا تَسْمَعُ فِيهَا لَـٰغِيَةً",
    "verse_key": "88:11"
  },
  {
    "verse_id": 12,
    "verse_content": "فِيهَا عَيْنٌ جَارِيَةٌ",
    "verse_key": "88:12"
  },
  {
    "verse_id": 13,
    "verse_content": "فِيهَا سُرُرٌ مَّرْفُوعَةٌ",
    "verse_key": "88:13"
  },
  {
    "verse_id": 14,
    "verse_content": "وَأَكْوَابٌ مَّوْضُوعَةٌ",
    "verse_key": "88:14"
  },
  {
    "verse_id": 15,
    "verse_content": "وَنَمَارِقُ مَصْفُوفَةٌ",
    "verse_key": "88:15"
  },
  {
    "verse_id": 16,
    "verse_content": "وَزَرَابِىُّ مَبْثُوثَةٌ",
    "verse_key": "88:16"
  },
  {
    "verse_id": 17,
    "verse_content": "أَفَلَا يَنظُرُونَ إِلَى ٱلْإِبِلِ كَيْفَ خُلِقَتْ",
    "verse_key": "88:17"
  },
  {
    "verse_id": 18,
    "verse_content": "وَإِلَى ٱلسَّمَآءِ كَيْفَ رُفِعَتْ",
    "verse_key": "88:18"
  },
  {
    "verse_id": 19,
    "verse_content": "وَإِلَى ٱلْجِبَالِ كَيْفَ نُصِبَتْ",
    "verse_key": "88:19"
  },
  {
    "verse_id": 20,
    "verse_content": "وَإِلَى ٱلْأَرْضِ كَيْفَ سُطِحَتْ",
    "verse_key": "88:20"
  },
  {
    "verse_id": 21,
    "verse_content": "فَذَكِّرْ إِنَّمَآ أَنتَ مُذَكِّرٌ",
    "verse_key": "88:21"
  },
  {
    "verse_id": 22,
    "verse_content": "لَّسْتَ عَلَيْهِم بِمُصَيْطِرٍ",
    "verse_key": "88:22"
  },
  {
    "verse_id": 23,
    "verse_content": "إِلَّا مَن تَوَلَّىٰ وَكَفَرَ",
    "verse_key": "88:23"
  },
  {
    "verse_id": 24,
    "verse_content": "فَيُعَذِّبُهُ ٱللَّهُ ٱلْعَذَابَ ٱلْأَكْبَرَ",
    "verse_key": "88:24"
  },
  {
    "verse_id": 25,
    "verse_content": "إِنَّ إِلَيْنَآ إِيَابَهُمْ",
    "verse_key": "88:25"
  },
  {
    "verse_id": 26,
    "verse_content": "ثُمَّ إِنَّ عَلَيْنَا حِسَابَهُم",
    "verse_key": "88:26"
  }
]