[
  {
    "verse_id": 1,
    "verse_content": " وَٱلنَّـٰزِعَـٰتِ غَرْقًا",
    "verse_key": "79:1"
  },
  {
    "verse_id": 2,
    "verse_content": "وَٱلنَّـٰشِطَـٰتِ نَشْطًا",
    "verse_key": "79:2"
  },
  {
    "verse_id": 3,
    "verse_content": "وَٱلسَّـٰبِحَـٰتِ سَبْحًا",
    "verse_key": "79:3"
  },
  {
    "verse_id": 4,
    "verse_content": "فَٱلسَّـٰبِقَـٰتِ سَبْقًا",
    "verse_key": "79:4"
  },
  {
    "verse_id": 5,
    "verse_content": "فَٱلْمُدَبِّرَٰتِ أَمْرًا",
    "verse_key": "79:5"
  },
  {
    "verse_id": 6,
    "verse_content": "يَوْمَ تَرْجُفُ ٱلرَّاجِفَةُ",
    "verse_key": "79:6"
  },
  {
    "verse_id": 7,
    "verse_content": "تَتْبَعُهَا ٱلرَّادِفَةُ",
    "verse_key": "79:7"
  },
  {
    "verse_id": 8,
    "verse_content": "قُلُوبٌ يَوْمَئِذٍ وَاجِفَةٌ",
    "verse_key": "79:8"
  },
  {
    "verse_id": 9,
    "verse_content": "أَبْصَـٰرُهَا خَـٰشِعَةٌ",
    "verse_key": "79:9"
  },
  {
    "verse_id": 10,
    "verse_content": "يَقُولُونَ أَءِنَّا لَمَرْدُودُونَ فِى ٱلْحَافِرَةِ",
    "verse_key": "79:10"
  },
  {
    "verse_id": 11,
    "verse_content": "أَءِذَا كُنَّا عِظَـٰمًا نَّخِرَةً",
    "verse_key": "79:11"
  },
  {
    "verse_id": 12,
    "verse_content": "قَالُوا۟ تِلْكَ إِذًا كَرَّةٌ خَاسِرَةٌ",
    "verse_key": "79:12"
  },
  {
    "verse_id": 13,
    "verse_content": "فَإِنَّمَا هِىَ زَجْرَةٌ وَٰحِدَةٌ",
    "verse_key": "79:13"
  },
  {
    "verse_id": 14,
    "verse_content": "فَإِذَا هُم بِٱلسَّاهِرَةِ",
    "verse_key": "79:14"
  },
  {
    "verse_id": 15,
    "verse_content": "هَلْ أَتَىٰكَ حَدِيثُ مُوسَىٰٓ",
    "verse_key": "79:15"
  },
  {
    "verse_id": 16,
    "verse_content": "إِذْ نَادَىٰهُ رَبُّهُۥ بِٱلْوَادِ ٱلْمُقَدَّسِ طُوًى",
    "verse_key": "79:16"
  },
  {
    "verse_id": 17,
    "verse_content": "ٱذْهَبْ إِلَىٰ فِرْعَوْنَ إِنَّهُۥ طَغَىٰ",
    "verse_key": "79:17"
  },
  {
    "verse_id": 18,
    "verse_content": "فَقُلْ هَل لَّكَ إِلَىٰٓ أَن تَزَكَّىٰ",
    "verse_key": "79:18"
  },
  {
    "verse_id": 19,
    "verse_content": "وَأَهْدِيَكَ إِلَىٰ رَبِّكَ فَتَخْشَىٰ",
    "verse_key": "79:19"
  },
  {
    "verse_id": 20,
    "verse_content": "فَأَرَىٰهُ ٱلْـَٔايَةَ ٱلْكُبْرَىٰ",
    "verse_key": "79:20"
  },
  {
    "verse_id": 21,
    "verse_content": "فَكَذَّبَ وَعَصَىٰ",
    "verse_key": "79:21"
  },
  {
    "verse_id": 22,
    "verse_content": "ثُمَّ أَدْبَرَ يَسْعَىٰ",
    "verse_key": "79:22"
  },
  {
    "verse_id": 23,
    "verse_content": "فَحَشَرَ فَنَادَىٰ",
    "verse_key": "79:23"
  },
  {
    "verse_id": 24,
    "verse_content": "فَقَالَ أَنَا۠ رَبُّكُمُ ٱلْأَعْلَىٰ",
    "verse_key": "79:24"
  },
  {
    "verse_id": 25,
    "verse_content": "فَأَخَذَهُ ٱللَّهُ نَكَالَ ٱلْـَٔاخِرَةِ وَٱلْأُولَىٰٓ",
    "verse_key": "79:25"
  },
  {
    "verse_id": 26,
    "verse_content": "إِنَّ فِى ذَٰلِكَ لَعِبْرَةً لِّمَن يَخْشَىٰٓ",
    "verse_key": "79:26"
  },
  {
    "verse_id": 27,
    "verse_content": "ءَأَنتُمْ أَشَدُّ خَلْقًا أَمِ ٱلسَّمَآءُ ۚ بَنَىٰهَا",
    "verse_key": "79:27"
  },
  {
    "verse_id": 28,
    "verse_content": "رَفَعَ سَمْكَهَا فَسَوَّىٰهَا",
    "verse_key": "79:28"
  },
  {
    "verse_id": 29,
    "verse_content": "وَأَغْطَشَ لَيْلَهَا وَأَخْرَجَ ضُحَىٰهَا",
    "verse_key": "79:29"
  },
  {
    "verse_id": 30,
    "verse_content": "وَٱلْأَرْضَ بَعْدَ ذَٰلِكَ دَحَىٰهَآ",
    "verse_key": "79:30"
  },
  {
    "verse_id": 31,
    "verse_content": "أَخْرَجَ مِنْهَا مَآءَهَا وَمَرْعَىٰهَا",
    "verse_key": "79:31"
  },
  {
    "verse_id": 32,
    "verse_content": "وَٱلْجِبَالَ أَرْسَىٰهَا",
    "verse_key": "79:32"
  },
  {
    "verse_id": 33,
    "verse_content": "مَتَـٰعًا لَّكُمْ وَلِأَنْعَـٰمِكُمْ",
    "verse_key": "79:33"
  },
  {
    "verse_id": 34,
    "verse_content": "فَإِذَا جَآءَتِ ٱلطَّآمَّةُ ٱلْكُبْرَىٰ",
    "verse_key": "79:34"
  },
  {
    "verse_id": 35,
    "verse_content": "يَوْمَ يَتَذَكَّرُ ٱلْإِنسَـٰنُ مَا سَعَىٰ",
    "verse_key": "79:35"
  },
  {
    "verse_id": 36,
    "verse_content": "وَبُرِّزَتِ ٱلْجَحِيمُ لِمَن يَرَىٰ",
    "verse_key": "79:36"
  },
  {
    "verse_id": 37,
    "verse_content": "فَأَمَّا مَن طَغَىٰ",
    "verse_key": "79:37"
  },
  {
    "verse_id": 38,
    "verse_content": "وَءَاثَرَ ٱلْحَيَوٰةَ ٱلدُّنْيَا",
    "verse_key": "79:38"
  },
  {
    "verse_id": 39,
    "verse_content": "فَإِنَّ ٱلْجَحِيمَ هِىَ ٱلْمَأْوَىٰ",
    "verse_key": "79:39"
  },
  {
    "verse_id": 40,
    "verse_content": "وَأَمَّا مَنْ خَافَ مَقَامَ رَبِّهِۦ وَنَهَى ٱلنَّفْسَ عَنِ ٱلْهَوَىٰ",
    "verse_key": "79:40"
  },
  {
    "verse_id": 41,
    "verse_content": "فَإِنَّ ٱلْجَنَّةَ هِىَ ٱلْمَأْوَىٰ",
    "verse_key": "79:41"
  },
  {
    "verse_id": 42,
    "verse_content": "يَسْـَٔلُونَكَ عَنِ ٱلسَّاعَةِ أَيَّانَ مُرْسَىٰهَا",
    "verse_key": "79:42"
  },
  {
    "verse_id": 43,
    "verse_content": "فِيمَ أَنتَ مِن ذِكْرَىٰهَآ",
    "verse_key": "79:43"
  },
  {
    "verse_id": 44,
    "verse_content": "إِلَىٰ رَبِّكَ مُنتَهَىٰهَآ",
    "verse_key": "79:44"
  },
  {
    "verse_id": 45,
    "verse_content": "إِنَّمَآ أَنتَ مُنذِرُ مَن يَخْشَىٰهَا",
    "verse_key": "79:45"
  },
  {
    "verse_id": 46,
    "verse_content": "كَأَنَّهُمْ يَوْمَ يَرَوْنَهَا لَمْ يَلْبَثُوٓا۟ إِلَّا عَشِيَّةً أَوْ ضُحَىٰهَا",
    "verse_key": "79:46"
  }
]