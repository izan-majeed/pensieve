[
  {
    "verse_id": 1,
    "verse_content": " وَٱلشَّمْسِ وَضُحَىٰهَا",
    "verse_key": "91:1"
  },
  {
    "verse_id": 2,
    "verse_content": "وَٱلْقَمَرِ إِذَا تَلَىٰهَا",
    "verse_key": "91:2"
  },
  {
    "verse_id": 3,
    "verse_content": "وَٱلنَّهَارِ إِذَا جَلَّىٰهَا",
    "verse_key": "91:3"
  },
  {
    "verse_id": 4,
    "verse_content": "وَٱلَّيْلِ إِذَا يَغْشَىٰهَا",
    "verse_key": "91:4"
  },
  {
    "verse_id": 5,
    "verse_content": "وَٱلسَّمَآءِ وَمَا بَنَىٰهَا",
    "verse_key": "91:5"
  },
  {
    "verse_id": 6,
    "verse_content": "وَٱلْأَرْضِ وَمَا طَحَىٰهَا",
    "verse_key": "91:6"
  },
  {
    "verse_id": 7,
    "verse_content": "وَنَفْسٍ وَمَا سَوَّىٰهَا",
    "verse_key": "91:7"
  },
  {
    "verse_id": 8,
    "verse_content": "فَأَلْهَمَهَا فُجُورَهَا وَتَقْوَىٰهَا",
    "verse_key": "91:8"
  },
  {
    "verse_id": 9,
    "verse_content": "قَدْ أَفْلَحَ مَن زَكَّىٰهَا",
    "verse_key": "91:9"
  },
  {
    "verse_id": 10,
    "verse_content": "وَقَدْ خَابَ مَن دَسَّىٰهَا",
    "verse_key": "91:10"
  },
  {
    "verse_id": 11,
    "verse_content": "كَذَّبَتْ ثَمُودُ بِطَغْوَىٰهَآ",
    "verse_key": "91:11"
  },
  {
    "verse_id": 12,
    "verse_content": "إِذِ ٱنۢبَعَثَ أَشْقَىٰهَا",
    "verse_key": "91:12"
  },
  {
    "verse_id": 13,
    "verse_content": "فَقَالَ لَهُمْ رَسُولُ ٱللَّهِ نَاقَةَ ٱللَّهِ وَسُقْيَـٰهَا",
    "verse_key": "91:13"
  },
  {
    "verse_id": 14,
    "verse_content": "فَكَذَّبُوهُ فَعَقَرُوهَا فَدَمْدَمَ عَلَيْهِمْ رَبُّهُم بِذَنۢبِهِمْ فَسَوَّىٰهَا",
    "verse_key": "91:14"
  },
  {
    "verse_id": 15,
    "verse_content": "وَلَا يَخَافُ عُقْبَـٰهَا",
    "verse_key": "91:15"
  }
]