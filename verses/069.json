[
  {
    "verse_id": 1,
    "verse_content": " ٱلْحَآقَّةُ",
    "verse_key": "69:1"
  },
  {
    "verse_id": 2,
    "verse_content": "مَا ٱلْحَآقَّةُ",
    "verse_key": "69:2"
  },
  {
    "verse_id": 3,
    "verse_content": "وَمَآ أَدْرَىٰكَ مَا ٱلْحَآقَّةُ",
    "verse_key": "69:3"
  },
  {
    "verse_id": 4,
    "verse_content": "كَذَّبَتْ ثَمُودُ وَعَادٌۢ بِٱلْقَارِعَةِ",
    "verse_key": "69:4"
  },
  {
    "verse_id": 5,
    "verse_content": "فَأَمَّا ثَمُودُ فَأُهْلِكُوا۟ بِٱلطَّاغِيَةِ",
    "verse_key": "69:5"
  },
  {
    "verse_id": 6,
    "verse_content": "وَأَمَّا عَادٌ فَأُهْلِكُوا۟ بِرِيحٍ صَرْصَرٍ عَاتِيَةٍ",
    "verse_key": "69:6"
  },
  {
    "verse_id": 7,
    "verse_content": "سَخَّرَهَا عَلَيْهِمْ سَبْعَ لَيَالٍ وَثَمَـٰنِيَةَ أَيَّامٍ حُسُومًا فَتَرَى ٱلْقَوْمَ فِيهَا صَرْعَىٰ كَأَنَّهُمْ أَعْجَازُ نَخْلٍ خَاوِيَةٍ",
    "verse_key": "69:7"
  },
  {
    "verse_id": 8,
    "verse_content": "فَهَلْ تَرَىٰ لَهُم مِّنۢ بَاقِيَةٍ",
    "verse_key": "69:8"
  },
  {
    "verse_id": 9,
    "verse_content": "وَجَآءَ فِرْعَوْنُ وَمَن قَبْلَهُۥ وَٱلْمُؤْتَفِكَـٰتُ بِٱلْخَاطِئَةِ",
    "verse_key": "69:9"
  },
  {
    "verse_id": 10,
    "verse_content": "فَعَصَوْا۟ رَسُولَ رَبِّهِمْ فَأَخَذَهُمْ أَخْذَةً رَّابِيَةً",
    "verse_key": "69:10"
  },
  {
    "verse_id": 11,
    "verse_content": "إِنَّا لَمَّا طَغَا ٱلْمَآءُ حَمَلْنَـٰكُمْ فِى ٱلْجَارِيَةِ",
    "verse_key": "69:11"
  },
  {
    "verse_id": 12,
    "verse_content": "لِنَجْعَلَهَا لَكُمْ تَذْكِرَةً وَتَعِيَهَآ أُذُنٌ وَٰعِيَةٌ",
    "verse_key": "69:12"
  },
  {
    "verse_id": 13,
    "verse_content": "فَإِذَا نُفِخَ فِى ٱلصُّورِ نَفْخَةٌ وَٰحِدَةٌ",
    "verse_key": "69:13"
  },
  {
    "verse_id": 14,
    "verse_content": "وَحُمِلَتِ ٱلْأَرْضُ وَٱلْجِبَالُ فَدُكَّتَا دَكَّةً وَٰحِدَةً",
    "verse_key": "69:14"
  },
  {
    "verse_id": 15,
    "verse_content": "فَيَوْمَئِذٍ وَقَعَتِ ٱلْوَاقِعَةُ",
    "verse_key": "69:15"
  },
  {
    "verse_id": 16,
    "verse_content": "وَٱنشَقَّتِ ٱلسَّمَآءُ فَهِىَ يَوْمَئِذٍ وَاهِيَةٌ",
    "verse_key": "69:16"
  },
  {
    "verse_id": 17,
    "verse_content": "وَٱلْمَلَكُ عَلَىٰٓ أَرْجَآئِهَا ۚ وَيَحْمِلُ عَرْشَ رَبِّكَ فَوْقَهُمْ يَوْمَئِذٍ ثَمَـٰنِيَةٌ",
    "verse_key": "69:17"
  },
  {
    "verse_id": 18,
    "verse_content": "يَوْمَئِذٍ تُعْرَضُونَ لَا تَخْفَىٰ مِنكُمْ خَافِيَةٌ",
    "verse_key": "69:18"
  },
  {
    "verse_id": 19,
    "verse_content": "فَأَمَّا مَنْ أُوتِىَ كِتَـٰبَهُۥ بِيَمِينِهِۦ فَيَقُولُ هَآؤُمُ ٱقْرَءُوا۟ كِتَـٰبِيَهْ",
    "verse_key": "69:19"
  },
  {
    "verse_id": 20,
    "verse_content": "إِنِّى ظَنَنتُ أَنِّى مُلَـٰقٍ حِسَابِيَهْ",
    "verse_key": "69:20"
  },
  {
    "verse_id": 21,
    "verse_content": "فَهُوَ فِى عِيشَةٍ رَّاضِيَةٍ",
    "verse_key": "69:21"
  },
  {
    "verse_id": 22,
    "verse_content": "فِى جَنَّةٍ عَالِيَةٍ",
    "verse_key": "69:22"
  },
  {
    "verse_id": 23,
    "verse_content": "قُطُوفُهَا دَانِيَةٌ",
    "verse_key": "69:23"
  },
  {
    "verse_id": 24,
    "verse_content": "كُلُوا۟ وَٱشْرَبُوا۟ هَنِيٓـًٔۢا بِمَآ أَسْلَفْتُمْ فِى ٱلْأَيَّامِ ٱلْخَالِيَةِ",
    "verse_key": "69:24"
  },
  {
    "verse_id": 25,
    "verse_content": "وَأَمَّا مَنْ أُوتِىَ كِتَـٰبَهُۥ بِشِمَالِهِۦ فَيَقُولُ يَـٰلَيْتَنِى لَمْ أُوتَ كِتَـٰبِيَهْ",
    "verse_key": "69:25"
  },
  {
    "verse_id": 26,
    "verse_content": "وَلَمْ أَدْرِ مَا حِسَابِيَهْ",
    "verse_key": "69:26"
  },
  {
    "verse_id": 27,
    "verse_content": "يَـٰلَيْتَهَا كَانَتِ ٱلْقَاضِيَةَ",
    "verse_key": "69:27"
  },
  {
    "verse_id": 28,
    "verse_content": "مَآ أَغْنَىٰ عَنِّى مَالِيَهْ ۜ",
    "verse_key": "69:28"
  },
  {
    "verse_id": 29,
    "verse_content": "هَلَكَ عَنِّى سُلْطَـٰنِيَهْ",
    "verse_key": "69:29"
  },
  {
    "verse_id": 30,
    "verse_content": "خُذُوهُ فَغُلُّوهُ",
    "verse_key": "69:30"
  },
  {
    "verse_id": 31,
    "verse_content": "ثُمَّ ٱلْجَحِيمَ صَلُّوهُ",
    "verse_key": "69:31"
  },
  {
    "verse_id": 32,
    "verse_content": "ثُمَّ فِى سِلْسِلَةٍ ذَرْعُهَا سَبْعُونَ ذِرَاعًا فَٱسْلُكُوهُ",
    "verse_key": "69:32"
  },
  {
    "verse_id": 33,
    "verse_content": "إِنَّهُۥ كَانَ لَا يُؤْمِنُ بِٱللَّهِ ٱلْعَظِيمِ",
    "verse_key": "69:33"
  },
  {
    "verse_id": 34,
    "verse_content": "وَلَا يَحُضُّ عَلَىٰ طَعَامِ ٱلْمِسْكِينِ",
    "verse_key": "69:34"
  },
  {
    "verse_id": 35,
    "verse_content": "فَلَيْسَ لَهُ ٱلْيَوْمَ هَـٰهُنَا حَمِيمٌ",
    "verse_key": "69:35"
  },
  {
    "verse_id": 36,
    "verse_content": "وَلَا طَعَامٌ إِلَّا مِنْ غِسْلِينٍ",
    "verse_key": "69:36"
  },
  {
    "verse_id": 37,
    "verse_content": "لَّا يَأْكُلُهُۥٓ إِلَّا ٱلْخَـٰطِـُٔونَ",
    "verse_key": "69:37"
  },
  {
    "verse_id": 38,
    "verse_content": "فَلَآ أُقْسِمُ بِمَا تُبْصِرُونَ",
    "verse_key": "69:38"
  },
  {
    "verse_id": 39,
    "verse_content": "وَمَا لَا تُبْصِرُونَ",
    "verse_key": "69:39"
  },
  {
    "verse_id": 40,
    "verse_content": "إِنَّهُۥ لَقَوْلُ رَسُولٍ كَرِيمٍ",
    "verse_key": "69:40"
  },
  {
    "verse_id": 41,
    "verse_content": "وَمَا هُوَ بِقَوْلِ شَاعِرٍ ۚ قَلِيلًا مَّا تُؤْمِنُونَ",
    "verse_key": "69:41"
  },
  {
    "verse_id": 42,
    "verse_content": "وَلَا بِقَوْلِ كَاهِنٍ ۚ قَلِيلًا مَّا تَذَكَّرُونَ",
    "verse_key": "69:42"
  },
  {
    "verse_id": 43,
    "verse_content": "تَنزِيلٌ مِّن رَّبِّ ٱلْعَـٰلَمِينَ",
    "verse_key": "69:43"
  },
  {
    "verse_id": 44,
    "verse_content": "وَلَوْ تَقَوَّلَ عَلَيْنَا بَعْضَ ٱلْأَقَاوِيلِ",
    "verse_key": "69:44"
  },
  {
    "verse_id": 45,
    "verse_content": "لَأَخَذْنَا مِنْهُ بِٱلْيَمِينِ",
    "verse_key": "69:45"
  },
  {
    "verse_id": 46,
    "verse_content": "ثُمَّ لَقَطَعْنَا مِنْهُ ٱلْوَتِينَ",
    "verse_key": "69:46"
  },
  {
    "verse_id": 47,
    "verse_content": "فَمَا مِنكُم مِّنْ أَحَدٍ عَنْهُ حَـٰجِزِينَ",
    "verse_key": "69:47"
  },
  {
    "verse_id": 48,
    "verse_content": "وَإِنَّهُۥ لَتَذْكِرَةٌ لِّلْمُتَّقِينَ",
    "verse_key": "69:48"
  },
  {
    "verse_id": 49,
    "verse_content": "وَإِنَّا لَنَعْلَمُ أَنَّ مِنكُم مُّكَذِّبِينَ",
    "verse_key": "69:49"
  },
  {
    "verse_id": 50,
    "verse_content": "وَإِنَّهُۥ لَحَسْرَةٌ عَلَى ٱلْكَـٰفِرِينَ",
    "verse_key": "69:50"
  },
  {
    "verse_id": 51,
    "verse_content": "وَإِنَّهُۥ لَحَقُّ ٱلْيَقِينِ",
    "verse_key": "69:51"
  },
  {
    "verse_id": 52,
    "verse_content": "فَسَبِّحْ بِٱسْمِ رَبِّكَ ٱلْعَظِيمِ",
    "verse_key": "69:52"
  }
]