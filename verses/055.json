  },
  {
    "verse_id": 73,
    "verse_content": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
    "verse_key": "55:73"
  },
  {
    "verse_id": 74,
    "verse_content": "لَمْ يَطْمِثْهُنَّ إِنسٌ قَبْلَهُمْ وَلَا جَآنٌّ",
    "verse_key": "55:74"
  },
  {
    "verse_id": 75,
    "verse_content": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
    "verse_key": "55:75"
  },
  {
    "verse_id": 76,
    "verse_content": "مُتَّكِـِٔينَ عَلَىٰ رَفْرَفٍ خُضْرٍ وَعَبْقَرِىٍّ حِسَانٍ",
