[
  {
    "verse_id": 1,
    "translation": "Sabbihi isma rabbika al-aAAla"
  },
  {
    "verse_id": 2,
    "translation": "Allathee khalaqa fasawwa"
  },
  {
    "verse_id": 3,
    "translation": "Wallathee qaddara fahada"
  },
  {
    "verse_id": 4,
    "translation": "Wallathee akhraja almarAAa"
  },
  {
    "verse_id": 5,
    "translation": "FajaAAalahu ghuthaan ahwa"
  },
  {
    "verse_id": 6,
    "translation": "Sanuqri-oka fala tansa"
  },
  {
    "verse_id": 7,
    "translation": "Illa ma shaa Allahuinnahu yaAAlamu aljahra wama yakhfa"
  },
  {
    "verse_id": 8,
    "translation": "Wanuyassiruka lilyusra"
  },
  {
    "verse_id": 9,
    "translation": "Fathakkir in nafaAAati aththikra"
  },
  {
    "verse_id": 10,
    "translation": "Sayaththakkaru man yakhsha"
  },
  {
    "verse_id": 11,
    "translation": "Wayatajannabuha al-ashqa"
  },
  {
    "verse_id": 12,
    "translation": "Allathee yasla annaraalkubra"
  },
  {
    "verse_id": 13,
    "translation": "Thumma la yamootu feeha walayahya"
  },
  {
    "verse_id": 14,
    "translation": "Qad aflaha man tazakka"
  },
  {
    "verse_id": 15,
    "translation": "Wathakara isma rabbihi fasalla"
  },
  {
    "verse_id": 16,
    "translation": "Bal tu/thiroona alhayata addunya"
  },
  {
    "verse_id": 17,
    "translation": "Wal-akhiratu khayrun waabqa"
  },
  {
    "verse_id": 18,
    "translation": "Inna hatha lafee assuhufial-oola"
  },
  {
    "verse_id": 19,
    "translation": "Suhufi ibraheema wamoosa"
  }
