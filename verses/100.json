[
  {
    "verse_id": 1,
    "verse_content": " وَٱلْعَـٰدِيَـٰتِ ضَبْحًا",
    "verse_key": "100:1"
  },
  {
    "verse_id": 2,
    "verse_content": "فَٱلْمُورِيَـٰتِ قَدْحًا",
    "verse_key": "100:2"
  },
  {
    "verse_id": 3,
    "verse_content": "فَٱلْمُغِيرَٰتِ صُبْحًا",
    "verse_key": "100:3"
  },
  {
    "verse_id": 4,
    "verse_content": "فَأَثَرْنَ بِهِۦ نَقْعًا",
    "verse_key": "100:4"
  },
  {
    "verse_id": 5,
    "verse_content": "فَوَسَطْنَ بِهِۦ جَمْعًا",
    "verse_key": "100:5"
  },
  {
    "verse_id": 6,
    "verse_content": "إِنَّ ٱلْإِنسَـٰنَ لِرَبِّهِۦ لَكَنُودٌ",
    "verse_key": "100:6"
  },
  {
    "verse_id": 7,
    "verse_content": "وَإِنَّهُۥ عَلَىٰ ذَٰلِكَ لَشَهِيدٌ",
    "verse_key": "100:7"
  },
  {
    "verse_id": 8,
    "verse_content": "وَإِنَّهُۥ لِحُبِّ ٱلْخَيْرِ لَشَدِيدٌ",
    "verse_key": "100:8"
  },
  {
    "verse_id": 9,
    "verse_content": "۞ أَفَلَا يَعْلَمُ إِذَا بُعْثِرَ مَا فِى ٱلْقُبُورِ",
    "verse_key": "100:9"
  },
  {
    "verse_id": 10,
    "verse_content": "وَحُصِّلَ مَا فِى ٱلصُّدُورِ",
    "verse_key": "100:10"
  },
  {
    "verse_id": 11,
    "verse_content": "إِنَّ رَبَّهُم بِهِمْ يَوْمَئِذٍ لَّخَبِيرٌۢ",
    "verse_key": "100:11"
  }
]