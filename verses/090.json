[
  {
    "verse_id": 1,
    "verse_content": " لَآ أُقْسِمُ بِهَـٰذَا ٱلْبَلَدِ",
    "verse_key": "90:1"
  },
  {
    "verse_id": 2,
    "verse_content": "وَأَنتَ حِلٌّۢ بِهَـٰذَا ٱلْبَلَدِ",
    "verse_key": "90:2"
  },
  {
    "verse_id": 3,
    "verse_content": "وَوَالِدٍ وَمَا وَلَدَ",
    "verse_key": "90:3"
  },
  {
    "verse_id": 4,
    "verse_content": "لَقَدْ خَلَقْنَا ٱلْإِنسَـٰنَ فِى كَبَدٍ",
    "verse_key": "90:4"
  },
  {
    "verse_id": 5,
    "verse_content": "أَيَحْسَبُ أَن لَّن يَقْدِرَ عَلَيْهِ أَحَدٌ",
    "verse_key": "90:5"
  },
  {
    "verse_id": 6,
    "verse_content": "يَقُولُ أَهْلَكْتُ مَالًا لُّبَدًا",
    "verse_key": "90:6"
  },
  {
    "verse_id": 7,
    "verse_content": "أَيَحْسَبُ أَن لَّمْ يَرَهُۥٓ أَحَدٌ",
    "verse_key": "90:7"
  },
  {
    "verse_id": 8,
    "verse_content": "أَلَمْ نَجْعَل لَّهُۥ عَيْنَيْنِ",
    "verse_key": "90:8"
  },
  {
    "verse_id": 9,
    "verse_content": "وَلِسَانًا وَشَفَتَيْنِ",
    "verse_key": "90:9"
  },
  {
    "verse_id": 10,
    "verse_content": "وَهَدَيْنَـٰهُ ٱلنَّجْدَيْنِ",
    "verse_key": "90:10"
  },
  {
    "verse_id": 11,
    "verse_content": "فَلَا ٱقْتَحَمَ ٱلْعَقَبَةَ",
    "verse_key": "90:11"
  },
  {
    "verse_id": 12,
    "verse_content": "وَمَآ أَدْرَىٰكَ مَا ٱلْعَقَبَةُ",
    "verse_key": "90:12"
  },
  {
    "verse_id": 13,
    "verse_content": "فَكُّ رَقَبَةٍ",
    "verse_key": "90:13"
  },
  {
    "verse_id": 14,
    "verse_content": "أَوْ إِطْعَـٰمٌ فِى يَوْمٍ ذِى مَسْغَبَةٍ",
    "verse_key": "90:14"
  },
  {
    "verse_id": 15,
    "verse_content": "يَتِيمًا ذَا مَقْرَبَةٍ",
    "verse_key": "90:15"
  },
  {
    "verse_id": 16,
    "verse_content": "أَوْ مِسْكِينًا ذَا مَتْرَبَةٍ",
    "verse_key": "90:16"
  },
  {
    "verse_id": 17,
    "verse_content": "ثُمَّ كَانَ مِنَ ٱلَّذِينَ ءَامَنُوا۟ وَتَوَاصَوْا۟ بِٱلصَّبْرِ وَتَوَاصَوْا۟ بِٱلْمَرْحَمَةِ",
    "verse_key": "90:17"
  },
  {
    "verse_id": 18,
    "verse_content": "أُو۟لَـٰٓئِكَ أَصْحَـٰبُ ٱلْمَيْمَنَةِ",
    "verse_key": "90:18"
  },
  {
    "verse_id": 19,
    "verse_content": "وَٱلَّذِينَ كَفَرُوا۟ بِـَٔايَـٰتِنَا هُمْ أَصْحَـٰبُ ٱلْمَشْـَٔمَةِ",
    "verse_key": "90:19"
  },
  {
    "verse_id": 20,
    "verse_content": "عَلَيْهِمْ نَارٌ مُّؤْصَدَةٌۢ",
    "verse_key": "90:20"
  }
]