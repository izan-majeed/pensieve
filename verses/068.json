[
  {
    "verse_id": 1,
    "verse_content": " نٓ ۚ وَٱلْقَلَمِ وَمَا يَسْطُرُونَ",
    "verse_key": "68:1"
  },
  {
    "verse_id": 2,
    "verse_content": "مَآ أَنتَ بِنِعْمَةِ رَبِّكَ بِمَجْنُونٍ",
    "verse_key": "68:2"
  },
  {
    "verse_id": 3,
    "verse_content": "وَإِنَّ لَكَ لَأَجْرًا غَيْرَ مَمْنُونٍ",
    "verse_key": "68:3"
  },
  {
    "verse_id": 4,
    "verse_content": "وَإِنَّكَ لَعَلَىٰ خُلُقٍ عَظِيمٍ",
    "verse_key": "68:4"
  },
  {
    "verse_id": 5,
    "verse_content": "فَسَتُبْصِرُ وَيُبْصِرُونَ",
    "verse_key": "68:5"
  },
  {
    "verse_id": 6,
    "verse_content": "بِأَييِّكُمُ ٱلْمَفْتُونُ",
    "verse_key": "68:6"
  },
  {
    "verse_id": 7,
    "verse_content": "إِنَّ رَبَّكَ هُوَ أَعْلَمُ بِمَن ضَلَّ عَن سَبِيلِهِۦ وَهُوَ أَعْلَمُ بِٱلْمُهْتَدِينَ",
    "verse_key": "68:7"
  },
  {
    "verse_id": 8,
    "verse_content": "فَلَا تُطِعِ ٱلْمُكَذِّبِينَ",
    "verse_key": "68:8"
  },
  {
    "verse_id": 9,
    "verse_content": "وَدُّوا۟ لَوْ تُدْهِنُ فَيُدْهِنُونَ",
    "verse_key": "68:9"
  },
  {
    "verse_id": 10,
    "verse_content": "وَلَا تُطِعْ كُلَّ حَلَّافٍ مَّهِينٍ",
    "verse_key": "68:10"
  },
  {
    "verse_id": 11,
    "verse_content": "هَمَّازٍ مَّشَّآءٍۭ بِنَمِيمٍ",
    "verse_key": "68:11"
  },
  {
    "verse_id": 12,
    "verse_content": "مَّنَّاعٍ لِّلْخَيْرِ مُعْتَدٍ أَثِيمٍ",
    "verse_key": "68:12"
  },
  {
    "verse_id": 13,
    "verse_content": "عُتُلٍّۭ بَعْدَ ذَٰلِكَ زَنِيمٍ",
    "verse_key": "68:13"
  },
  {
    "verse_id": 14,
    "verse_content": "أَن كَانَ ذَا مَالٍ وَبَنِينَ",
    "verse_key": "68:14"
  },
  {
    "verse_id": 15,
    "verse_content": "إِذَا تُتْلَىٰ عَلَيْهِ ءَايَـٰتُنَا قَالَ أَسَـٰطِيرُ ٱلْأَوَّلِينَ",
    "verse_key": "68:15"
  },
  {
    "verse_id": 16,
    "verse_content": "سَنَسِمُهُۥ عَلَى ٱلْخُرْطُومِ",
    "verse_key": "68:16"
  },
  {
    "verse_id": 17,
    "verse_content": "إِنَّا بَلَوْنَـٰهُمْ كَمَا بَلَوْنَآ أَصْحَـٰبَ ٱلْجَنَّةِ إِذْ أَقْسَمُوا۟ لَيَصْرِمُنَّهَا مُصْبِحِينَ",
    "verse_key": "68:17"
  },
  {
    "verse_id": 18,
    "verse_content": "وَلَا يَسْتَثْنُونَ",
    "verse_key": "68:18"
  },
  {
    "verse_id": 19,
    "verse_content": "فَطَافَ عَلَيْهَا طَآئِفٌ مِّن رَّبِّكَ وَهُمْ نَآئِمُونَ",
    "verse_key": "68:19"
  },
  {
    "verse_id": 20,
    "verse_content": "فَأَصْبَحَتْ كَٱلصَّرِيمِ",
    "verse_key": "68:20"
  },
  {
    "verse_id": 21,
    "verse_content": "فَتَنَادَوْا۟ مُصْبِحِينَ",
    "verse_key": "68:21"
  },
  {
    "verse_id": 22,
    "verse_content": "أَنِ ٱغْدُوا۟ عَلَىٰ حَرْثِكُمْ إِن كُنتُمْ صَـٰرِمِينَ",
    "verse_key": "68:22"
  },
  {
    "verse_id": 23,
    "verse_content": "فَٱنطَلَقُوا۟ وَهُمْ يَتَخَـٰفَتُونَ",
    "verse_key": "68:23"
  },
  {
    "verse_id": 24,
    "verse_content": "أَن لَّا يَدْخُلَنَّهَا ٱلْيَوْمَ عَلَيْكُم مِّسْكِينٌ",
    "verse_key": "68:24"
  },
  {
    "verse_id": 25,
    "verse_content": "وَغَدَوْا۟ عَلَىٰ حَرْدٍ قَـٰدِرِينَ",
    "verse_key": "68:25"
  },
  {
    "verse_id": 26,
    "verse_content": "فَلَمَّا رَأَوْهَا قَالُوٓا۟ إِنَّا لَضَآلُّونَ",
    "verse_key": "68:26"
  },
  {
    "verse_id": 27,
    "verse_content": "بَلْ نَحْنُ مَحْرُومُونَ",
    "verse_key": "68:27"
  },
  {
    "verse_id": 28,
    "verse_content": "قَالَ أَوْسَطُهُمْ أَلَمْ أَقُل لَّكُمْ لَوْلَا تُسَبِّحُونَ",
    "verse_key": "68:28"
  },
  {
    "verse_id": 29,
    "verse_content": "قَالُوا۟ سُبْحَـٰنَ رَبِّنَآ إِنَّا كُنَّا ظَـٰلِمِينَ",
    "verse_key": "68:29"
  },
  {
    "verse_id": 30,
    "verse_content": "فَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٍ يَتَلَـٰوَمُونَ",
    "verse_key": "68:30"
  },
  {
    "verse_id": 31,
    "verse_content": "قَالُوا۟ يَـٰوَيْلَنَآ إِنَّا كُنَّا طَـٰغِينَ",
    "verse_key": "68:31"
  },
  {
    "verse_id": 32,
    "verse_content": "عَسَىٰ رَبُّنَآ أَن يُبْدِلَنَا خَيْرًا مِّنْهَآ إِنَّآ إِلَىٰ رَبِّنَا رَٰغِبُونَ",
    "verse_key": "68:32"
  },
  {
    "verse_id": 33,
    "verse_content": "كَذَٰلِكَ ٱلْعَذَابُ ۖ وَلَعَذَابُ ٱلْـَٔاخِرَةِ أَكْبَرُ ۚ لَوْ كَانُوا۟ يَعْلَمُونَ",
    "verse_key": "68:33"
  },
  {
    "verse_id": 34,
    "verse_content": "إِنَّ لِلْمُتَّقِينَ عِندَ رَبِّهِمْ جَنَّـٰتِ ٱلنَّعِيمِ",
    "verse_key": "68:34"
  },
  {
    "verse_id": 35,
    "verse_content": "أَفَنَجْعَلُ ٱلْمُسْلِمِينَ كَٱلْمُجْرِمِينَ",
    "verse_key": "68:35"
  },
  {
    "verse_id": 36,
    "verse_content": "مَا لَكُمْ كَيْفَ تَحْكُمُونَ",
    "verse_key": "68:36"
  },
  {
    "verse_id": 37,
    "verse_content": "أَمْ لَكُمْ كِتَـٰبٌ فِيهِ تَدْرُسُونَ",
    "verse_key": "68:37"
  },
  {
    "verse_id": 38,
    "verse_content": "إِنَّ لَكُمْ فِيهِ لَمَا تَخَيَّرُونَ",
    "verse_key": "68:38"
  },
  {
    "verse_id": 39,
    "verse_content": "أَمْ لَكُمْ أَيْمَـٰنٌ عَلَيْنَا بَـٰلِغَةٌ إِلَىٰ يَوْمِ ٱلْقِيَـٰمَةِ ۙ إِنَّ لَكُمْ لَمَا تَحْكُمُونَ",
    "verse_key": "68:39"
  },
  {
    "verse_id": 40,
    "verse_content": "سَلْهُمْ أَيُّهُم بِذَٰلِكَ زَعِيمٌ",
    "verse_key": "68:40"
  },
  {
    "verse_id": 41,
    "verse_content": "أَمْ لَهُمْ شُرَكَآءُ فَلْيَأْتُوا۟ بِشُرَكَآئِهِمْ إِن كَانُوا۟ صَـٰدِقِينَ",
    "verse_key": "68:41"
  },
  {
    "verse_id": 42,
    "verse_content": "يَوْمَ يُكْشَفُ عَن سَاقٍ وَيُدْعَوْنَ إِلَى ٱلسُّجُودِ فَلَا يَسْتَطِيعُونَ",
    "verse_key": "68:42"
  },
  {
    "verse_id": 43,
    "verse_content": "خَـٰشِعَةً أَبْصَـٰرُهُمْ تَرْهَقُهُمْ ذِلَّةٌ ۖ وَقَدْ كَانُوا۟ يُدْعَوْنَ إِلَى ٱلسُّجُودِ وَهُمْ سَـٰلِمُونَ",
    "verse_key": "68:43"
  },
  {
    "verse_id": 44,
    "verse_content": "فَذَرْنِى وَمَن يُكَذِّبُ بِهَـٰذَا ٱلْحَدِيثِ ۖ سَنَسْتَدْرِجُهُم مِّنْ حَيْثُ لَا يَعْلَمُونَ",
    "verse_key": "68:44"
  },
  {
    "verse_id": 45,
    "verse_content": "وَأُمْلِى لَهُمْ ۚ إِنَّ كَيْدِى مَتِينٌ",
    "verse_key": "68:45"
  },
  {
    "verse_id": 46,
    "verse_content": "أَمْ تَسْـَٔلُهُمْ أَجْرًا فَهُم مِّن مَّغْرَمٍ مُّثْقَلُونَ",
    "verse_key": "68:46"
  },
  {
    "verse_id": 47,
    "verse_content": "أَمْ عِندَهُمُ ٱلْغَيْبُ فَهُمْ يَكْتُبُونَ",
    "verse_key": "68:47"
  },
  {
    "verse_id": 48,
    "verse_content": "فَٱصْبِرْ لِحُكْمِ رَبِّكَ وَلَا تَكُن كَصَاحِبِ ٱلْحُوتِ إِذْ نَادَىٰ وَهُوَ مَكْظُومٌ",
    "verse_key": "68:48"
  },
  {
    "verse_id": 49,
    "verse_content": "لَّوْلَآ أَن تَدَٰرَكَهُۥ نِعْمَةٌ مِّن رَّبِّهِۦ لَنُبِذَ بِٱلْعَرَآءِ وَهُوَ مَذْمُومٌ",
    "verse_key": "68:49"
  },
  {
    "verse_id": 50,
    "verse_content": "فَٱجْتَبَـٰهُ رَبُّهُۥ فَجَعَلَهُۥ مِنَ ٱلصَّـٰلِحِينَ",
    "verse_key": "68:50"
  },
  {
    "verse_id": 51,
    "verse_content": "وَإِن يَكَادُ ٱلَّذِينَ كَفَرُوا۟ لَيُزْلِقُونَكَ بِأَبْصَـٰرِهِمْ لَمَّا سَمِعُوا۟ ٱلذِّكْرَ وَيَقُولُونَ إِنَّهُۥ لَمَجْنُونٌ",
    "verse_key": "68:51"
  },
  {
    "verse_id": 52,
    "verse_content": "وَمَا هُوَ إِلَّا ذِكْرٌ لِّلْعَـٰلَمِينَ",
    "verse_key": "68:52"
  }
]