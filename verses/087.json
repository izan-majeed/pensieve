[
  {
    "verse_id": 1,
    "verse_content": " سَبِّحِ ٱسْمَ رَبِّكَ ٱلْأَعْلَى",
    "verse_key": "87:1"
  },
  {
    "verse_id": 2,
    "verse_content": "ٱلَّذِى خَلَقَ فَسَوَّىٰ",
    "verse_key": "87:2"
  },
  {
    "verse_id": 3,
    "verse_content": "وَٱلَّذِى قَدَّرَ فَهَدَىٰ",
    "verse_key": "87:3"
  },
  {
    "verse_id": 4,
    "verse_content": "وَٱلَّذِىٓ أَخْرَجَ ٱلْمَرْعَىٰ",
    "verse_key": "87:4"
  },
  {
    "verse_id": 5,
    "verse_content": "فَجَعَلَهُۥ غُثَآءً أَحْوَىٰ",
    "verse_key": "87:5"
  },
  {
    "verse_id": 6,
    "verse_content": "سَنُقْرِئُكَ فَلَا تَنسَىٰٓ",
    "verse_key": "87:6"
  },
  {
    "verse_id": 7,
    "verse_content": "إِلَّا مَا شَآءَ ٱللَّهُ ۚ إِنَّهُۥ يَعْلَمُ ٱلْجَهْرَ وَمَا يَخْفَىٰ",
    "verse_key": "87:7"
  },
  {
    "verse_id": 8,
    "verse_content": "وَنُيَسِّرُكَ لِلْيُسْرَىٰ",
    "verse_key": "87:8"
  },
  {
    "verse_id": 9,
    "verse_content": "فَذَكِّرْ إِن نَّفَعَتِ ٱلذِّكْرَىٰ",
    "verse_key": "87:9"
  },
  {
    "verse_id": 10,
    "verse_content": "سَيَذَّكَّرُ مَن يَخْشَىٰ",
    "verse_key": "87:10"
  },
  {
    "verse_id": 11,
    "verse_content": "وَيَتَجَنَّبُهَا ٱلْأَشْقَى",
    "verse_key": "87:11"
  },
  {
    "verse_id": 12,
    "verse_content": "ٱلَّذِى يَصْلَى ٱلنَّارَ ٱلْكُبْرَىٰ",
    "verse_key": "87:12"
  },
  {
    "verse_id": 13,
    "verse_content": "ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحْيَىٰ",
    "verse_key": "87:13"
  },
  {
    "verse_id": 14,
    "verse_content": "قَدْ أَفْلَحَ مَن تَزَكَّىٰ",
    "verse_key": "87:14"
  },
  {
    "verse_id": 15,
    "verse_content": "وَذَكَرَ ٱسْمَ رَبِّهِۦ فَصَلَّىٰ",
    "verse_key": "87:15"
  },
  {
    "verse_id": 16,
    "verse_content": "بَلْ تُؤْثِرُونَ ٱلْحَيَوٰةَ ٱلدُّنْيَا",
    "verse_key": "87:16"
  },
  {
    "verse_id": 17,
    "verse_content": "وَٱلْـَٔاخِرَةُ خَيْرٌ وَأَبْقَىٰٓ",
    "verse_key": "87:17"
  },
  {
    "verse_id": 18,
    "verse_content": "إِنَّ هَـٰذَا لَفِى ٱلصُّحُفِ ٱلْأُولَىٰ",
    "verse_key": "87:18"
  },
  {
    "verse_id": 19,
    "verse_content": "صُحُفِ إِبْرَٰهِيمَ وَمُوسَىٰ",
    "verse_key": "87:19"
  }
]