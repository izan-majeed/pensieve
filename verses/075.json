[
  {
    "verse_id": 1,
    "verse_content": " لَآ أُقْسِمُ بِيَوْمِ ٱلْقِيَـٰمَةِ",
    "verse_key": "75:1"
  },
  {
    "verse_id": 2,
    "verse_content": "وَلَآ أُقْسِمُ بِٱلنَّفْسِ ٱللَّوَّامَةِ",
    "verse_key": "75:2"
  },
  {
    "verse_id": 3,
    "verse_content": "أَيَحْسَبُ ٱلْإِنسَـٰنُ أَلَّن نَّجْمَعَ عِظَامَهُۥ",
    "verse_key": "75:3"
  },
  {
    "verse_id": 4,
    "verse_content": "بَلَىٰ قَـٰدِرِينَ عَلَىٰٓ أَن نُّسَوِّىَ بَنَانَهُۥ",
    "verse_key": "75:4"
  },
  {
    "verse_id": 5,
    "verse_content": "بَلْ يُرِيدُ ٱلْإِنسَـٰنُ لِيَفْجُرَ أَمَامَهُۥ",
    "verse_key": "75:5"
  },
  {
    "verse_id": 6,
    "verse_content": "يَسْـَٔلُ أَيَّانَ يَوْمُ ٱلْقِيَـٰمَةِ",
    "verse_key": "75:6"
  },
  {
    "verse_id": 7,
    "verse_content": "فَإِذَا بَرِقَ ٱلْبَصَرُ",
    "verse_key": "75:7"
  },
  {
    "verse_id": 8,
    "verse_content": "وَخَسَفَ ٱلْقَمَرُ",
    "verse_key": "75:8"
  },
  {
    "verse_id": 9,
    "verse_content": "وَجُمِعَ ٱلشَّمْسُ وَٱلْقَمَرُ",
    "verse_key": "75:9"
  },
  {
    "verse_id": 10,
    "verse_content": "يَقُولُ ٱلْإِنسَـٰنُ يَوْمَئِذٍ أَيْنَ ٱلْمَفَرُّ",
    "verse_key": "75:10"
  },
  {
    "verse_id": 11,
    "verse_content": "كَلَّا لَا وَزَرَ",
    "verse_key": "75:11"
  },
  {
    "verse_id": 12,
    "verse_content": "إِلَىٰ رَبِّكَ يَوْمَئِذٍ ٱلْمُسْتَقَرُّ",
    "verse_key": "75:12"
  },
  {
    "verse_id": 13,
    "verse_content": "يُنَبَّؤُا۟ ٱلْإِنسَـٰنُ يَوْمَئِذٍۭ بِمَا قَدَّمَ وَأَخَّرَ",
    "verse_key": "75:13"
  },
  {
    "verse_id": 14,
    "verse_content": "بَلِ ٱلْإِنسَـٰنُ عَلَىٰ نَفْسِهِۦ بَصِيرَةٌ",
    "verse_key": "75:14"
  },
  {
    "verse_id": 15,
    "verse_content": "وَلَوْ أَلْقَىٰ مَعَاذِيرَهُۥ",
    "verse_key": "75:15"
  },
  {
    "verse_id": 16,
    "verse_content": "لَا تُحَرِّكْ بِهِۦ لِسَانَكَ لِتَعْجَلَ بِهِۦٓ",
    "verse_key": "75:16"
  },
  {
    "verse_id": 17,
    "verse_content": "إِنَّ عَلَيْنَا جَمْعَهُۥ وَقُرْءَانَهُۥ",
    "verse_key": "75:17"
  },
  {
    "verse_id": 18,
    "verse_content": "فَإِذَا قَرَأْنَـٰهُ فَٱتَّبِعْ قُرْءَانَهُۥ",
    "verse_key": "75:18"
  },
  {
    "verse_id": 19,
    "verse_content": "ثُمَّ إِنَّ عَلَيْنَا بَيَانَهُۥ",
    "verse_key": "75:19"
  },
  {
    "verse_id": 20,
    "verse_content": "كَلَّا بَلْ تُحِبُّونَ ٱلْعَاجِلَةَ",
    "verse_key": "75:20"
  },
  {
    "verse_id": 21,
    "verse_content": "وَتَذَرُونَ ٱلْـَٔاخِرَةَ",
    "verse_key": "75:21"
  },
  {
    "verse_id": 22,
    "verse_content": "وُجُوهٌ يَوْمَئِذٍ نَّاضِرَةٌ",
    "verse_key": "75:22"
  },
  {
    "verse_id": 23,
    "verse_content": "إِلَىٰ رَبِّهَا نَاظِرَةٌ",
    "verse_key": "75:23"
  },
  {
    "verse_id": 24,
    "verse_content": "وَوُجُوهٌ يَوْمَئِذٍۭ بَاسِرَةٌ",
    "verse_key": "75:24"
  },
  {
    "verse_id": 25,
    "verse_content": "تَظُنُّ أَن يُفْعَلَ بِهَا فَاقِرَةٌ",
    "verse_key": "75:25"
  },
  {
    "verse_id": 26,
    "verse_content": "كَلَّآ إِذَا بَلَغَتِ ٱلتَّرَاقِىَ",
    "verse_key": "75:26"
  },
  {
    "verse_id": 27,
    "verse_content": "وَقِيلَ مَنْ ۜ رَاقٍ",
    "verse_key": "75:27"
  },
  {
    "verse_id": 28,
    "verse_content": "وَظَنَّ أَنَّهُ ٱلْفِرَاقُ",
    "verse_key": "75:28"
  },
  {
    "verse_id": 29,
    "verse_content": "وَٱلْتَفَّتِ ٱلسَّاقُ بِٱلسَّاقِ",
    "verse_key": "75:29"
  },
  {
    "verse_id": 30,
    "verse_content": "إِلَىٰ رَبِّكَ يَوْمَئِذٍ ٱلْمَسَاقُ",
    "verse_key": "75:30"
  },
  {
    "verse_id": 31,
    "verse_content": "فَلَا صَدَّقَ وَلَا صَلَّىٰ",
    "verse_key": "75:31"
  },
  {
    "verse_id": 32,
    "verse_content": "وَلَـٰكِن كَذَّبَ وَتَوَلَّىٰ",
    "verse_key": "75:32"
  },
  {
    "verse_id": 33,
    "verse_content": "ثُمَّ ذَهَبَ إِلَىٰٓ أَهْلِهِۦ يَتَمَطَّىٰٓ",
    "verse_key": "75:33"
  },
  {
    "verse_id": 34,
    "verse_content": "أَوْلَىٰ لَكَ فَأَوْلَىٰ",
    "verse_key": "75:34"
  },
  {
    "verse_id": 35,
    "verse_content": "ثُمَّ أَوْلَىٰ لَكَ فَأَوْلَىٰٓ",
    "verse_key": "75:35"
  },
  {
    "verse_id": 36,
    "verse_content": "أَيَحْسَبُ ٱلْإِنسَـٰنُ أَن يُتْرَكَ سُدًى",
    "verse_key": "75:36"
  },
  {
    "verse_id": 37,
    "verse_content": "أَلَمْ يَكُ نُطْفَةً مِّن مَّنِىٍّ يُمْنَىٰ",
    "verse_key": "75:37"
  },
  {
    "verse_id": 38,
    "verse_content": "ثُمَّ كَانَ عَلَقَةً فَخَلَقَ فَسَوَّىٰ",
    "verse_key": "75:38"
  },
  {
    "verse_id": 39,
    "verse_content": "فَجَعَلَ مِنْهُ ٱلزَّوْجَيْنِ ٱلذَّكَرَ وَٱلْأُنثَىٰٓ",
    "verse_key": "75:39"
  },
  {
    "verse_id": 40,
    "verse_content": "أَلَيْسَ ذَٰلِكَ بِقَـٰدِرٍ عَلَىٰٓ أَن يُحْـِۧىَ ٱلْمَوْتَىٰ",
    "verse_key": "75:40"
  }
]