[
  {
    "verse_id": 1,
    "verse_content": " إِذَا ٱلشَّمْسُ كُوِّرَتْ",
    "verse_key": "81:1"
  },
  {
    "verse_id": 2,
    "verse_content": "وَإِذَا ٱلنُّجُومُ ٱنكَدَرَتْ",
    "verse_key": "81:2"
  },
  {
    "verse_id": 3,
    "verse_content": "وَإِذَا ٱلْجِبَالُ سُيِّرَتْ",
    "verse_key": "81:3"
  },
  {
    "verse_id": 4,
    "verse_content": "وَإِذَا ٱلْعِشَارُ عُطِّلَتْ",
    "verse_key": "81:4"
  },
  {
    "verse_id": 5,
    "verse_content": "وَإِذَا ٱلْوُحُوشُ حُشِرَتْ",
    "verse_key": "81:5"
  },
  {
    "verse_id": 6,
    "verse_content": "وَإِذَا ٱلْبِحَارُ سُجِّرَتْ",
    "verse_key": "81:6"
  },
  {
    "verse_id": 7,
    "verse_content": "وَإِذَا ٱلنُّفُوسُ زُوِّجَتْ",
    "verse_key": "81:7"
  },
  {
    "verse_id": 8,
    "verse_content": "وَإِذَا ٱلْمَوْءُۥدَةُ سُئِلَتْ",
    "verse_key": "81:8"
  },
  {
    "verse_id": 9,
    "verse_content": "بِأَىِّ ذَنۢبٍ قُتِلَتْ",
    "verse_key": "81:9"
  },
  {
    "verse_id": 10,
    "verse_content": "وَإِذَا ٱلصُّحُفُ نُشِرَتْ",
    "verse_key": "81:10"
  },
  {
    "verse_id": 11,
    "verse_content": "وَإِذَا ٱلسَّمَآءُ كُشِطَتْ",
    "verse_key": "81:11"
  },
  {
    "verse_id": 12,
    "verse_content": "وَإِذَا ٱلْجَحِيمُ سُعِّرَتْ",
    "verse_key": "81:12"
  },
  {
    "verse_id": 13,
    "verse_content": "وَإِذَا ٱلْجَنَّةُ أُزْلِفَتْ",
    "verse_key": "81:13"
  },
  {
    "verse_id": 14,
    "verse_content": "عَلِمَتْ نَفْسٌ مَّآ أَحْضَرَتْ",
    "verse_key": "81:14"
  },
  {
    "verse_id": 15,
    "verse_content": "فَلَآ أُقْسِمُ بِٱلْخُنَّسِ",
    "verse_key": "81:15"
  },
  {
    "verse_id": 16,
    "verse_content": "ٱلْجَوَارِ ٱلْكُنَّسِ",
    "verse_key": "81:16"
  },
  {
    "verse_id": 17,
    "verse_content": "وَٱلَّيْلِ إِذَا عَسْعَسَ",
    "verse_key": "81:17"
  },
  {
    "verse_id": 18,
    "verse_content": "وَٱلصُّبْحِ إِذَا تَنَفَّسَ",
    "verse_key": "81:18"
  },
  {
    "verse_id": 19,
    "verse_content": "إِنَّهُۥ لَقَوْلُ رَسُولٍ كَرِيمٍ",
    "verse_key": "81:19"
  },
  {
    "verse_id": 20,
    "verse_content": "ذِى قُوَّةٍ عِندَ ذِى ٱلْعَرْشِ مَكِينٍ",
    "verse_key": "81:20"
  },
  {
    "verse_id": 21,
    "verse_content": "مُّطَاعٍ ثَمَّ أَمِينٍ",
    "verse_key": "81:21"
  },
  {
    "verse_id": 22,
    "verse_content": "وَمَا صَاحِبُكُم بِمَجْنُونٍ",
    "verse_key": "81:22"
  },
  {
    "verse_id": 23,
    "verse_content": "وَلَقَدْ رَءَاهُ بِٱلْأُفُقِ ٱلْمُبِينِ",
    "verse_key": "81:23"
  },
  {
    "verse_id": 24,
    "verse_content": "وَمَا هُوَ عَلَى ٱلْغَيْبِ بِضَنِينٍ",
    "verse_key": "81:24"
  },
  {
    "verse_id": 25,
    "verse_content": "وَمَا هُوَ بِقَوْلِ شَيْطَـٰنٍ رَّجِيمٍ",
    "verse_key": "81:25"
  },
  {
    "verse_id": 26,
    "verse_content": "فَأَيْنَ تَذْهَبُونَ",
    "verse_key": "81:26"
  },
  {
    "verse_id": 27,
    "verse_content": "إِنْ هُوَ إِلَّا ذِكْرٌ لِّلْعَـٰلَمِينَ",
    "verse_key": "81:27"
  },
  {
    "verse_id": 28,
    "verse_content": "لِمَن شَآءَ مِنكُمْ أَن يَسْتَقِيمَ",
    "verse_key": "81:28"
  },
  {
    "verse_id": 29,
    "verse_content": "وَمَا تَشَآءُونَ إِلَّآ أَن يَشَآءَ ٱللَّهُ رَبُّ ٱلْعَـٰلَمِينَ",
    "verse_key": "81:29"
  }
]