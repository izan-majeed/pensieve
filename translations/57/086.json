[
  {
    "verse_id": 1,
    "translation": "Wassama-i wattariq"
  },
  {
    "verse_id": 2,
    "translation": "Wama adraka ma attariq"
  },
  {
    "verse_id": 3,
    "translation": "Annajmu aththaqib"
  },
  {
    "verse_id": 4,
    "translation": "In kullu nafsin lamma AAalayha hafith"
  },
  {
    "verse_id": 5,
    "translation": "Falyanthuri al-insanumimma khuliq"
  },
  {
    "verse_id": 6,
    "translation": "Khuliqa min ma-in dafiq"
  },
  {
    "verse_id": 7,
    "translation": "Yakhruju min bayni assulbi wattara-ib"
  },
  {
    "verse_id": 8,
    "translation": "Innahu AAala rajAAihi laqadir"
  },
  {
    "verse_id": 9,
    "translation": "Yawma tubla assara-ir"
  },
  {
    "verse_id": 10,
    "translation": "Fama lahu min quwwatin wala nasir"
  },
  {
    "verse_id": 11,
    "translation": "Wassama-i thati arrajAA"
  },
  {
    "verse_id": 12,
    "translation": "Wal-ardi thati assadAA"
  },
  {
    "verse_id": 13,
    "translation": "Innahu laqawlun fasl"
  },
  {
    "verse_id": 14,
    "translation": "Wama huwa bilhazl"
  },
  {
    "verse_id": 15,
    "translation": "Innahum yakeedoona kayda"
  },
  {
    "verse_id": 16,
    "translation": "Waakeedu kayda"
  },
  {
    "verse_id": 17,
    "translation": "Famahhili alkafireena amhilhum ruwayda"
  }
]