[
  {
    "verse_id": 1,
    "verse_content": " وَٱلسَّمَآءِ ذَاتِ ٱلْبُرُوجِ",
    "verse_key": "85:1"
  },
  {
    "verse_id": 2,
    "verse_content": "وَٱلْيَوْمِ ٱلْمَوْعُودِ",
    "verse_key": "85:2"
  },
  {
    "verse_id": 3,
    "verse_content": "وَشَاهِدٍ وَمَشْهُودٍ",
    "verse_key": "85:3"
  },
  {
    "verse_id": 4,
    "verse_content": "قُتِلَ أَصْحَـٰبُ ٱلْأُخْدُودِ",
    "verse_key": "85:4"
  },
  {
    "verse_id": 5,
    "verse_content": "ٱلنَّارِ ذَاتِ ٱلْوَقُودِ",
    "verse_key": "85:5"
  },
  {
    "verse_id": 6,
    "verse_content": "إِذْ هُمْ عَلَيْهَا قُعُودٌ",
    "verse_key": "85:6"
  },
  {
    "verse_id": 7,
    "verse_content": "وَهُمْ عَلَىٰ مَا يَفْعَلُونَ بِٱلْمُؤْمِنِينَ شُهُودٌ",
    "verse_key": "85:7"
  },
  {
    "verse_id": 8,
    "verse_content": "وَمَا نَقَمُوا۟ مِنْهُمْ إِلَّآ أَن يُؤْمِنُوا۟ بِٱللَّهِ ٱلْعَزِيزِ ٱلْحَمِيدِ",
    "verse_key": "85:8"
  },
  {
    "verse_id": 9,
    "verse_content": "ٱلَّذِى لَهُۥ مُلْكُ ٱلسَّمَـٰوَٰتِ وَٱلْأَرْضِ ۚ وَٱللَّهُ عَلَىٰ كُلِّ شَىْءٍ شَهِيدٌ",
    "verse_key": "85:9"
  },
  {
    "verse_id": 10,
    "verse_content": "إِنَّ ٱلَّذِينَ فَتَنُوا۟ ٱلْمُؤْمِنِينَ وَٱلْمُؤْمِنَـٰتِ ثُمَّ لَمْ يَتُوبُوا۟ فَلَهُمْ عَذَابُ جَهَنَّمَ وَلَهُمْ عَذَابُ ٱلْحَرِيقِ",
    "verse_key": "85:10"
  },
  {
    "verse_id": 11,
    "verse_content": "إِنَّ ٱلَّذِينَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّـٰلِحَـٰتِ لَهُمْ جَنَّـٰتٌ تَجْرِى مِن تَحْتِهَا ٱلْأَنْهَـٰرُ ۚ ذَٰلِكَ ٱلْفَوْزُ ٱلْكَبِيرُ",
    "verse_key": "85:11"
  },
  {
    "verse_id": 12,
    "verse_content": "إِنَّ بَطْشَ رَبِّكَ لَشَدِيدٌ",
    "verse_key": "85:12"
  },
  {
    "verse_id": 13,
    "verse_content": "إِنَّهُۥ هُوَ يُبْدِئُ وَيُعِيدُ",
    "verse_key": "85:13"
  },
  {
    "verse_id": 14,
    "verse_content": "وَهُوَ ٱلْغَفُورُ ٱلْوَدُودُ",
    "verse_key": "85:14"
  },
  {
    "verse_id": 15,
    "verse_content": "ذُو ٱلْعَرْشِ ٱلْمَجِيدُ",
    "verse_key": "85:15"
  },
  {
    "verse_id": 16,
    "verse_content": "فَعَّالٌ لِّمَا يُرِيدُ",
    "verse_key": "85:16"
  },
  {
    "verse_id": 17,
    "verse_content": "هَلْ أَتَىٰكَ حَدِيثُ ٱلْجُنُودِ",
    "verse_key": "85:17"
  },
  {
    "verse_id": 18,
    "verse_content": "فِرْعَوْنَ وَثَمُودَ",
    "verse_key": "85:18"
  },
  {
    "verse_id": 19,
    "verse_content": "بَلِ ٱلَّذِينَ كَفَرُوا۟ فِى تَكْذِيبٍ",
    "verse_key": "85:19"
  },
  {
    "verse_id": 20,
    "verse_content": "وَٱللَّهُ مِن وَرَآئِهِم مُّحِيطٌۢ",
    "verse_key": "85:20"
  },
  {
    "verse_id": 21,
    "verse_content": "بَلْ هُوَ قُرْءَانٌ مَّجِيدٌ",
    "verse_key": "85:21"
  },
  {
    "verse_id": 22,
    "verse_content": "فِى لَوْحٍ مَّحْفُوظٍۭ",
    "verse_key": "85:22"
  }
]