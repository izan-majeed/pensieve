[
  {
    "verse_id": 1,
    "verse_content": " وَٱلْمُرْسَلَـٰتِ عُرْفًا",
    "verse_key": "77:1"
  },
  {
    "verse_id": 2,
    "verse_content": "فَٱلْعَـٰصِفَـٰتِ عَصْفًا",
    "verse_key": "77:2"
  },
  {
    "verse_id": 3,
    "verse_content": "وَٱلنَّـٰشِرَٰتِ نَشْرًا",
    "verse_key": "77:3"
  },
  {
    "verse_id": 4,
    "verse_content": "فَٱلْفَـٰرِقَـٰتِ فَرْقًا",
    "verse_key": "77:4"
  },
  {
    "verse_id": 5,
    "verse_content": "فَٱلْمُلْقِيَـٰتِ ذِكْرًا",
    "verse_key": "77:5"
  },
  {
    "verse_id": 6,
    "verse_content": "عُذْرًا أَوْ نُذْرًا",
    "verse_key": "77:6"
  },
  {
    "verse_id": 7,
    "verse_content": "إِنَّمَا تُوعَدُونَ لَوَٰقِعٌ",
    "verse_key": "77:7"
  },
  {
    "verse_id": 8,
    "verse_content": "فَإِذَا ٱلنُّجُومُ طُمِسَتْ",
    "verse_key": "77:8"
  },
  {
    "verse_id": 9,
    "verse_content": "وَإِذَا ٱلسَّمَآءُ فُرِجَتْ",
    "verse_key": "77:9"
  },
  {
    "verse_id": 10,
    "verse_content": "وَإِذَا ٱلْجِبَالُ نُسِفَتْ",
    "verse_key": "77:10"
  },
  {
    "verse_id": 11,
    "verse_content": "وَإِذَا ٱلرُّسُلُ أُقِّتَتْ",
    "verse_key": "77:11"
  },
  {
    "verse_id": 12,
    "verse_content": "لِأَىِّ يَوْمٍ أُجِّلَتْ",
    "verse_key": "77:12"
  },
  {
    "verse_id": 13,
    "verse_content": "لِيَوْمِ ٱلْفَصْلِ",
    "verse_key": "77:13"
  },
  {
    "verse_id": 14,
    "verse_content": "وَمَآ أَدْرَىٰكَ مَا يَوْمُ ٱلْفَصْلِ",
    "verse_key": "77:14"
  },
  {
    "verse_id": 15,
    "verse_content": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
    "verse_key": "77:15"
  },
  {
    "verse_id": 16,
    "verse_content": "أَلَمْ نُهْلِكِ ٱلْأَوَّلِينَ",
    "verse_key": "77:16"
  },
  {
    "verse_id": 17,
    "verse_content": "ثُمَّ نُتْبِعُهُمُ ٱلْـَٔاخِرِينَ",
    "verse_key": "77:17"
  },
  {
    "verse_id": 18,
    "verse_content": "كَذَٰلِكَ نَفْعَلُ بِٱلْمُجْرِمِينَ",
    "verse_key": "77:18"
  },
  {
    "verse_id": 19,
    "verse_content": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
    "verse_key": "77:19"
  },
  {
    "verse_id": 20,
    "verse_content": "أَلَمْ نَخْلُقكُّم مِّن مَّآءٍ مَّهِينٍ",
    "verse_key": "77:20"
  },
  {
    "verse_id": 21,
    "verse_content": "فَجَعَلْنَـٰهُ فِى قَرَارٍ مَّكِينٍ",
    "verse_key": "77:21"
  },
  {
    "verse_id": 22,
    "verse_content": "إِلَىٰ قَدَرٍ مَّعْلُومٍ",
    "verse_key": "77:22"
  },
  {
    "verse_id": 23,
    "verse_content": "فَقَدَرْنَا فَنِعْمَ ٱلْقَـٰدِرُونَ",
    "verse_key": "77:23"
  },
  {
    "verse_id": 24,
    "verse_content": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
    "verse_key": "77:24"
  },
  {
    "verse_id": 25,
    "verse_content": "أَلَمْ نَجْعَلِ ٱلْأَرْضَ كِفَاتًا",
    "verse_key": "77:25"
  },
  {
    "verse_id": 26,
    "verse_content": "أَحْيَآءً وَأَمْوَٰتًا",
    "verse_key": "77:26"
  },
  {
    "verse_id": 27,
    "verse_content": "وَجَعَلْنَا فِيهَا رَوَٰسِىَ شَـٰمِخَـٰتٍ وَأَسْقَيْنَـٰكُم مَّآءً فُرَاتًا",
    "verse_key": "77:27"
  },
  {
    "verse_id": 28,
    "verse_content": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
    "verse_key": "77:28"
  },
  {
    "verse_id": 29,
    "verse_content": "ٱنطَلِقُوٓا۟ إِلَىٰ مَا كُنتُم بِهِۦ تُكَذِّبُونَ",
    "verse_key": "77:29"
  },
  {
    "verse_id": 30,
    "verse_content": "ٱنطَلِقُوٓا۟ إِلَىٰ ظِلٍّ ذِى ثَلَـٰثِ شُعَبٍ",
    "verse_key": "77:30"
  },
  {
    "verse_id": 31,
    "verse_content": "لَّا ظَلِيلٍ وَلَا يُغْنِى مِنَ ٱللَّهَبِ",
    "verse_key": "77:31"
  },
  {
    "verse_id": 32,
    "verse_content": "إِنَّهَا تَرْمِى بِشَرَرٍ كَٱلْقَصْرِ",
    "verse_key": "77:32"
  },
  {
    "verse_id": 33,
    "verse_content": "كَأَنَّهُۥ جِمَـٰلَتٌ صُفْرٌ",
    "verse_key": "77:33"
  },
  {
    "verse_id": 34,
    "verse_content": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
    "verse_key": "77:34"
  },
  {
    "verse_id": 35,
    "verse_content": "هَـٰذَا يَوْمُ لَا يَنطِقُونَ",
    "verse_key": "77:35"
  },
  {
    "verse_id": 36,
    "verse_content": "وَلَا يُؤْذَنُ لَهُمْ فَيَعْتَذِرُونَ",
    "verse_key": "77:36"
  },
  {
    "verse_id": 37,
    "verse_content": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
    "verse_key": "77:37"
  },
  {
    "verse_id": 38,
    "verse_content": "هَـٰذَا يَوْمُ ٱلْفَصْلِ ۖ جَمَعْنَـٰكُمْ وَٱلْأَوَّلِينَ",
    "verse_key": "77:38"
  },
  {
    "verse_id": 39,
    "verse_content": "فَإِن كَانَ لَكُمْ كَيْدٌ فَكِيدُونِ",
    "verse_key": "77:39"
  },
  {
    "verse_id": 40,
    "verse_content": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
    "verse_key": "77:40"
  },
  {
    "verse_id": 41,
    "verse_content": "إِنَّ ٱلْمُتَّقِينَ فِى ظِلَـٰلٍ وَعُيُونٍ",
    "verse_key": "77:41"
  },
  {
    "verse_id": 42,
    "verse_content": "وَفَوَٰكِهَ مِمَّا يَشْتَهُونَ",
    "verse_key": "77:42"
  },
  {
    "verse_id": 43,
    "verse_content": "كُلُوا۟ وَٱشْرَبُوا۟ هَنِيٓـًٔۢا بِمَا كُنتُمْ تَعْمَلُونَ",
    "verse_key": "77:43"
  },
  {
    "verse_id": 44,
    "verse_content": "إِنَّا كَذَٰلِكَ نَجْزِى ٱلْمُحْسِنِينَ",
    "verse_key": "77:44"
  },
  {
    "verse_id": 45,
    "verse_content": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
    "verse_key": "77:45"
  },
  {
    "verse_id": 46,
    "verse_content": "كُلُوا۟ وَتَمَتَّعُوا۟ قَلِيلًا إِنَّكُم مُّجْرِمُونَ",
    "verse_key": "77:46"
  },
  {
    "verse_id": 47,
    "verse_content": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
    "verse_key": "77:47"
  },
  {
    "verse_id": 48,
    "verse_content": "وَإِذَا قِيلَ لَهُمُ ٱرْكَعُوا۟ لَا يَرْكَعُونَ",
    "verse_key": "77:48"
  },
  {
    "verse_id": 49,
    "verse_content": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
    "verse_key": "77:49"
  },
  {
    "verse_id": 50,
    "verse_content": "فَبِأَىِّ حَدِيثٍۭ بَعْدَهُۥ يُؤْمِنُونَ",
    "verse_key": "77:50"
  }
]